<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="icnews.acquire">

    <include file="admin.zcml" />

    <browser:page
        name="rss"
        for="icnews.acquire.interfaces.IAdqnews"
        class=".rss.RSS2View"
        permission="zope2.View"
        />

    <browser:page
        name="stored-rss"
        for="icnews.acquire.interfaces.IAdqnews"
        class=".rss.StoredNews"
        permission="zope2.View"
        />

</configure>
