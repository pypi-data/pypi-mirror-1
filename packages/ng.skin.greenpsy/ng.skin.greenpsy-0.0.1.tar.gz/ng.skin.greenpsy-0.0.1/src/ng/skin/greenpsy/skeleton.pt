<metal:block define-macro="page">
  <metal:block define-slot="doctype">
  <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
</metal:block>
<html xmlns="http://www.w3.org/1999/xhtml">
<head metal:use-macro="views/ng_skin_macros/head">
  <metal:block fill-slot="other">
    <base href="" tal:attributes="href python:path('context/@@absolute_url')+'/'"/>
    <metal:block define-slot="headers" />
    <metal:block define-slot="style_slot" />
    <metal:block define-slot="ecmascript_slot" />
  </metal:block>
</head>
<body metal:define-slot="mainbody">

  <div id="header">
    <img src="logo.jpg" alt="GreenFuture" style="float: left;"
      tal:attributes="src context/++resource++images/logo.jpg"
    />
    <img src="title.gif" alt="GreenFuture" style="float: right;"
      tal:attributes="src context/++resource++images/title.gif"
    />
  </div>
<div style="background-color: #a2d935;
            overflow: auto;
            height: 27px;
            clear: both;">
  <metal:block use-macro="views/ng_skin_macros/mainmenu">
     <metal:block fill-slot="item_before">
       <a class="mainmenuitem" href="/">Главная</a>
     </metal:block>
  </metal:block>
</div>


    <div id="breadcrumb">
      <ul>
        <tal:block repeat="breadcrumb context/@@absolute_url/breadcrumbs">
          <li tal:condition="repeat/breadcrumb/start">
            <a href=""
                tal:attributes="
                href string:${breadcrumb/url}/"
                i18n:translate="">Main</a>
          </li>
          <li tal:condition="not:repeat/breadcrumb/start">
            <a href=""
                tal:content="string:${breadcrumb/name}"
                tal:attributes="
                href string:${breadcrumb/url}/">
                breadcrumb item</a>
          </li>
        </tal:block>
      </ul>
      <span id="youarehere">вы здесь:</span>
    </div>

  <div id="leftcolumn">
    <tal:block content="structure context/++registry++leftbanner:"/>
    <tal:block tal:content="structure provider:leftcolumn">
    </tal:block>
  
  </div>

  <div id="rightcolumn">
    <tal:block tal:content="structure provider:rightcolumn">
    </tal:block>
 <!--   <iframe src="@@statistic" width="180px" frameborder="0" scrolling="no" height="200px"></iframe> -->
  </div>

  <div id="search">
    <h1>Поиск по сайту <iframe align="right" src="@@statistic" width="300px" height="20px" frameborder="0" scrolling="no"></iframe></h1>
    <form action="@@searchpage.html">
      <input type="text" name="common" style="width: 80%;" />
      <input type="submit" value="Найти" class="button" />
    </form>
  </div>
  <div id="mediaheader">Green Future</div>
  <metal:block define-slot="content">
    <metal:block use-macro="views/ng_skin_macros/content">
      <metal:block fill-slot="body">
        <metal:block define-slot="body"/>
      </metal:block>
    </metal:block>
  </metal:block>

<div style="background-color: #a2d935; overflow: auto; height: 27px; clear: both;">
  <metal:block use-macro="views/ng_skin_macros/referencemenu" />
</div>

  <div id="footer">
    <div><a href="http://www.dreambot.ru/" title="Site concept and development by DreamBot Team. Python, open source solutions." rel="friend">Site developed by Dreambot Team</a></div>
  </div>

  <metal:block use-macro="views/ng_skin_macros/smartbanner" />

<!--  <metal:block use-macro="views/ng_skin_macros/editormenu" /> -->

<!--  <tal:block tal:condition="python:path('context/@@+/AddArticle.html')" >OK</tal:block> -->

</body>
</html>
</metal:block>
