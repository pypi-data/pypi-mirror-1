<?xml version="1.0"?>
<!-- Install our custom portal_properties. Plone's defaults are in CMFPlone/profiles/default/propertiestool.xml -->

<object name="portal_properties" meta_type="Plone Properties Tool">
 <object name="navtree_properties" meta_type="Plone Property Sheet">
  <property name="metaTypesNotToList" type="lines" purge="False">
    <element value="PingTool"/>
    <element value="PingInfo"/>
  </property>
 </object>
 <object name="site_properties" meta_type="Plone Property Sheet">
  <property name="use_folder_tabs" type="lines" purge="False"> 
    <element value="PingTool"/>
  </property> 
  <property name="typesLinkToFolderContentsInFC" type="lines" purge="False">
    <element value="PingTool"/>
  </property>
  <property name="typesUseViewActionInListings" type="lines" purge="False">
    <element value="PingInfo"/>
  </property>
 </object>
</object>
