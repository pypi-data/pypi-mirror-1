<ul xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    i18n:domain="plone"
    id="document-action-feed">

<li id="" tal:repeat="feed view/feeds" tal:attributes="id string:document-action-${feed/name}">
    <a href="" 
       tal:attributes="href string:${feed/url}"
       tal:condition="feed/enabled" >
        <img tal:attributes="src context/++resource++small-feed-icon" />
        <tal:block tal:replace="feed/name" />
    </a>
</li>
</ul>
