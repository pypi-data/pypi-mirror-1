<dl class="portlet portletFeedLinks"
    i18n:domain="vice">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <span class="tile"
              tal:content="view/title">_title_</span>
        <span class="portletTopRight"></span>
    </dt>

        <dd tal:repeat="feed_dict view/getFeeds"
            tal:attributes="class python:repeat['feed_dict'].odd() and 'portletItem odd' or 'portletItem even'"
            class="portletItem odd">

          <a href="#"
             tal:attributes="href feed_dict/url">
            <img tal:define="img_url python:view.getImageURL(feed_dict['format'])"
                 src=""
                 alt=""
                 tal:attributes="src   img_url;
                                 alt   feed_dict/name;
                                 title feed_dict/name" />
          </a>
        </dd>

        <dd class="portletFooter">
          <span class="portletBottomLeft"></span>
          <span class="portletBottomRight"></span>
        </dd>
</dl>
