<h2>Authenticated User Attributes</h2>
<tal:block tal:define="properties python:view.getAuthMemberProperties()">
<span class="message" tal:condition="properties/has_error" tal:content="properties" />

<dl>
<tal:memberattrs tal:repeat="property python:properties.value.keys()">
		<dt tal:content="property"></dt>
		<dd tal:content="python:str(properties.value[property])"></dd>
</tal:memberattrs>
</dl>

</tal:block>