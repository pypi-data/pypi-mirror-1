<h2>Context Information</h2>
	<dl tal:define="context_info view/getContextInfo">
		<dt>ID</dt>
		<dd tal:content="context_info/id" tal:attributes="class context_info/id/getClass"></dd>
		<dt>Type</dt>
		<dd tal:content="context_info/portal_type" tal:attributes="class context_info/portal_type/getClass"></dd>



		<dt>Path</dt>
		<dd tal:content="context_info/path" tal:attributes="class context_info/path/getClass"></dd>
		<dt>Absolute URL</dt>
		<dd tal:content="context_info/url" tal:attributes="class context_info/url/getClass"></dd>
		<dt>Temporary Object</dt>
		<dd tal:content="context_info/temporary" tal:attributes="class context_info/temporary/getClass"></dd>
	</dl>