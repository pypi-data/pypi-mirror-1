<h2>Catalog Indexes for Context</h2>
<tal:catalogindexes tal:define="catalog_info view/getCatalogIndexes">


<span tal:condition="catalog_info/has_error" class="message" tal:content="catalog_info/error_message" />

<tal:block tal:condition="not:catalog_info/has_error">

    <span tal:condition="python:len(catalog_info.value)==0" class="message">No catalog indexes for this object</span>
    <dl tal:condition="python:len(catalog_info())!=0">
    <tal:indexes tal:repeat="index python:catalog_info()">
    	<dt tal:content="index"></dt>
    	<dd tal:content="python:catalog_info()[index]"></dd>
    </tal:indexes>
    </dl>

</tal:block>


</tal:catalogindexes>