#! /usr/bin/python

# $Id: amenukit-demo 19 2009-10-09 10:13:14Z pytrash $

import sys

try:
    kit = sys.argv[1]
except Exception:
    kit = ''

if kit == 'tk':
    from amenukit import tkmenukit as mk
elif kit == 'gtk':
    from amenukit import gtkmenukit as mk
elif kit == 'qt':
    from amenukit import qtmenukit as mk
elif kit == 'wx':
    from amenukit import wxmenukit as mk
else:
    print "Usage: amenukit-demo [ tk | gtk | qt | wx ]"
    sys.exit()

mk.doDemo()

