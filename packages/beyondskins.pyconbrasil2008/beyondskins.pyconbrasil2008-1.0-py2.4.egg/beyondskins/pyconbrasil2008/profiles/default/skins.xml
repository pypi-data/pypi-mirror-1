<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="pyconbrasil2008">

 <object name="pyconbrasil2008_images"
    meta_type="Filesystem Directory View"
    directory="pyconbrasil2008/skins/pyconbrasil2008_images"/>
 <object name="pyconbrasil2008_custom_images"
    meta_type="Filesystem Directory View"
    directory="pyconbrasil2008/skins/pyconbrasil2008_custom_images"/>
 <object name="pyconbrasil2008_styles"
    meta_type="Filesystem Directory View"
    directory="pyconbrasil2008/skins/pyconbrasil2008_styles"/>
 <object name="pyconbrasil2008_custom_styles"
    meta_type="Filesystem Directory View"
    directory="pyconbrasil2008/skins/pyconbrasil2008_custom_styles"/>
 <object name="pyconbrasil2008_templates"
    meta_type="Filesystem Directory View"
    directory="pyconbrasil2008/skins/pyconbrasil2008_templates"/>
 <object name="pyconbrasil2008_custom_templates"
    meta_type="Filesystem Directory View"
    directory="pyconbrasil2008/skins/pyconbrasil2008_custom_templates"/>

 <skin-path name="pyconbrasil2008"  based-on="Plone Default">
     <layer name="pyconbrasil2008_images" insert-before="*"/>
     <layer name="pyconbrasil2008_custom_images" insert-after="pyconbrasil2008_images"/>
     <layer name="pyconbrasil2008_styles" insert-after="pyconbrasil2008_custom_images"/>
     <layer name="pyconbrasil2008_custom_styles" insert-after="pyconbrasil2008_styles"/>
     <layer name="pyconbrasil2008_templates" insert-after="pyconbrasil2008_custom_styles"/>
     <layer name="pyconbrasil2008_custom_templates" insert-after="pyconbrasil2008_templates"/>
     <layer name="custom" insert-before="*"/>
 </skin-path>

</object>
