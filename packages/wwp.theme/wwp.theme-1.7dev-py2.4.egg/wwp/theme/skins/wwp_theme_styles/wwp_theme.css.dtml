/*---------- GENERAL STYLE ----------*/
.logo {
  font-family:Arial,sans-serif;
  font-size:200%;
  font-weight: bold;
  family-name: arial;
  color:#000080;
  background-color: transparent;
  display:block;
}

.logo a {
color:#000080;
}


#portal-header{
background-color: #CC99FF;
padding: 5px 5px 5px 15px;
}

blockquote {
border:0px;
}

h1 {
font-family:arial,sans-serif,Trebuchet MS,helvetica;
font-size:14pt;
font-style:normal;
font-variant:small-caps;
font-weight:bold;
text-align:left;
color:#000080;
border-bottom:0px;
}

h2 {
font-family:arial,sans-serif,Trebuchet MS,helvetica;
font-size:12pt;
font-style:normal;
font-weight:bold;
margin-left:0;
margin-right:0;
text-align:left;
color:#666699;
border-bottom:0px;
}


body {
    font-family:arial,sans-serif,Trebuchet MS,helvetica;
    color: black;
    padding: 0;
    background-color:#FFFFCC;
    margin: 0 auto 0 auto;
    width: 1024px;
    border-left: solid 0px #dddddd;
    border-right: solid 0px #dddddd;
}

#portal-column-one .visualPadding {
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
}

#portal-column-one {
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    border-collapse:collapse;
    padding:0;
    vertical-align:top;
    width:16em;
    background-color:transparent;
}

}
#portal-column-contents .visualPadding {
    padding: 0px 0px 0px 0px;
    margin: 2px 2px 2px 2px;
    background-color:transparent;
}

#portal-column-two .visualPadding {
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    width: 320px;
    background-color:transparent;
}

#portal-column-two {
    padding: 0px 0px 0px 0px;
    margin: 0px 0px 0px 0px;
    border-collapse:collapse;
    padding:0;
    vertical-align:top;
    width:16em;
    background-color:transparent;
}

#portal-personaltools {
background-color: transparent;
border:0px;
color:Black;
line-height:1.6em;
list-style-image:none;
list-style-position:outside;
list-style-type:none;
margin:0;
padding:0 3em;
text-align:right;
text-transform:lowercase;
}

#portal-column-content {
border-collapse:collapse;
margin:0 0 2em;
padding:2px 10px 0;
vertical-align:top;
}

.documentContent {
background: transparent none repeat scroll 0 0;
font-size:110%;
padding:2px 2px 2px 2px !important;
}


#portal-siteactions {
background-color:transparent;
border:0px;
float:right;
height:auto;
line-height:normal;
list-style-image:none;
list-style-type:none;
margin:1px 0 0;
padding:3px 2em 3px 0;
white-space:nowrap;
}

#portal-siteactions li a {
background-color:transparent;
border:0px;
color:#000080;
height:auto;
margin-right:0.2em;
padding:0.2em;
text-decoration:none;
text-transform:lowercase;
}


/*---------- Global Sections Navigation ----------*/

#portal-globalnav {
  background-color:transparent;
  height:auto;
  line-height:normal;
  list-style-image:none;
  list-style-position:outside;
  list-style-type:none; 
  margin:0;
  padding:0 0 0 2em;
  white-space:nowrap;
  border-bottom:1px solid #8CACBB;
}

#portal-globalnav li.selected a {
  background-color:#DEE7EC;
  border-width:0px;
  color:#436976;
}

#portal-globalnav li a {
  background-color:transparent;
  border-width:0px;
  color:#436976;
  height:auto;
  margin-right:0.5em;
  padding:0 1.0em;
  text-decoration:none;
  text-transform:lowercase;
}



/*---------- Viewlet Managers ----------*/

#portal-wwpdate{
    background-color: transparent;
    border-top: 1px; solid #8cacbb;
    border-bottom: 1px; solid #8cacbb;
    float: none;
    padding: 0;
    margin: 0;font-family:"Lucida Grande",Verdana,Lucida,Helvetica,Arial,sans-serif;
font-size:69%;
    text-align: center;
}

/* ============================================== */
/* adsense-top             */
/* ============================================== */

.portal-adblocktop {
    background-color: #FFCC66;
    border-top: 0px;
    border-bottom: 0px;
    float: none;
    padding: 2px;
    margin: 0;
    text-align: center;
}
/* ============================================== */

#portal-breadcrumbs {
border-bottom:0px;
background-color: transparent; 
line-height:1.6em;
padding-left:2em;
padding-right:2em;
text-transform:lowercase;
}



/*---------- Portlets ----------*/
.portlet {
  padding: 0px 0px 0px 0px;
  margin: 0px 0px 10px 0px;
  border:0px;
  background-color: #CCFFFF;
}

.portletHeader {
  background-color: #FFCC66;
  color: #666699;
  border:0px;
  font-weight:bold;
  line-height:1.6em;
  padding:0 5px 0px 5px;
  text-transform:lowercase;
}
.portlet a.tile {
  display:block;
  color: #666699;
}




.portletFooter {
  background-color:#FFCC66;
  color:#666699;
  margin:0;
  border-left:0px;
  border-right:0px;
  border-top:0px;
  border-bottom:0px;
  padding:0 5px 0px 5px;
  text-align:right;
}


/* --- this is the page footer --- */
#portal-footer {
border-left:0px;
border-right:0px;
border-top:0px;
border-bottom:0px;
line-height:1.2em;
margin:0;
padding:2px 2px 2px 2px;
text-align:center;
color:#666699;
background-color: transparent;
}



.portletItem {
border-left:0px;
border-right:0px;
border-top:0px;
border-bottom:0px;
  margin:0;
  padding:2px 5px 2px 5px;
  background-color: #CCFFFF;
}







.portlet a.tile {
color:#666699;
display:block;
}

.portletItem a, .portletFooter a {
color:#666699;
}
.portletItem a:visited, .portletFooter a:visited {
color:#666699;
}

#portal-footer h7 {
color:#666699;
}
#portal-footer h7 a {

color:#660099;
}


.logo_footer {
  font-family:"Lucida Grande",Verdana,Lucida,Helvetica,Arial,sans-serif;
  font-size:200%;
  font-weight: bold;
  color:#666699;
  background-color: #CC99FF;
  display:block;
  text-align:right;
}



/* ============================================== */
/* Calendar                                       */
/* ============================================== */

.portletCalendar dt {
background-color:transparent;
border-bottom:medium none;
font-weight:normal;
line-height:1.6em;
text-align:center;
}

/* ============================================== */
/* NAVIGATION                                     */
/* ============================================== */

.navTreeCurrentItem {
background-color:#FFCC66;
border:1px solid #666699 !important;
color:#666699;
}

/* hovered nav items */
.navTreeItem a, dd.portletItem .navTreeItem a {
text-align:left;
border:0px;
padding-top:0px;
padding-bottom:0px;
margin:0 0 0 0;
display:block;
text-decoration:none;
font-size:8pt;
}


.navTreeItem a:hover, dd.portletItem .navTreeItem a:hover {
border:0px;
display:block;
text-decoration:none;
}

/* hovered nav items */
.navTree a:hover { 
margin-left: 0px; 
padding-right:1px; 
background-color:#FFCC66 !important;
border: 1px #666699 collapse !important;
}

/* current nav items */
.navTreeCurrentItem a, dd.portletItem .navTreeCurrentItem a {
padding-left:10px;
color:#000000;
border:0px;
display:block;
text-decoration:none;
font-size:8pt;
}


/* Customized Portlet-Style */


#portlet-navigation .portletItem {
    padding:0;
}

#portlet-navigation ul,
#portlet-navigation li {
    margin:0px !important;
    padding:0px !important;  
    line-height: 1.2em !important;  
}

#portlet-navigation a {
    border:none !important;
}

#portlet-navigation a:hover {
    border:none !important;
    background-color:transparent;
}

#portlet-navigation .item {
    border-bottom:0px;
}


