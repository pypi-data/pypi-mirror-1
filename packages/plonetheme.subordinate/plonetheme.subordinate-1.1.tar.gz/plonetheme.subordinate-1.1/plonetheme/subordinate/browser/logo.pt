<a class="logo-heading"
   tal:define="plone_view context/@@plone_portal_state;
               title plone_view/portal_title;
               titles python:title.split('  ');
               t1 python:titles.pop(0);
               t2 python:len(titles)>0 and titles.pop(0) or '';"
   tal:attributes="href plone_view/portal_url">
	<div class="logo-text" tal:content="t1">Subordinate</div>
	<div class="logo-smalltext" tal:content="t2">Plone Design</div>
</a>
