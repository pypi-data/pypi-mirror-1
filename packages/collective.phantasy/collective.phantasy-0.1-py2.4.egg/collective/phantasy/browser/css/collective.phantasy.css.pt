    background: ${css_properties/evenRowBackgroundColor}; 
    border: ${css_properties/borderWidth} ${css_properties/borderStyleAnnotations} ${css_properties/globalBorderColor}; 
}

.comment h1,
.comment h2,
.comment h3,
.comment h4, 
.comment h5,
.comment h6 {
    border-bottom: ${css_properties/borderWidth} ${css_properties/borderStyleAnnotations} ${css_properties/discreetColor};
}

.comment h3 a {
    background-image: url(${css_url}/discussionitem_icon.gif);
}

dl.collapsible {
    border: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor} !important;

}

dl.collapsible dt.collapsibleHeader {
    background: ${css_properties/backgroundColor};
}

dl.expandedInlineCollapsible dt.collapsibleHeader,
dl.expandedBlockCollapsible dt.collapsibleHeader {
    background: ${css_properties/backgroundColor} url(treeExpanded.gif) no-repeat 6px 50%;
}

.documentEditable .documentContent {
    border: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/contentViewBorderColor};
}

.optionsToggle {
    border: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor};
    color: ${css_properties/fontColor};
    background-color: ${css_properties/globalBackgroundColor};
    font-size: ${css_properties/fontSmallSize};
}

#portal-column-content fieldset > * input:focus,
#portal-column-content fieldset > * textarea:focus {
    border-color: ${css_properties/notifyBorderColor};
    border-width: ${css_properties/borderWidth};
}

.currentItem {
    border-style: ${css_properties/borderStyle};
    border-color: ${css_properties/notifyBorderColor};
}

.managePortletsLink {
    color: ${css_properties/discreetColor};
    font-size: ${css_properties/fontSmallSize};
}

li.formTab a {
    border-top: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor};
    border-bottom: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor};
    border-left: ${css_properties/borderWidth} dotted ${css_properties/globalBorderColor};
    background: ${css_properties/backgroundColor};
}

li.firstFormTab a {
    border-left: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor};
}

li.lastFormTab a {
    border-right: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor};
}

li.formTab a.selected {
    background: ${css_properties/globalBackgroundColor};
}

li.formTab a:hover {
    background: ${css_properties/globalBackgroundColor};
}

li.formTab a.notify {
    background-color: ${css_properties/notifyBackgroundColor};
    color: ${css_properties/fontColor};
}

li.formTab a.required span {
    background-image: url(${css_url}/required.gif);
}

li.formTab a.notify:hover {
    background-color: ${css_properties/notifyBorderColor};
}

.formPanel {
    padding: 1em 1em 1em 1em; 
    border: ${css_properties/borderWidth} ${css_properties/borderStyle} ${css_properties/globalBorderColor};
}

" />  

