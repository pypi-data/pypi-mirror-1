#portal-logo img {
    display: none;
    margin: 0;
}

#portal-logo {
    display:block;
    text-decoration: none;
    overflow: hidden;
    border: 0;
    margin: 0;
    padding: 0;    
    padding-top: ${logo/height}px;
    height: 0px !important;
    height /**/: ${logo/height}px;
    width: ${logo/width}px;
    background: transparent url(${css_url}/${logoName}) top left no-repeat;
    cursor: pointer;
    margin: 1em 0em 1em 2em;
}           

