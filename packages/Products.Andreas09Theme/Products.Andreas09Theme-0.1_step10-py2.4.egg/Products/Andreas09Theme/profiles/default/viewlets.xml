  </order>

  <hidden manager="plone.portalheader" skinname="Andreas09">
    <viewlet name="plone.site_actions" />
    <viewlet name="plone.logo" />
  </hidden>

  <order manager="plone.contentviews" skinname="Andreas09"
         based-on="Plone Default">
