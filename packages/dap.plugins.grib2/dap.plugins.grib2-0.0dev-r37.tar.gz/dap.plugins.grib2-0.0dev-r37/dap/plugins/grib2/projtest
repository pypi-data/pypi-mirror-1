#!/usr/bin/python

# Use grib2's version of Proj
from grib2.proj import Proj
import gdal 

projparams = {'lon_0': 210.0, 'R': 6371229.0, 'lat_0': 90, 'proj': 'stere', 'lat_ts': 60.0}
llcrnrlon = 174.163
llcrnrlat = 19.132
nx = 180
ny = 156
dx = 47625.0
dy = 47625.0

stereo = Proj(projparams,llcrnrlon,llcrnrlat,(nx-1)*dx,(ny-1)*dy,urcrnrislatlon=False)

print stereo.projparams
