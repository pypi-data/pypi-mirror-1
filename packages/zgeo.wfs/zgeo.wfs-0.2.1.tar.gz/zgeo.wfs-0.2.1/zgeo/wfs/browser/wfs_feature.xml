<?xml version='1.0' encoding="ISO-8859-1" ?>
<wfs:FeatureCollection
   xmlns:myns="http://www.ttt.org/myns"
   xmlns:tal="http://xml.zope.org/namespaces/tal"
   xmlns:wfs="http://www.opengis.net/wfs"
   xmlns:gml="http://www.opengis.net/gml"
   xmlns:ogc="http://www.opengis.net/ogc"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   tal:attributes="xsi:schemaLocation python:'http://www.opengis.net/wfs http://schemas.opengeospatial.net/wfs/1.0.0/WFS-basic.xsd http://www.ttt.org/myns '+here.absolute_url()+'/wfs?SERVICE=WFS&amp;VERSION=1.0.0&amp;REQUEST=DescribeFeatureType&amp;TYPENAME=&amp;OUTPUTFORMAT=XMLSCHEMA'">
      
      <gml:boundedBy tal:define="bbox python:options['bbox']" tal:content="structure python:view.getGMLBoundingBox(bbox)">Feature Members Bounding Box</gml:boundedBy>
      <tal:boucle tal:define="items python:options['items']" tal:repeat="brain items">
         <gml:featureMember tal:content="structure brain/getGML">item GML section</gml:featureMember>
	  </tal:boucle>
 
 </wfs:FeatureCollection>

 