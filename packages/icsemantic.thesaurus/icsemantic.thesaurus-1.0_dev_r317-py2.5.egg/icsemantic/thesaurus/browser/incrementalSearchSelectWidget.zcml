<configure xmlns="http://namespaces.zope.org/zope"
                     xmlns:browser="http://namespaces.zope.org/browser"
                     xmlns:five="http://namespaces.zope.org/five"
                     xmlns:i18n="http://xml.zope.org/namespaces/i18n"
                     i18n_domain="icsemantic.thesaurus">
    <browser:page
        for="icsemantic.core.interfaces.IicSemanticSite"
        class=".incrementalSearchSelectWidget.IncrementalSearchSelectAzax"
        attribute="autocompleteConcepts"
        name="icsemantic.thesaurus.browser.incrementalSearchSelectWidget.autocompleteConcepts"
        permission="cmf.ManagePortal"
    />
    <browser:page
        for="icsemantic.core.interfaces.IicSemanticSite"
        class=".incrementalSearchSelectWidget.IncrementalSearchSelectAzax"
        attribute="chooseSelectedConcept"
        name="icsemantic.thesaurus.browser.incrementalSearchSelectWidget.chooseSelectedConcept"
        permission="cmf.ManagePortal"
    />
</configure>