.incrementalSearchSelect:click {
	action-server: icsemantic.thesaurus.browser.incrementalSearchSelectWidget.autocompleteConcepts;
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.autocompleteConcepts-widgetname:			nodeAttr("widgetname");
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.autocompleteConcepts-searchexpression:	currentFormVar();
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.autocompleteConcepts-concepts:			nodeAttr("concepts");
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.autocompleteConcepts-selectedindex:		nodeAttr("selectedindex");
}
.chooseSelectedConcept:click {
	action-server: icsemantic.thesaurus.browser.incrementalSearchSelectWidget.chooseSelectedConcept;
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.chooseSelectedConcept-widgetname:		nodeAttr("widgetname");
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.chooseSelectedConcept-searchexpression:	currentFormVar();
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.chooseSelectedConcept-concepts:			nodeAttr("concepts");
	icsemantic.thesaurus.browser.incrementalSearchSelectWidget.chooseSelectedConcept-selectedindex:		nodeAttr("selectedindex");
}