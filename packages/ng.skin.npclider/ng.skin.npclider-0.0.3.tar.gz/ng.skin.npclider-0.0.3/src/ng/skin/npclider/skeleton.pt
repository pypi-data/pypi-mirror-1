<metal:block define-macro="page">
  <metal:block define-slot="doctype">
  <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
</metal:block>
<html xmlns="http://www.w3.org/1999/xhtml">
<head metal:use-macro="views/ng_skin_macros/head">
  <metal:block fill-slot="other">
    <base href="" tal:attributes="href python:path('context/@@absolute_url')+'/'"/>
    <metal:block define-slot="headers" />
    <metal:block define-slot="style_slot" />
    <metal:block define-slot="ecmascript_slot" />
  </metal:block>
</head>
<body metal:define-slot="mainbody">

<div align="center">
<div id="wholepage">

<div id="header">
  <img id="headerleft" src="images/headerleft.gif" alt=""
      tal:attributes="src context/++resource++images/headerleft.gif" />
  <img id="headertitle" src="images/header.gif" alt=""
      tal:attributes="src context/++resource++images/header.gif" />
  <img id="headerright" src="images/headerright.gif" alt=""
      tal:attributes="src context/++resource++images/headerright.gif" />
</div>

<div id="leftslot">
  <div id="logoblock">
    <a href="" title="Главная страница" tal:attributes="href context/@@main/@@absolute_url"><img src="images/logo.gif" alt=""
        tal:attributes="src context/++resource++images/logo.gif" /></a>
  </div>

  <tal:block content="structure provider:leftcolumn" />
  
</div>


<div id="mainmenu">
  <div class="mainmenuitem"
      tal:repeat="item context/@@main/++nsinterface++ng.content.article.interfaces.IContentContainer/values">
    <img class="menuitemleft" src="images/menuitemleft.gif" alt=""
        tal:attributes="src context/++resource++images/menuitemleft.gif" />
    <a href="#"
        class="mainmenuitem"
        tal:attributes="href item/@@absolute_url;
                        class python: path('context/@@checksublocation').check(item) and 'mainmenuitemactive' or 'mainmenuitem'"
        tal:content="item/++nsinterface++ng.adapter.ianytitle.interfaces.IAnyTitle/title | item/__name__">
      о компании
    </a>

    <img class="menuitemright" src="images/menuitemright.gif" alt=""
        tal:attributes="src context/++resource++images/menuitemright.gif" />
  </div>
</div>

<div id="contentcontainer">
  <div id="topline">
    <img src="images/contenttopleft.gif" alt="" style="display: block; float: left;"
        tal:attributes="src context/++resource++images/contenttopleft.gif"
    />
    <img src="images/contenttopright.gif" alt="" style="display: block; float: right;"
        tal:attributes="src context/++resource++images/contenttopright.gif"
    />
  </div>
  <div id="topslogan">
    <nobr> <span tal:replace="python: u'\u25cf'" /> Проектирование <span tal:replace="python: u'\u25cf'" /> Строительство <span tal:replace="python: u'\u25cf'" /> Реконструкция <span tal:replace="python: u'\u25cf'" /></nobr>
  </div>

  <metal:block define-slot="content">
    <div id="content" metal:define-macro="content" >
      <div id="body">
          <metal:block define-slot="body"/>
      </div>          
      <metal:block use-macro="views/ng_skin_macros/localnavy"/>
    </div>
  </metal:block>

  <div id="bottomslogan">
    Научный подход к строительству
  </div>
  <div id="bottomline">
    <img src="images/contentbottomleft.gif" alt="" style="display: block; float: left;"
        tal:attributes="src context/++resource++images/contentbottomleft.gif"
    />
    <img src="images/contentbottomright.gif" alt="" style="display: block; float: right;"
        tal:attributes="src context/++resource++images/contentbottomright.gif"
    />
  </div>
  
</div>

<div id="referencemenu">
  <div class="referencemenuitem"
      tal:repeat="url context/@@main/urls">
    <img class="referencemenuitemleft" src="images/referencemenuitemleft.gif" alt=""
        tal:attributes="src context/++resource++images/referencemenuitemleft.gif" />

    <a href="#"
        tal:attributes="href url/URL;
                        title url/alternative"
        tal:content="url/title">
      link
    </a>
    <img class="referencemenuitemright" src="images/referencemenuitemright.gif" alt=""
        tal:attributes="src context/++resource++images/referencemenuitemright.gif" />

  </div>

</div>




<div id="footer">
  <img id="footerleft" src="images/footerleft.gif" alt=""
      tal:attributes="src context/++resource++images/footerleft.gif" />
  <div id="contacts">
    Контактная информация: &nbsp;&nbsp;e-mail: info@npclider.ru<br/>
    <nobr>тел.:+7 (499) 906 95 63 </nobr>
  </div>
  <img id="footerright" src="images/footerright.gif" alt=""
      tal:attributes="src context/++resource++images/footerright.gif" />
  <div id="copyright">
    <a href="http://www.dreambot.ru/" title="Site concept and development by DreamBot Team. Python, open source solutions." rel="friend">Site developed by Dreambot Team</a>
  </div>

</div>

<metal:block use-macro="views/ng_skin_macros/smartbanner" />

</div>
</div>
</body>
</html>
</metal:block>