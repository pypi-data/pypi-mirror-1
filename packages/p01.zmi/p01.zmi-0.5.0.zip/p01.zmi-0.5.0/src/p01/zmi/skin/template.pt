      <tal:block replace="structure provider:IHeaderProvider">
        header
      </tal:block>
    </div>
    <div class="clear"> </div>
    <div id="globalMenuContainer">
      <ul>
        <tal:block replace="structure provider:IGlobalMenu"> </tal:block>
