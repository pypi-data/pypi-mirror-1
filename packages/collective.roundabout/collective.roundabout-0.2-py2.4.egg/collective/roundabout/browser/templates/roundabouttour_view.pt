<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

  <head><title></title></head>


  <body>
    <metal:fill fill-slot="main">
      <metal:main_macro define-macro="main">
      	
		<tal:block tal:content="structure here/above_content|nothing"/>

		<div class="roundabout" tal:attributes="title view/getViewerData">
		<tal:block tal:repeat="item view/getImages">
			<img src="" alt="" class="roundaboutimage" tal:attributes="src item/url; alt item/data; width item/width; height item/height"/>
		</tal:block>
		<tal:block tal:repeat="item view/getMaps">
			<img src="" alt="" class="roundaboutmap" tal:attributes="src item/url; alt item/data; width item/width; height item/height"/>
		</tal:block>
		</div>

		<tal:block tal:content="structure here/below_content|nothing"/>

      </metal:main_macro>
    </metal:fill>
  </body>
</html>
