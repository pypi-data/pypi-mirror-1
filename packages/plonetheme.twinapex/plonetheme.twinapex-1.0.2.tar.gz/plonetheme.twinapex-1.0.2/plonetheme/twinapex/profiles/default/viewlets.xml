<?xml version="1.0"?>

<object>

  <hidden manager="plone.portaltop" skinname="Twinapex Theme">
  	<!-- Move breadcrumbs  from portaltop to contentview -->
    <viewlet name="plone.path_bar" />
	<!-- Move lang selector from portaltop to header -->
	<viewlet name="plone.app.i18n.locales.languageselector" />
  </hidden>

  <hidden manager="plone.portalheader" skinname="Twinapex Theme">
    <viewlet name="plone.global_sections" />
  </hidden>  

  <order manager="plone.portalheader" skinname="Twinapex Theme">
    <viewlet name="plone.app.i18n.locales.languageselector" insert-before="*" />
  </order>
   
  <order manager="plone.contentviews" skinname="Twinapex Theme">
    <viewlet name="plone.path_bar" insert-before="*" />
  </order>

  <order manager="twinapex.navmanager" skinname="Twinapex Theme">
    <viewlet name="plone.path_bar" insert-before="*" />
  </order>
  
  <!-- We use custon navigation bar -->
  <hidden manager="plone.contentviews" skinname="Twinapex Theme">
    <viewlet name="plone.path_bar" />
  </hidden>

  <hidden manager="plone.portalfooter" skinname="Twinapex Theme">
    <viewlet name="plone.colophon" />
  </hidden>

</object>
