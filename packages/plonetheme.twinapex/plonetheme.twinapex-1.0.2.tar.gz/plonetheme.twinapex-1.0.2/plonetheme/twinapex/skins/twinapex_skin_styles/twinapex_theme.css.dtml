/* Stylesheet for the 'Twinapex Theme' Plone theme */

/* <dtml-with base_properties> */
/* <dtml-call "REQUEST.set('portal_url', portal_url())"> */

html, body {
    height: 100%;
}

body {
	margin: 0;
	padding: 0;
	font-size: &dtml-fontSize;; /* Apply default font size over Plone em based font definitions */
    background: white url(++resource++plonetheme.twinapex.images/stripe.png);	
}

.documentContent, .documentContent table {
	font-size: &dtml-fontSize;;
}

/* --- Base --- */
h1 {
    border-bottom: none;
    margin-top: 15px;
    margin-bottom: 15px;
}
.documentFirstHeading {
    margin-top: 15px;
}

h1 {
  font-size: 1.8em;
}

h1, h2, h3, h4, h5, h6 {
    color: &dtml-headingColor;;
}

h3, h4, h5, h6 {
    color: &dtml-fontColor;;
    font-size: 1.2em;
}

h3 {
    margin-top: 20px;
}

p {
    margin-bottom: 1.5em;
}

li {
  margin-bottom: 0;
}

table {
	font-size: &dtml-fontSize;;
}

/* no link underlinings */
a,
a:active,
a:visited,
.documentContent li a,
.documentContent li a:active,
.documentContent li a:visited,
.documentContent p a,
.documentContent p a:visited,
.documentContent p a:active {
	border: 0;
	text-decoration: none;
}

.kupu a,
.kupu a:active,
.kupu a:visited {
	border: 0 !important;
	text-decoration: none !important;
}

a:hover,
.documentContent li a:hover,
.documentContent p a:hover {
  text-decoration: underline;
}

.kupu a:hover {
   text-decoration: underline  !important;
}

#content a[href^="http:"], a.link-external {
	background: none;
	padding: 0;
}


/* Content styles */
.image-right, .image-left {
  border: none;
}

/* Make text area text more visible than normal grey */
input[type=text],textarea,select {
	color: black;
}

.documentContent ul {
    list-style-image: url(++resource++plonetheme.twinapex.images/li.png);
}

/* --- Main layout --- */
#visual-portal-wrapper {
  width: 960px;
  margin: 0;
  padding: 0;
  margin-left: auto;
  margin-right: auto;  
}

#margin-wrapper {
	width: 920px;
	padding: 0;
	margin: 0;
	padding-left: 20px;
	padding-right: 20px;
	background: white;
}

/* Shadows */
#shadows {
    margin: 0 auto;
    height: 100%;
    min-height: 100%;
    position: relative;
    background: black;
    border: 0;
    border-collapse: collapse;
}

#shadows, #shadow-left, #shadow-right, #shadow-center, #shadow-footer {
    border-spacing: 0;
    padding: 0;
}

#shadow-center, #shadow-footer {
   border-right: solid 2px black;
   border-left: solid 2px black;
   vertical-align: top;
}

#shadow-footer {
  height: 30px;
}

.shadow-dummy {
    width: 20px;
}

#shadow-left {
    background: white url(++resource++plonetheme.twinapex.images/shadow-left.gif) repeat-y top right;
    width: 30px;
}
#shadow-right {
    background: url(++resource++plonetheme.twinapex.images/shadow-right.gif) white repeat-y top left;
    width: 30px;
}

#shadow-center {
    background-color: #FFF;
}

#shadow-footer {
	background: black;
	color: white;
}

#portal-header {
  padding-top: 17px;
}

.main-layout {
	margin: 0;
	padding: 0;
	border: 0;
}

#portal-columns  {
  margin-top: 0px;
  border-collapse: collapse;
}

#portal-column-one {
    padding: 0;   
}

#portal-column-two {
	padding-top: 20px;
}

#portal-column-two {
}

#portal-column-one .portletWrapper {
	margin-right: 20px;
	width: 170px;
}

/* XXX: remove
#portal-column-one {
	width: 170px;
    padding: 0;   
    padding-right: 20px;	
}*/

#portal-breadcrumbs {
   float: left;
   display: inline;
   font-size: &dtml-fontSize;;
}

.path-bar {
   padding-left:190px;
   padding-top: 29px;
   padding-bottom: 5px;
}

.documentActions {
	display: inline;
	float: right;
	margin: 0;
	padding: 0;
	border: 0;	
    width: 200px;	
}

.documentActions ul {
	display: inline;
}

.documentActions ul li {
	padding: 0;
	margin: 0;
	margin-left: 10px;
}

.header-cell {
	width: 750px;
	margin: 0;
	margin-left: 20px;
	padding: 0;
	padding-bottom: 20px;
	overflow: hidden;
}


.header-cell object {
	padding: 0;
	margin: 0;
}

#portal-column-two {
   margin: 0;
   padding: 0;   
   padding-top: 0px;
   padding-left:20px;
   margin-right:20px;
   width:244px;   
}

#portal-column-content {
	width: 486px;
	padding-top: 1px;
	padding-left: 0px;
	padding-right: 0px;
}

table.with-left-portlets #portal-column-content {
	width: 726px;
}

table.with-right-portlets #portal-column-content {
	width: 656px;
}

.documentContent {
	border-top: 1px solid &dtml-globalBorderColor;;
}

.header-layout .documentContent {
	border-top: 0;
}

#portal-column-wrapper {
  position: relative;
}

#region-content {  /* also add .documentEditable here? */
    margin: 0;
    padding: 0 !important;
    padding-top: 20px;
}

#portal-column-one .visualPadding, #portal-column-two .visualPadding {
  padding: 0;
}


/* --- Viewlets --- */

/* Logo */
#portal-logo img {
  margin-left: 0;
  margin-top: 1.0em;
  margin-bottom: 1.7em;
}

/* Site actions */
#portal-siteactions {
    padding-right: 0;
    letter-spacing: -0.05em;
    font-family: &dtml-componentFont;;
}

#portal-siteactions li {
  border-left: 1px solid #8c8c8c;
}

#portal-siteactions li#siteaction-contact {
	border: 0; /* leftmost */
}

#portal-siteactions li a,
#portal-siteactions li a:visited,
#portal-siteactions li a:active,
#portal-siteactions li a:hover {
  margin-right: 0;
  padding-right: 4px;
  padding-left: 8px;
  font-size: 1.0em;
  text-transform: none;
  color: &dtml-linkColor;;
}

#portal-siteactions li a:hover {
    border: &dtml-borderWidth; &dtml-borderStyle; #FFF;
    text-decoration: underline;
}

#portal-siteactions li#siteaction-sitemap {
    border-right: none;
}

/* Search bar */
#portal-searchbox {
    margin-top: 0.7em;
    padding: 0;
    margin-right: 0;
}

#portal-searchbox input.searchButton {
    margin-bottom: &dtml-borderWidth; ! important;
    color: white;
    font-size: 14px;
    background-image: url(++resource++plonetheme.twinapex.images/searchbg.gif);;
    border: none;
    text-align: center;
    width:  102px;
    height: 32px;
    padding: 1px;
}

#searchGadget {
    border: &dtml-borderWidth; solid &dtml-searchBorderColor;;
    background-image: none;
    height: 20px;
    line-height: 23px;
    width: 195px;
    font-size: 14px;
    vertical-align: middle;
    padding-top: 4px;    
}

.searchSection {
    display: none;
}

.LSResult {
	z-index:22;
}

/* Globalnav */
#portal-globalnav {
    background: white url(++resource++plonetheme.twinapex.images/menubar.png) repeat-x 50% 50%;
    margin: 0;
    border: 0;
	font-size: 14px;
	height: 32px;
	font-weight: normal;
	letter-spacing: 0.1em;
}

#portal-globalnav li.top-menu {
    margin: 0;
    padding-top: 8px;
    padding-bottom: 8px;
    border: 0;
    color: black;  
    padding-right: 14px;
    padding-left: 14px;
    border-right: 1px solid #600;
}

#portal-globalnav li a {
    padding: 0;
    margin: 0;
    border: 0;
}

#portal-globalnav li a, #portal-globalnav li.selected a, #portal-globalnav li a:hover {
    color: white;
    border: 0;
    margin: 0;
    margin-left. 8px,
    margin-right: 8px;
    padding 0;
    text-decoration: none;
    background-color: transparent;
}


#portal-globalnav li:hover {
	background: &dtml-linkColor;;
}



/* Student button */
li#portaltab-blog,
li#portaltab-events {
	overflow: hidden;
	width: 70px;
	float: right;
	text-align: center;
}

/* Personal tools */
#portal-personaltools-wrapper {
    clear: left; /* clears floated navbar */
    border-top: none;
}

/* Breacrumbs */
#portal-breadcrumbs {
    padding: 0;
    border: 0;
}

#portal-breadcrumbs a {
	color: #CC0000;
}

#portal-breadcrumbs a:hover {
	text-decoration: underline;
}

/* Footer */
#portal-footer {
    text-align: left;
    border: 0;
    margin: 0;
    padding:16px;
    background: transparent;
    color: rgb(152, 152, 152);
}


#copyright {
	margin-right: 20px;
}



#portal-footer a,
#portal-footer a:active,
#portal-footer a:visited,
#portal-footer a:hover {
    color: &dtml-linkColor;;
}

#portal-footer .footer-item {
    margin: 0;
    padding: 0;
}

#portal-footer .footer-item a.text-link {
    padding-right: 7px;
    margin-right: 5px;    
    border-right: 1px solid rgb(152, 152, 152);
}

#portal-footer span.last a {
	border: 0 !important;
}




/* Full width layout overrides */

.full-width #portal-column-one,
.full-width #portal-breadcrumbs,
.full-width #portal-column-two
{
	display: none;
}

.full-width #portal-column-content {
	margin: 0;
	padding: 0;
    width: 920px;
}


/* --- Portlets --- */
.portlet {
    margin-bottom: 2.5em;
}

.portletHeader {
    border-left: none;
    border-right: none;
    /*height: 38px;
    line-height: 38px;*/
    padding-top: 8px;
    padding-bottom: 8px;
    text-transform: uppercase;
    font-size: 1.4em;
    padding-right: 2px;
    background-image: url(++resource++plonetheme.twinapex.images/portlet_header.png);
    background-repeat: no-repeat;
    padding-left: 35px;
    line-height: 14px;
}

.header-layout #portal-column-two .portletHeader:first-child {
/*	border-top: 0;*/
}

.portletHeader span {
    color: &dtml-portletHeadingColor;;
}

.portletItem {
    padding-top: 12px;
    border: none;
}

.portletItem p {
    font-size: &dtml-fontSize;;
}

.portletTopRight, .portletTopLeft, .portletBottomLeft, .portletBottomRight {
    display: none;
}

/* Portlet Navtree */
.portletNavigationTree,
.portletSectionNavigationTree {
    border-bottom: none;
}

.portletNavigationTree .portletHeader,
.portletSectionNavigationTree .portletHeader {
    height: auto;
    background-image: none;
    padding-left: 0;
    border-bottom: none;
    line-height: 135%;
    font-size: 1.3em;
    padding-bottom: 15px;
}

.portletNavigationTree .portletHeader span,
.portletSectionNavigationTree .portletHeader span {
    color: &dtml-discreetColor;;
    font-weight: normal;
}

.portletNavigationTree .portletItem,
.portletSectionNavigationTree .portletItem {
    padding: 0;
}

.navTree li {
    margin-bottom: 0;
}

.navTreeItem {
    /* border-bottom: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor;; */
}

.navTreeItem a:hover,
dd.portletItem .navTreeItem a:hover {
    border: &dtml-borderWidth; &dtml-borderStyle; #FFF;
    text-decoration: underline;
}

.navTreeItem a:focus,
dd.portletItem .navTreeItem a:focus {
	border: 1px dashed grey;
}

.navTreeItem img {
    display: none;
}

dd.portletItem .navTreeItem a {
    font-size: &dtml-fontSize;;
    color: &dtml-discreetColor;;
    padding-top: 3px;
    padding-bottom: 1px;
}

.navTreeCurrentNode {
  font-weight: bold;
  color: #CC0000 !important;
}



.navTreeCurrentItem {
    border: &dtml-borderWidth; &dtml-borderStyle; #FFF !important;
}

.navTreeItemInPath {
    border: &dtml-borderWidth; &dtml-borderStyle; #FFF !important;
    color: #CC0000 !important;
    font-weight: bold;
}

.navTreeItemInPath .navTreeItem {
    font-weight: normal;
}

.navTreeLevel1 {
}

.navTreeLevel1 .navTreeItem a {
	  color: #CC0000 !important;
	  margin-left: 0em;
}

.navTreeLevel1 .navTreeCurrentNode {
  font-weight: bold !important;
}

.navTreeCurrentItem {
    font-weight: bold !important;
}

/* Don't highlight all the text under the selection */
.navTreeCurrentNode ul {
	font-weight: normal !important;
}

.navTreeCurrentNode ul .navTreeCurrentNode {
	font-weight: bold !important;
}



/* Static portlet */

.portletStaticText .portletItem {
   padding: 8px;
   padding-top:12px;
}

.portletGrey {
	background-color: &dtml-evenRowBackgroundColor;;
}

.portletStaticText .portletItem ul {
    list-style-image: url(++resource++plonetheme.twinapex.images/li.png);
/*    list-style-type: none;*/
    margin-left: 0.5em;
    padding-left: 24px;
}

.portletStaticText .portletItem ul a, .portletStaticText .portletItem ul a:visited {
    color: &dtml-portletStaticLinkColor;;
}

.portletStaticText .portletItem ul li {
  margin-bottom: 0.3em;
}

/* News */

.portletNews {
	background: &dtml-evenRowBackgroundColor;;
}

.portletNews dd {
	margin-left: 0; /* IE style reset */
}

.portletNews-body {
   padding: 8px;
   padding-top:12px;
}

.portletNews .portletItem a {
    color: &dtml-linkColor;;
}

.portletNews .portletItem {
    padding: 1.0em 1.6em;
}

.portletNews .portletItem a img {
    display: none;
}

.portletNews .portletItemDetails {
    text-align: left;
}

.portletNews .portletFooter {
  display: none;
}

.portletNews .odd {
    background: &dtml-evenRowBackgroundColor;;
}

/* Special header layout fixes */

.headerClass {
	
}

/* --- Management end overrides over overrides --- */
ul.configlets {
    list-style-image:none;
    list-style-position:outside;
    list-style-type:none;
}

.managedPortletActions a {
    background-image: none;
    padding-left: 3px;
}

.managedPortlet .portletHeader {
    min-height: auto !important;
    background: none;
}

.managedPortlet .portletHeader a {
    padding-left: 0;
}

/* --- Dropdown menus (webcourtier.dropdownmenu) --- */

#portal-globalnav ul {
    border: none !important;
    z-index: 10;
    display: block;
}

#portal-globalnav ul a {
    padding-left: 1.3em;
    padding-right: 1.5em;
}

#portal-globalnav ul.submenu {
  	top: 32px;
  	margin-left:-14px;
    border: none !important;
    font-size: 12px;
    letter-spacing: 0;
}


#portal-globalnav ul.submenu a {
    background-color: #3A3A3A;
    line-height: 35px;
    margin-top: 10px;
    padding-bottom: 4px;

    border-color: black !important;
    border-top: 1px solid black !important;
}

#portal-globalnav ul.submenu a,
#portal-globalnav ul.submenu a:visited,
#portal-globalnav ul.submenu a:active,
#portal-globalnav ul.submenu a:hover {
    color: &dtml-navBarLinkColor; !important;
    margin: 0;
    padding: 0;
    padding-left: 10px;
}


/* Submenu styles */ 
#portal-globalnav ul {
    width: 300px;
    border-color: blue;
}

#portal-globalnav li ul a:hover {
   color: white !important;
   background-color: #c00 !important;
   text-decoration: none;    
}

#portal-globalnav li ul a:hover, /* fix for IE6  */
#portal-globalnav li.selected ul.submenu a:hover {
    border-top: 1px solid black !important;
}

#portal-globalnav li.selected ul.submenu {
    margin-top: 0px;
}

a.hasDropDown {
    background-image:url('arrowRight.gif') !important;
    background-position:290px 50% !important;
    background-repeat:no-repeat !important;
}

/* side-dropdown menu styles */
#portal-globalnav li ul ul {
    top:0;
    margin: 0 0 0 20em;
}

#portal-globalnav .navTreeItem {
    border-bottom: none;
    border-left: 1px solid black;
}

/* Authoring */

#content h2.tileHeadline {
    clear: none;
    font-size: 150%;
}

.contenttype-customer-reference .tileImage {
    float: left;
    margin: 1em;
}


.currentItem {
	border-color: #c00;
}

/* Added Kupu styles */
p.twinapex-breaker {
	margin: 0;
	padding: 0;
	padding-bottom: 5px;
	margin-bottom: 5px;
	border-bottom: 1px solid &dtml-globalBorderColor;;	
}

ul.orange-bullet {
    list-style-image: url(++resource++plonetheme.twinapex.images/li.png);
    margin-left: 5px;
    font-size: 1.2em;
    padding-left: 24px;
}

ul.orange-bullet a,
ul.orange-bullet a:active,
ul.orange-bullet a:visited,
ul.orange-bullet a:hover,
.documentContent li a,
.documentContent li a:active,
.documentContent li a:visited,
.documentContent li a:hover {
	color: &dtml-linkColor; !important;
}

.documentContent td {
	/* equal to <p> in base.css */
	line-height: 1.5em; 
}

ul.blue-bullet {
    list-style-image: url(++resource++plonetheme.twinapex.images/li.png);
    margin-left: 5px;
    font-size: 1.2em;
    padding-left: 24px;
}

ul.blue-bullet a,
ul.blue-bullet a:active,
ul.blue-bullet a:visited,
ul.blue-bullet a:hover {
	color: #CC0000 !important;
}

ul.grey-bullet {
    list-style-image: url(++resource++plonetheme.twinapex.images/li_grey.png);
    margin-left: 5px;
    font-size: 1.2em;
    padding-left: 24px;
}

p.separated {
    padding-bottom: 5px;
	border-bottom: 1px solid #aca899;
}

p.grey-box {
	background: #f8f8f8;
	border-top: 3px solid white;
	border-bottom: 3px solid white;	
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 40px;
	padding-right: 50px;
	margin: 0px;
}

span.yellow-text {
	color: #E88233;
}

/* Need to make double borders from kupu, see fixStyles javascript */
div.grey-box-wrapper {
	border-top: 1px solid #e9eaeb;
	border-bottom: 1px solid #e9eaeb;	
}

h3.blue {
	color: #CC0000;
}

.documentContent h3,
h3.blue {
}

.state-published {
	color: #CC0000 !important;	
}

table.twinapex-table {
	width: 100%;
	
	border: 0;
	border-collapse: collapse;
	margin-top: 2px;	
}

/* jquery magic - this one is  created dynamically */
div.table-wrapper {
	border-top: 1px solid #e9eaeb;
	
	/* For some reason font size gets bumped back to 10 px if this 
	   definition is not here */
	font-size:12px !important;
}

table.twinapex-table td,
table.twinapex-table th {
	padding-top: 4px;
	padding-bottom: 4px;
	vertical-align: middle;
	text-align: left;
	padding-left: 8px;
	border-bottom: 1px solid #e9eaeb;
	vertical-align: top;
	
}

table.grey-all {
	background: #f8f8f8;
}

table.grey-all thead th {
	font-weight: normal;
}

table.grey-all thead th,
table.grey-all tbody th {
	color: #CC0000;
	font-weight: bold;	
	width: 180px;
}

table.grey-all thead th,
table.grey-all tbody th,
table.grey-all td {
	padding-top: 12px;
	padding-bottom: 12px;
}

table.orange-table thead th {
	background: #f8f8f8;
	color: #E88233;
	font-weight: bold;	
}

table.blue-table thead th {
	background: #f8f8f8;
	color: #CC0000;
	font-weight: bold;	
}

/* Kupu tabs for collective.kuputabs product - include kuputabs.css.dtml in the registry */

ul.kuputabs-selectors {
	background: #eee; 4223424
	height: auto;
}

li.kuputab-selector a {
    border-top: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor; !important;
    border-bottom: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor; !important;
    border-left: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor; !important;
}

li.kuputab-selector a.selected {
    background: #c00 !important;
    color: white !important;
}

li.kuputab-selector a:hover {
    background:#c00;
    color: white !important;
    text-decoration: none;
}


li.kuputab-selector-last a {
   border-right: &dtml-borderWidth; &dtml-borderStyle; &dtml-globalBorderColor; !important;
}

/* Front page specials */

table.front-page h2 a {
	color: #c00;
}

table.front-page {
	width:100%;
	margin: 0;
	border: 0;
	padding: 0;
	border-collapse: collapse;
}

table.front-page td {
	vertical-align: top;
	padding: 10px;
}	

table.front-page .column-2 {
	width: 50%
}

table.front-page .timestamp {
	font-size:80%;
	text-align: right;
	color: &dtml-globalBorderColor;;
	margin-top: 8px;
	margin-bottom: 0;
}

table.front-page p.more {
	text-align: right;
}
table.front-page-table td {
	width: 230px;
	text-align: center;
	vertical-align: top;
	padding: 0 12px;
}
table.front-page-table img {
	border: 1px solid black;
	margin-bottom: 10px;
}
table.front-page-table h2 {
	border: 0;
}

table.front-page h2 .title-link {
    float: left;
}

table.front-page .rss-link {
    float: right;
}

table.front-page h2 a {
   color: black !important;
}

table.front-page h2 {
   margin-bottom: 1em;
   font-size: 12px;
   font-weight: bold;
   border: 0;
}


table.front-page .slogan {
      font-size: 32px;
      color: #c00;
}

table.introduction {
      font-size: 16px;
}

table.introduction ul {
     list-style: none;
     margin: 0;
}

table.introduction li {
     margin-bottom: 1em;
