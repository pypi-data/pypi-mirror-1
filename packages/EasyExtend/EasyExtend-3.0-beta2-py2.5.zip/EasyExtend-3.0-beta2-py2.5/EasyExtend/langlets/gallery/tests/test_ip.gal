import unittest
import test_support

class TestIP(unittest.TestCase):
    def test_ip(self):
        self.assertEqual( isinstance(129.0.0.89, tuple), True)
        self.assertEqual( 129.0.0.89[0], 129)

def test_main():
    test_support.run_unittest(TestIP)

if __name__ == "__main__":
    test_main()


