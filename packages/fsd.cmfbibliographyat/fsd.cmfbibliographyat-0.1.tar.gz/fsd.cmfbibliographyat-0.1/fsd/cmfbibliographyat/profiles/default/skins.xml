<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="fsd_cmfbibliographyat">

 <object name="fsd_cmfbibliographyat_custom_images"
    meta_type="Filesystem Directory View"
    directory="fsd.cmfbibliographyat:skins/fsd_cmfbibliographyat_custom_images"/>
 <object name="fsd_cmfbibliographyat_custom_templates"
    meta_type="Filesystem Directory View"
    directory="fsd.cmfbibliographyat:skins/fsd_cmfbibliographyat_custom_templates"/>
 <object name="fsd_cmfbibliographyat_styles"
    meta_type="Filesystem Directory View"
    directory="fsd.cmfbibliographyat:skins/fsd_cmfbibliographyat_styles"/>

 <skin-path name="fsd_cmfbibliographyat" based-on="Plone Default">
  <layer name="fsd_cmfbibliographyat_custom_images"
     insert-after="custom"/>
  <layer name="fsd_cmfbibliographyat_custom_templates"
     insert-after="fsd_cmfbibliographyat_custom_images"/>
  <layer name="fsd_cmfbibliographyat_styles"
     insert-after="fsd_cmfbibliographyat_custom_templates"/>
 </skin-path>

</object>
