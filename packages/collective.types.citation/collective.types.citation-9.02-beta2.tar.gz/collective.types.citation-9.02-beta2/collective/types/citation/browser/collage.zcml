<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:zcml="http://namespaces.zope.org/zcml"
    i18n_domain="myph.types">

  <!-- (C) 2005-2009. University of Washington. All rights reserved. -->

  
  <!-- Citation views -->
  <browser:pages
      for="collective.types.citation.content.ICitation"
      class=".collage.CollageView"
      permission="zope2.View"
      layer="Products.Collage.interfaces.ICollageBrowserLayer" >
    <browser:page
        name="article" />
    <browser:page
        name="book" />
    <browser:page
        name="report" />
    <browser:page
        name="other" />
  </browser:pages>

</configure>