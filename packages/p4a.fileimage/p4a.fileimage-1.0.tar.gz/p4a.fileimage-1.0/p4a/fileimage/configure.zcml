<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

  <browser:page
      for="*"
      name="downloadfile"
      permission="zope2.View"
      class=".browser.DownloadFile"
      />
  <browser:page
      for="*"
      name="viewimage"
      permission="zope2.View"
      class=".browser.ViewImage"
      />

  <include package=".file" />
  <include package=".image" />
    
</configure>
