<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="sc.base.memberdataexport">
  <configlet title="Configure Export Member Data" action_id="memberdata_export-panel"
     appId="sc.base.memberdataexport" category="Products" condition_expr=""
     url_expr="string:${portal_url}/@@plone-memberdata-export-panel"
     visible="True" i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
  <configlet title="Export Member Data" action_id="memberdata_export"
     appId="sc.base.memberdataexport" category="Products" condition_expr=""
     url_expr="string:${portal_url}/@@plone-memberdata-export"
     visible="True" i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
