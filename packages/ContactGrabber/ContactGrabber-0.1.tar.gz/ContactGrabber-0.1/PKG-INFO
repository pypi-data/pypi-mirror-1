        try:
        emails = grab_contacts('mike', 'password_of_mike', 'rediff')
        if len(emails) == 0:
        print 'No contacts found. Did you enter invalid user ID / pasword?'
        print str(emails)
        except:
        print 'Invalid UserID/Password'
        
