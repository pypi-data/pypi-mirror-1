"""Create a new environment by invoking virtualenv."""

from __future__ import absolute_import
import sys, os, vecmd
from vecmd import script

def main(*args):
    return vecmd.virtualenv(*args)

if __name__ == '__main__':
    main(*sys.argv[1:])
