<configure
	xmlns="http://namespaces.zope.org/zope"
	xmlns:five="http://namespaces.zope.org/five"
	i18n_domain='icSemantic'>

	<utility
		factory=".multilingual.ContentTypesMultilingualPatcher"
		/>

	<adapter
	    for="Products.Archetypes.Field.TextField"
	    provides="..interfaces.IFieldEmptiness"
	    factory=".fields.TextFieldEmptiness"
	    />

	<adapter
	    for="*"
	    provides="..interfaces.IFieldEmptiness"
	    factory=".fields.FieldEmptiness"
	    />

</configure>
