<table border="0" class="ordered-selection-field"
       tal:define="view_name python: view.name">
  <tr>
    <td>
      <select id="from" name="from" size="5" multiple=""
          tal:attributes="name python:'%s:list'%view_name;
                          id python:'%s:list'%view_name;">
        <option tal:repeat="entry python: view.choices()"
                tal:attributes="value entry/value"
                tal:content="entry/text" i18n:translate=""/>
      </select>
    </td>
  </tr>
</table>

