@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix cctk: <http://www.cct.lsu.edu/~dstark/cctk/0.1/> .
@prefix cactus: <http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/> .
@prefix : <http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/> .

<http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil>
    rdf:type cctk:Thorn;
    cctk:Parameter <http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil/out_dir>;
    cctk:Parameter <http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil/out_every>;
    cctk:Parameter <http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil/verbose> .
    
<http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil/out_every>
    rdf:type cctk:Parameter;
    rdf:type cctk:IntegerParameter;
    cctk:value 1 .

<http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil/out_dir>
    rdf:type cctk:Parameter;
    rdf:type cctk:StringParameter;
    cctk:value "." .

<http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/IOUtil/verbose>
    rdf:type cctk:Parameter;
    rdf:type cctk:KeywordParameter;
    cctk:value "full" .

<http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/Thorn/CarpetIOHDF5>
    rdf:type cctk:Thorn;
    cctk:out_dir ".";
    cctk:out_every 1;
    cctk:checkpoint "false" .
    
