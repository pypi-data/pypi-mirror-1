<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE owl [
  <!ENTITY dc     'http://purl.org/dc/elements/1.1/'>
  <!ENTITY rdf    'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
  <!ENTITY xsd    'http://www.w3.org/2001/XMLSchema#'>
  <!ENTITY xsd    'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
  <!ENTITY cctk   'http://www.cct.lsu.edu/~dstark/cctk/0.1/'>
  <!ENTITY cactus 'http://rdfstore.astrogrid.de/cactus-simulations/uniqeID/'>
]>
<rdf:RDF
  xmlns:dc   = "&dc;"
  xmlns:rdf  = "&rdf;"
  xmlns:xsd  = "&xsd;"
  xmlns:cctk = "&cctk;"

  xml:base   = "&cactus;"
>

<!-- ============================================================ -->
<!-- unique ID for this simulation                                -->
<!-- ============================================================ -->
<rdf:Description rdf:ID="&cactus;">
  <cctk:simulationID>"&cactus;"</cctk:simulationID>
</rdf:Description>

<!-- ============================================================ -->
<!-- graph with all active thorns                                 -->
<!-- ============================================================ -->
<cctk:ThornList rdf:ID="ThornList">
  <cctk:containsThorn rdf:resource="#Thorns/IOUtil"/>
  <cctk:containsThorn rdf:resource="#Thorns/CarpetIOHDF5"/>
</cctk:ThornList>

<!--
<rdf:Description rdf:ID="ThornList">
  <cctk:ThornList rdf:parseType="Collection">
    <rdf:Description rdf:resource="#ThornList/IOUtil"/>
    <rdf:Description rdf:resource="#ThornList/CarpetIOHDF5"/>
    <rdf:Description rdf:resource="#ThornList/C"/>
  </cctk:ThornList>
</rdf:Description>
-->

<!-- ============================================================ -->
<!-- thorn graphs with their parameters                           -->
<!-- ============================================================ -->
<cctk:Thorn rdf:ID="Thorns/IOUtil">
  <cctk:hasName>IOUtil</cctk:hasName>
  <cctk:hasParameter rdf:resource="#Parameters/IOUtil/out_every"/>
  <cctk:hasParameter rdf:resource="#Parameters/IOUtil/out_dir"/>
  <cctk:hasParameter rdf:resource="#Parameters/IOUtil/checkpoint_every"/>
  <cctk:hasParameter rdf:resource="#Parameters/IOUtil/verbose"/>
</cctk:Thorn>

<cctk:Thorn rdf:ID="Thorns/CarpetIOHDF5">
  <cctk:hasName>CarpetIOHDF5</cctk:hasName>
  <cctk:hasParameter rdf:resource="#Parameters/CarpetIOHDF5/out_every"/>
  <cctk:hasParameter rdf:resource="#Parameters/CarpetIOHDF5/checkpoint"/>
  <cctk:hasParameter rdf:resource="#Parameters/CarpetIOHDF5/out_dir"/>
  <cctk:hasParameter rdf:resource="#Parameters/CarpetIOHDF5/out_vars"/>
</cctk:Thorn>

<!-- ============================================================ -->
<!-- list of parameters and their values                          -->
<!-- ============================================================ -->
<cctk:IntegerParameter rdf:ID="Parameters/IOUtil/out_every">
  <cctk:hasName>IO::out_every</cctk:hasName>
  <cctk:hasValue>1</cctk:hasValue>
</cctk:IntegerParameter>
<cctk:StringParameter rdf:ID="Parameters/IOUtil/out_dir">
  <cctk:hasName>IO::out_dir</cctk:hasName>
  <cctk:hasValue>"."</cctk:hasValue>
</cctk:StringParameter>
<cctk:RealParameter rdf:ID="Parameters/IOUtil/checkpoint_every">
  <cctk:hasName>IO::checkpoint_every</cctk:hasName>
  <cctk:hasValue>1</cctk:hasValue>
</cctk:RealParameter>
<cctk:KeywordParameter rdf:ID="Parameters/IOUtil/verbose">
  <cctk:hasName>IO::verbose</cctk:hasName>
  <cctk:hasValue>"full"</cctk:hasValue>
</cctk:KeywordParameter>

<cctk:IntegerParameter rdf:ID="Parameters/CarpetIOHDF5/out_every">
  <cctk:hasName>IOHDF5::out_every</cctk:hasName>
  <cctk:hasValue>20</cctk:hasValue>
</cctk:IntegerParameter>
<cctk:StringParameter rdf:ID="Parameters/CarpetIOHDF5/out_dir">
  <cctk:hasName>IOHDF5::out_dir</cctk:hasName>
  <cctk:hasValue>"hdf5"</cctk:hasValue>
</cctk:StringParameter>
<cctk:StringParameter rdf:ID="Parameters/CarpetIOHDF5/out_vars">
  <cctk:hasName>IOHDF5::out_vars</cctk:hasName>
  <cctk:hasValue>"wavetoy::phi"</cctk:hasValue>
</cctk:StringParameter>
<cctk:BooleanParameter rdf:ID="Parameters/CarpetIOHDF5/checkpoint">
  <cctk:hasName>IOHDF5::checkpoint</cctk:hasName>
  <cctk:hasValue>false</cctk:hasValue>
</cctk:BooleanParameter>

</rdf:RDF>
