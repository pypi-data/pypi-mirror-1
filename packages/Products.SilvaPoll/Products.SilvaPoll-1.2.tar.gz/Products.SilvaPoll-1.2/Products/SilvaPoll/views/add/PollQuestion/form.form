<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype>None</enctype>
  <method>POST</method>

  <i18n_domain>silva_poll</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>object_id</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>id</title>
          <truncate type="int">0</truncate>
          <description>No spaces or special characters besides &amp;#8216;_&amp;#8217; or &amp;#8216;-&amp;#8217; or &amp;#8216;.&amp;#8217;</description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
          <extra>string: id="field_${field/id}"</extra>
        </tales>
      </field>

      <field><id>object_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <required type="int">0</required>
          <css_class>store</css_class>
          <default></default>
          <title>title of poll</title>
          <truncate type="int">0</truncate>
          <description>Optional</description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>

      <field><id>question</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">2</height>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>question</title>
          <description></description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>

      <field><id>answers</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <max_linelength></max_linelength>
          <max_length></max_length>
          <max_lines></max_lines>
          <width type="int">40</width>
          <external_validator></external_validator>
          <height type="int">5</height>
          <required type="int">1</required>
          <css_class>store</css_class>
          <default></default>
          <title>answers</title>
          <description>Enter multiple answers separated by empty lines.</description>
          <extra></extra>
          <unicode type="int">1</unicode>
        </values>
        <tales>
        </tales>
      </field>

      </fields>
    </group>
  </groups>
</form>
