<?xml version="1.0"?>
<object>

  <hidden manager="plone.portalheader" skinname="Overlapped Tabs">
    <viewlet name="plone.searchbox" />
    <viewlet name="plone.site_actions" />
  </hidden>

  <hidden manager="plone.portaltop" skinname="Overlapped Tabs">
    <viewlet name="plone.path_bar" />
    <viewlet name="plone.personal_bar" />
  </hidden>

  <hidden manager="plone.belowcontentbody" skinname="Overlapped Tabs">
    <viewlet name="plone.belowcontentbody.workflowhistory" />
  </hidden>

  <order manager="plone.portalheader" skinname="Overlapped Tabs"
         based-on="Plone Default">
    <viewlet name="plone.global_sections" insert-before="*" />
  </order>

  <order manager="plone.portalfooter" skinname="Overlapped Tabs"
         based-on="Plone Default">
    <viewlet name="plone.global_sections_line_below" insert-before="*"/>
  </order>

  <order manager="plone.contentviews" skinname="Overlapped Tabs"
         based-on="Plone Default">
    <viewlet name="plone.global_sections_line_above" insert-after="*"/>
  </order>

  <hidden manager="plone.portalfooter" skinname="Overlapped Tabs">
    <viewlet name="plone.footer" />
    <viewlet name="plone.analytics" />
  </hidden>

</object>
