<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Overlapped Tabs">

 <object name="plonetheme_overlappedtabs_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.overlappedtabs:skins/plonetheme_overlappedtabs_custom_images"/>
 <object name="plonetheme_overlappedtabs_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.overlappedtabs:skins/plonetheme_overlappedtabs_custom_templates"/>
 <object name="plonetheme_overlappedtabs_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.overlappedtabs:skins/plonetheme_overlappedtabs_styles"/>

 <skin-path name="Overlapped Tabs" based-on="Plone Default">
  <layer name="plonetheme_overlappedtabs_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_overlappedtabs_custom_templates"
     insert-after="plonetheme_overlappedtabs_custom_images"/>
  <layer name="plonetheme_overlappedtabs_styles"
     insert-after="plonetheme_overlappedtabs_custom_templates"/>
 </skin-path>

</object>
