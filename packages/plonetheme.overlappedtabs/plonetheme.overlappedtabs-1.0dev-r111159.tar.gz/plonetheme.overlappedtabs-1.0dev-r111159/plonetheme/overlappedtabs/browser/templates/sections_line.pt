<tal:tabs tal:condition="view/portal_tabs"
          i18n:domain="plone">

    <div class="navlinewrapper"
         id="underlinesection0"
         tal:attributes="id string:${view/name}underlinesection${view/selected_section}">
        <div id="navline">
                <ul>
            <tal:tabs tal:repeat="tab view/portal_tabs"><li tal:attributes="
                                class python:'%s section%s' % (view.selected_portal_tab==tab['id'] and 'selected' or 'plain', repeat['tab'].number())"
                ><div>1</div></li></tal:tabs>
                </ul>
        </div>
    </div>
    <div style="clear:both"></div>

</tal:tabs>
    <div id="logout" tal:condition="view/show_logout">
        <a href="logout">logout</a>
    </div>
