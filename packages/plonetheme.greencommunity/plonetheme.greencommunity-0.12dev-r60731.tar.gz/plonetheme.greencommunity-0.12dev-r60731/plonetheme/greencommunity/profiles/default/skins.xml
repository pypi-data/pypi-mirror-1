<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Green Community">

 <object name="plonetheme_greencommunity_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.greencommunity:skins/plonetheme_greencommunity_custom_images"/>
 <object name="plonetheme_greencommunity_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.greencommunity:skins/plonetheme_greencommunity_custom_templates"/>
 <object name="plonetheme_greencommunity_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.greencommunity:skins/plonetheme_greencommunity_styles"/>

 <skin-path name="Green Community" based-on="Plone Default">
  <layer name="plonetheme_greencommunity_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_greencommunity_custom_templates"
     insert-after="plonetheme_greencommunity_custom_images"/>
  <layer name="plonetheme_greencommunity_styles"
     insert-after="plonetheme_greencommunity_custom_templates"/>
 </skin-path>

</object>
