<metal:block define-macro="page"><metal:block define-slot="doctype"><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"
></metal:block>
<html xmlns="http://www.w3.org/1999/xhtml">
<head metal:use-macro="views/ng_skin_macros/head">
  <metal:block fill-slot="other">
    <metal:block define-slot="headers" />
    <metal:block define-slot="style_slot" />
    <metal:block define-slot="ecmascript_slot" />
  </metal:block>
</head>
<body>
  <metal:block use-macro="views/ng_skin_macros/header"/>

  <div id="mainbody">

    <div id="column">
      <metal:block use-macro="views/ng_skin_macros/logobar"/>

      <div id="topmenu" class="mainmenuplace" >
        <img class="mainmenuplace" tal:attributes="src context/++resource++images/angleup.gif">
        <metal:block use-macro="views/ng_skin_macros/mainmenu"/>
      </div>        

      <metal:block use-macro="views/ng_skin_macros/menucolumn">
        <metal:block fill-slot="other"/>
      </metal:block>

      <div class="mainmenuplace">
        <img class="mainmenuplace" tal:attributes="src context/++resource++images/angledown.gif">      
        <metal:block use-macro="views/ng_skin_macros/referencemenu"/>
      </div>        
    </div>

    <metal:block define-slot="content">
      <metal:block use-macro="views/ng_skin_macros/content">
        <metal:block fill-slot="body">
          <metal:block define-slot="body"/>
        </metal:block> 
      </metal:block> 
    </metal:block> 
  </div>

  <metal:block use-macro="views/ng_skin_macros/footer"/>

  <metal:block use-macro="views/ng_skin_macros/smartbanner"/>
    
</body>
</html>
</metal:block>

