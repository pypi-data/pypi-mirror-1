<html metal:use-macro="views/standard_macros/page">
  <metal:block fill-slot="style_slot">
    <link
      href="main.css" rel="stylesheet" type="text/css"
        tal:attributes="href context/++resource++main.css" />
    <!--[if IE]><style type="text/css">
	div.ohloh {
	 display: None;
	 float: left;
	}
    </style><![endif]-->
  </metal:block>
  
	<body metal:fill-slot="body" border="2" tal:omit-tag="">
		<tal:block repeat="item python:path('context/++registry++maintablelinks:DreamBotSite').split()">
			<a  tal:define="ob python:path('context/%s' % item)"
					tal:content="ob/title"
					tal:attributes="href ob/@@absolute_url" /> 
		</tal:block>            
    <h1 class="abstract" tal:content="context/title"> Попытка не пытка </h1>
    <div class="ohloh"><script type="text/javascript"  src="http://www.ohloh.net/projects/9611/widgets/project_factoids"
            tal:attributes="src python: 'http://www.ohloh.net/projects/%s/widgets/project_factoids' % path('context/++registry++ohloh_zopelada:9611')"
    ></script></div>
    <div class="abstract" tal:content="structure context/++nsinterface++ng.app.converter.object2psadapter.interfaces.IPropertySheet/abstract"/>

    <table>
      <tr tal:repeat="item python:path('context/++registry++maintablerow:DreamBotSite').split()">
        <td> <div class="shortview"><tal:block tal:content="structure python:path('context/%s/@@short' % item)">
          item
          </tal:block> </div>
        </td>
      </tr>
      <tr><td>
        <tal:block repeat="item python:path('context/++registry++maintablelinks:DreamBotSite').split()">
          <a  tal:define="ob python:path('context/%s' % item)"
              tal:content="ob/title"
              tal:attributes="href ob/@@absolute_url" /> 
        </tal:block>            
      </td></tr>    
    </table>
    <div class="abstract" tal:content="structure context/++registry++contacts:"/>    
    <div class="abstract" tal:content="structure context/++registry++disclaimer:"/>
	</body>
</html>
