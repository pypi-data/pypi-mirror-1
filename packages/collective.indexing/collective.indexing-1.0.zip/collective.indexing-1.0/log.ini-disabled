[logger_root]
level=NOTSET
handlers=stderr

[handler_stderr]
class=StreamHandler
level=NOTSET
formatter=common
args=(sys.stderr,)

[formatter_common]
format=%(asctime)s %(levelname)s %(name)s %(message)s
datefmt=%Y-%m-%dT%H:%M:%S

[loggers]
keys=root

[handlers]
keys=stderr

[formatters]
keys=common
