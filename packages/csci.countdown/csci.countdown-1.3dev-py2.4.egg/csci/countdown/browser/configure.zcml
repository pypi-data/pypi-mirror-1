<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="csci.countdown">

  <include package="plone.app.contentmenu" />

  <!-- -*- extra stuff goes here -*- -->

  <browser:page
      for="*"
      name="dailytweet_view"
      class=".dailytweetview.dailytweetView"
      template="dailytweetview.pt"
      allowed_interface=".dailytweetview.IdailytweetView"
      permission="zope.Public"
      />


  <browser:page
      for="*"
      name="countdown_view"
      class=".countdownview.countdownView"
      template="countdownview.pt"
      allowed_interface=".countdownview.IcountdownView"
      permission="zope.Public"
      />


</configure>
