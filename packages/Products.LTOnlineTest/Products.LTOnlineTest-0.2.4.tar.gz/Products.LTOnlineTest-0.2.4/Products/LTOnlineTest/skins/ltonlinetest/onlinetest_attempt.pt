<html metal:use-macro="here/main_template/macros/master">
  <head>
    <title tal:content="template/title">The title</title>
  </head>
  <body>

<div metal:fill-slot="main">

  <div tal:define="subRecord python:here.getSubmissionRecord();
                   items python:here.getQuestionSet();">
  <div tal:condition="items">

  <form action="onlinetest_process" method="POST">
  
   <ol>
     <li  tal:repeat="item items">
       <b tal:content="item/Title" /><br /><br />
    
       <span tal:replace="structure item/getQuestion-text" />
       
       <blockquote>
         <div tal:omit-tag="" tal:repeat="choice python:item.getChoices()">
           <input type="radio" 
				  tal:attributes="name item/id;
				 			      value choice">
		   <span tal:replace="choice" />
		   <br />
         </div>
       </blockquote>
     </li>
   </ol>

  <input type="submit" name="submit" value=" Submit Test " />

  </form>

  </div>
  </div>

</div><!-- main slot -->

  </body>
</html>






























