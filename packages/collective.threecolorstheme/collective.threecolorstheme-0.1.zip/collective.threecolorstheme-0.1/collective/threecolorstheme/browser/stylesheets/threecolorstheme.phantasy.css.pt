#portal-logo {
    padding-top: ${logo/height}px;
    height: 0px !important;
    height /**/: ${logo/height}px;
    width: ${logo/width}px;
    background-image: url('${css_url}/${logoName}');
}           

" />


<tal:block content="structure string:

#visual-portal-wrapper {
    padding: ${css_properties/portalPadding};
    background-image: url('${css_url}/${css_properties/portalBackgroundImageName}');
}             
" />

<tal:block condition="css_properties/contentBackgroundColor"
           content="structure string:

#content {
     background-color:${css_properties/contentBackgroundColor};
}            
" />

<tal:block define="contentBackgroundImageName css_properties/contentBackgroundImageName"
           condition="python:getattr(context,contentBackgroundImageName, '')"
           content="structure string:           

#content {
     background-image:url(${css_url}/${css_properties/contentBackgroundImageName};
     background-repeat: no-repeat;
}            
" />   

<tal:comments replace="nothing">
 Specific threecolorstheme skin properties
 </tal:comments>



<tal:block content="structure string:

#visual-portal-wrapper 
  {
  border-color: ${css_properties/globalBorderColor};
  }
  
#portal-header
  {
  padding: 0 ${css_properties/columnTwoWidth} 0 ${css_properties/columnOneWidth};
  background-image: url(${css_url}/${css_properties/headerIllustrationName});
  }    

#portal-searchbox {
  margin-right: -${css_properties/columnTwoWidth};
}

#portal-searchbox #searchGadget {
    border: ${css_properties/borderWidth} solid ${css_properties/lightColor1};
}

#portal-searchbox .searchButton  {
  color:  ${css_properties/lightColor1};
}

.livesearchContainer {
  width: ${css_properties/columnTwoWidth};
}
  
#portal-personaltools li a,
#portal-siteactions li a {
    color: ${css_properties/discreetColor};
    text-transform: ${css_properties/textTransform};
}

#portal-siteactions {
     margin-right: -${css_properties/columnTwoWidth};
}

 li.selected a,
#portal-siteactions li a:hover,
#portal-personaltools li a:hover {
    color: ${css_properties/fontColor};
}
#portal-personaltools #user-name {
    background-image: url(${css_url}/user.gif);
}

#portal-breadcrumbs {
    border-bottom-color: ${css_properties/leadingColor};
    margin-left:  ${css_properties/columnOneWidth};    
    text-transform: ${css_properties/textTransform};
}

#portal-globalnav
  {
  background-color: ${css_properties/leadingColor};
  border-top-color: white;
  }

#portal-globalnav li 
  {
  border-right-color: ${css_properties/lightColor1};
  } 
  

#portal-globalnav li a
  {
  /* The normal, unselected tabs. They are all links */
  background-color: transparent;
  }

/*overload plone css */
#portal-globalnav li a:hover,
#portal-globalnav li.selected a {
  background-color: transparent;
}

  
#portal-globalnav li.selected a,
#portal-globalnav li:hover a,
#portal-globalnav li.sfhover a
  {
  color: black !important;
  }  
  
#portal-globalnav li.selected,
#portal-globalnav li:hover,
#portal-globalnav li.sfhover
  {
  background-color: ${css_properties/lightColor1};
  border-right-color: ${css_properties/leadingColor};
  }  
    

/* threecolorstheme global dropdown menus */
 

#portal-globalnav li ul {
    background-color: transparent;
}

#portal-globalnav li li {
    background-color: transparent;
}

#portal-globalnav li li a,
#portal-globalnav li li a:hover,
#portal-globalnav li.sfhover li a,
#portal-globalnav li:hover li a,
#portal-globalnav li.selected li a {
    background-color: ${css_properties/lightColor1};
    border-top-color: White !important;
    border-right-color: White !important;
    border-left-color: White !important;
    color: white !important;
}


#portal-globalnav li li a:hover {
    color: black !important;
}

#portal-globalnav li li a.lastItem {
    border-bottom-color: White !important;
}
  
.portletNavigationTree .navTreeItem  a.navTreeCurrentItem,
.portletNavigationTree  .navTreeItem a,
.portletNavigationTree  .navTreeItem a:hover
  {
  background-color: transparent !important;
  border:none !important;
  color: black;
  }   
  
.portletNavigationTree .navTreeItem a
  {
  background-image: url(${css_url}/@@collapsed-icon.gif) !important;
  } 
  
.portletNavigationTree  .navTreeItem a:hover
  {
  color: ${css_properties/leadingColor} !important;
  }    
  
.portletNavigationTree a.navTreeItemInPath,
.portletNavigationTree a.navTreeCurrentNode
  {
  background-image: url(${css_url}/@@expanded-icon.gif) !important;
  }         

#portal-column-two .portletHeader
  {
  font-family: ${css_properties/headingFontFamily} !important;
  }  
  
#portal-column-two .portletHeader a
  {
  color: ${css_properties/leadingColor} !important;
  border-bottom-color: ${css_properties/discreetColor};
  }  
 

#portal-column-two .portletItem
  {
  border-color: ${css_properties/leadingColor} !important;
  }

#portal-column-one .portletHeader
  {
  background-color:  ${css_properties/lightColor1};  
  }

#portal-column-one .portletHeader a
  {
  color: white;  
  }  

#portal-column-two .portletItem a,
#portal-column-two .portletItem a:visited,
#portal-column-two .portletItem a:hover
  {
  color: ${css_properties/leadingColor} !important;
  }    

#portal-column-two .lastItem
  {
  border-top-color: ${css_properties/leadingColor};
  background-color:${css_properties/evenRowBackgroundColor};
  }
  
#portal-column-two .portletFooter
  {
  border-color: ${css_properties/leadingColor};
  background-color:${css_properties/evenRowBackgroundColor};
  }  

  
.dayPopup {
    background-color: ${css_properties/contentViewBackgroundColor};
}

.ploneCalendar {
    border-color: ${css_properties/leadingColor};
}

.ploneCalendar th {
    background-color: ${css_properties/contentViewBackgroundColor};
}
.ploneCalendar .weekdays td {
    background-color: ${css_properties/contentViewBackgroundColor};
    border-color: ${css_properties/leadingColor};
}
.ploneCalendar .event {
    background-color: ${css_properties/lightColor1};
}
.ploneCalendar .todayevent {
    background-color: ${css_properties/lightColor1};
    border-color: black;
}
.ploneCalendar .todaynoevent {
    border-color: black;
}

.ploneCalendar .event a,
.ploneCalendar .todayevent a,
.ploneCalendar .todaynoevent a {
    color:white;
}  

#portal-column-two .portletBody  
  {
  border-color: ${css_properties/leadingColor}; 
  }
   
.contentViews .selected a,
.contentViews .selected a:hover {
background-color: ${css_properties/contentViewBorderColor};
border-bottom: ${css_properties/contentViewBorderColor} ${css_properties/borderWidth} ${css_properties/borderStyle};
color: #FFFFFF;
}  

.contentActions {
background-color: ${css_properties/contentViewBorderColor};
}

.contentActions li {
background-color: ${css_properties/contentViewBackgroundColor};
}

h3,
h3 a,
h4,
h4 a,
h5
h5 a {
    color : ${css_properties/discreetColor};
}

" />

