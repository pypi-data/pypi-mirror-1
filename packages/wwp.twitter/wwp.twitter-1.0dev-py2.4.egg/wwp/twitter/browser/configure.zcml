<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="wwp.twitter">

  <include package="plone.app.contentmenu" />

  <!-- -*- extra stuff goes here -*- -->

  <browser:page
      for="*"
      name="updatetweet_view"
      class=".updatetweetview.updatetweetView"
      template="updatetweetview.pt"
      allowed_interface=".updatetweetview.IupdatetweetView"
      permission="zope.Public"
      />


  <browser:page
      for="*"
      name="twittertrends_view"
      class=".twittertrendsview.twittertrendsView"
      template="twittertrendsview.pt"
      allowed_interface=".twittertrendsview.ItwittertrendsView"
      permission="zope.Public"
      />

    
    <browser:page
        for="*"
        name="addtweet"
        class=".addtweet.addtweet"
        permission="zope.Public" />


  <browser:page
      for="*"
      name="twitterpage_view"
      class=".twitterpageview.twitterpageView"
      template="twitterpageview.pt"
      allowed_interface=".twitterpageview.ItwitterpageView"
      permission="zope.Public"
      />


</configure>
