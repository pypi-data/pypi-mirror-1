<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="wwp.twitter">
<body>
    <div metal:fill-slot="main">
        <tal:main-macro metal:define-macro="main"
                        tal:define="testview view/test">

	<div tal:define="tweets python:here.objectValues();">
	
	<!-- Twitter API stuff -->
	
	<span tal:define="  tweet_title here/title;
						tweet_desc  here/description;
						tweets      view/twitter_posts;
						username    here/username">


		<h1 tal:content="tweet_title">Twitter Title</h1>
		<p tal:content="structure tweet_desc">Twitter Description</p>

		<table align="center" border="0" width="300" cellpadding="2" cellspacing="2">
			<tr><td><hr></td></tr>
			<span tal:repeat="tweet tweets">
			<tr>
			<td valign="top">
			  <span tal:content="structure tweet" />
			</td>
			</tr>
			<tr><td><hr></td></tr>
		</span>
		</table>
	</span>
	</div>

            
        </tal:main-macro>
    </div>
</body>
</html>
