<configure
   xmlns="http://namespaces.zope.org/zope"
   xmlns:zcml="http://namespaces.zope.org/zcml">

  <utility
     factory="ice.template.tests.sampledata.MailTemplatesPlugin"
     provides="z3c.sampledata.interfaces.ISampleDataPlugin"
     name="templates.mail"
     />
  <utility
     factory="ice.template.tests.sampledata.SkinTemplatesPlugin"
       provides="z3c.sampledata.interfaces.ISampleDataPlugin"
       name="templates.skin"
       />

  <SampleManager name="ice.template">
    <generator
       name="z3c.sampledata.site"
       />
    <generator
       name="templates.mail"
       contextFrom="z3c.sampledata.site"
       />
    <generator
       name="templates.skin"
       contextFrom="z3c.sampledata.site"
       />
  </SampleManager>

</configure>
