<!DOCTYPE struct PUBLIC "-//xooof.org//XML Specifications//struct" "../../struct/dtds/struct.dtd">
<?xml-stylesheet href="../../struct/mapping.html/xslt/struct2html.xslt" type="text/xsl"?>
<struct class="MClass">
  <descr>Class information</descr>

  <vfield name="name">
    <descr>Class name</descr>
    <tstring/>
  </vfield>

  <glfield name="descr" class="MLabel" minOccur="1">
    <descr>Short description</descr>
  </glfield>

  <glfield name="classmethods" class="MClassMethod">
    <descr>Class methods</descr>
  </glfield>

  <glfield name="instancemethods" class="MInstanceMethod">
    <descr>Instance methods</descr>
  </glfield>

  <glfield name="states" class="MState">
    <descr>States</descr>
  </glfield>

</struct>
