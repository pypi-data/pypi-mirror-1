<div id="portal-multi-logo" class="logoPanel">
    <div id="portal-multi-logo-items">
        <tal:logos repeat="item view/items">
            <div tal:omit-tag="view/inline">
                <span tal:attributes="class item/css_class">
                    <a href="" tal:omit-tag="not:item/logo_link"
                        tal:attributes="href item/logo_link;
                                        target item/target"
                        tal:content="structure item/logo_text">
                        Logo
                    </a>
                </span>
            </div>
            <tal:separator condition="not:repeat/item/end"><span tal:replace="item/separator"/></tal:separator>
        </tal:logos>
    </div>
    <div id="portal-multi-logo-promo" tal:content="view/promo_line" tal:condition="view/promo_line">
        Promo line
    </div>
        
</div>