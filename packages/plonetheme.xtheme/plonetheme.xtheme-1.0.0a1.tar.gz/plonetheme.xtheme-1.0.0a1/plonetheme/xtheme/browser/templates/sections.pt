<tal:tabs tal:condition="view/portal_tabs"
          i18n:domain="plone">
    <h5 class="hiddenStructure" i18n:translate="heading_sections">Sections</h5>
    <div id="round-globalnav">
    <div id="round-gntopleft"></div>
    <ul id="portal-globalnav" class="roundcont"> 
        <tal:tabs tal:repeat="tab view/portal_tabs">
        <li tal:attributes="id string:portaltab-${tab/id};
                            class python:view.selected_portal_tab==tab['id'] and 'selected' or 'plain'"
            tal:condition="python:repeat['tab'].number() != repeat['tab'].length()">
            <a href="" 
               tal:content="tab/name"
               tal:attributes="href tab/url;
                               title tab/description|nothing"
               class="other_global_nav">
            Tab Name
            </a></li>
        <li tal:attributes="id string:portaltab-${tab/id};
                            class python:view.selected_portal_tab==tab['id'] and 'selected' or 'plain'"
              tal:condition="python:repeat['tab'].number() == repeat['tab'].length()">
            <a href="" 
               tal:content="tab/name"
               tal:attributes="href tab/url;
                               title tab/description|nothing" 
                
               class="last_global_nav" >
            Tab Name
            </a></li></tal:tabs>	


    </ul>
    </div>
</tal:tabs>
