<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Bronze Cube Theme">

 <object name="plonetheme_bronzecube_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.bronzecube:skins/plonetheme_bronzecube_custom_images"/>
 <object name="plonetheme_bronzecube_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.bronzecube:skins/plonetheme_bronzecube_custom_templates"/>
 <object name="plonetheme_bronzecube_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.bronzecube:skins/plonetheme_bronzecube_styles"/>

 <skin-path name="Bronze Cube Theme" based-on="Plone Default">
  <layer name="plonetheme_bronzecube_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_bronzecube_custom_templates"
     insert-after="plonetheme_bronzecube_custom_images"/>
  <layer name="plonetheme_bronzecube_styles"
     insert-after="plonetheme_bronzecube_custom_templates"/>
 </skin-path>

</object>
