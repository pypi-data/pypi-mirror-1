# vim:syntax=python
from numscons import GetNumpyEnvironment

env = GetNumpyEnvironment(ARGUMENTS)

import SCons
def foo(*args, **kw):
    print "YOYO"

env['BUILDERS']['PythonExtension'].action = SCons.Action.Action([env['BUILDERS']['PythonExtension'].action, SCons.Action.Action(foo, "")])
env.DistutilsPythonExtension('spam', source = ['hellomodule.c'])
