<configure
    xmlns="http://namespaces.zope.org/zope"
	xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="icsemantic.langfallback">

	<browser:page
		name="multi_languages"
		for="*"
		class=".views.MultiLanguages"
		template="templates/base_view.pt"
		permission="zope2.View"
		/>

	<browser:view
		name="fixed_language"
		for="*"
		class=".views.FixedLanguage"
		permission="zope2.View"
		/>

    <include file="admin.zcml" />

</configure>
