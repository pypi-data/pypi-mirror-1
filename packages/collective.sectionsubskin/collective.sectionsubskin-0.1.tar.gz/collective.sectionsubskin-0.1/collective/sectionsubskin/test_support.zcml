<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
>

        <interface interface="collective.sectionsubskin.test_support.IRedSkin" />
        <interface interface="collective.sectionsubskin.test_support.IBlueSkin" />

        <utility
             name="redskin"
             provides="collective.sectionsubskin.test_support.IRedSkin"
             factory="collective.sectionsubskin.test_support.RedSkin"
         />
 
        <utility
             name="blueskin"
             provides="collective.sectionsubskin.test_support.IBlueSkin"
             factory="collective.sectionsubskin.test_support.BlueSkin"
         />
 
        <browser:page
            for="*"
            name="colours.css"
            class="collective.sectionsubskin.test_support.colours"
            permission="zope2.View"
        />

</configure>