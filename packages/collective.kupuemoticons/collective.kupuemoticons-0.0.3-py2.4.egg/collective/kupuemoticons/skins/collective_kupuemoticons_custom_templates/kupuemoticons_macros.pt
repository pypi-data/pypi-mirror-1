<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone">
<body>

  <metal:block define-macro="emoticontoolbar">
    <!-- EMOTICON TOOLBAR -->
    <span class="kupu-tb-buttongroup">
      <button type="button" class="kupu-emoticons" id="kupu-emoticondrawer-button" title="Insert Emoticon" i18n:attributes="title">&#160;</button>
    </span>
  </metal:block>

  <metal:block define-macro="emoticondrawer">
    <!-- EMOTICON DRAWER -->
    <div id="kupu-emoticondrawer" class="kupu-drawer kupu-emoticondrawer">
      <h1 class="kupu-drawer-title">Emoticons</h1>

      <div class="kupu-emoticondrawer-buttongroup">
        <button type="button" class="kupu-smile" id="kupu-smile-button" title="Smile" i18n:attributes="title;" onclick="drawertool.current_drawer.addSmileEmoticon()">&#160;</button>
        <button type="button" class="kupu-sad" id="kupu-sad-button" title="Sad" i18n:attributes="title;" onclick="drawertool.current_drawer.addSadEmoticon()">&#160;</button>
        <button type="button" class="kupu-angry" id="kupu-angry-button" title="Angry" i18n:attributes="title;" onclick="drawertool.current_drawer.addAngryEmoticon()">&#160;</button>
        <button type="button" class="kupu-bored" id="kupu-bored-button" title="Bored" i18n:attributes="title;" onclick="drawertool.current_drawer.addBoredEmoticon()">&#160;</button>
        <button type="button" class="kupu-cool" id="kupu-cool-button" title="Cool" i18n:attributes="title;" onclick="drawertool.current_drawer.addCoolEmoticon()">&#160;</button>
        <button type="button" class="kupu-curious" id="kupu-curious-button" title="Curious" i18n:attributes="title;" onclick="drawertool.current_drawer.addCuriousEmoticon()">&#160;</button>
      </div>
      <div class="kupu-emoticondrawer-buttongroup">
        <button type="button" class="kupu-dead" id="kupu-dead-button" title="Dead" i18n:attributes="title;" onclick="drawertool.current_drawer.addDeadEmoticon()">&#160;</button>
        <button type="button" class="kupu-doh" id="kupu-doh-button" title="Doh" i18n:attributes="title;" onclick="drawertool.current_drawer.addDohEmoticon()">&#160;</button>
        <button type="button" class="kupu-grin" id="kupu-grin-button" title="Grin" i18n:attributes="title;" onclick="drawertool.current_drawer.addGrinEmoticon()">&#160;</button>
        <button type="button" class="kupu-josh" id="kupu-josh-button" title="Josh" i18n:attributes="title;" onclick="drawertool.current_drawer.addJoshEmoticon()">&#160;</button>
        <button type="button" class="kupu-kiss" id="kupu-kiss-button" title="Kiss" i18n:attributes="title;" onclick="drawertool.current_drawer.addKissEmoticon()">&#160;</button>
        <button type="button" class="kupu-lol" id="kupu-lol-button" title="Lol" i18n:attributes="title;" onclick="drawertool.current_drawer.addLolEmoticon()">&#160;</button>
      </div>
      <div class="kupu-emoticondrawer-buttongroup">
        <button type="button" class="kupu-nod" id="kupu-nod-button" title="Nod" i18n:attributes="title;" onclick="drawertool.current_drawer.addNodEmoticon()">&#160;</button>
        <button type="button" class="kupu-nope" id="kupu-nope-button" title="Nope" i18n:attributes="title;" onclick="drawertool.current_drawer.addNopeEmoticon()">&#160;</button>
        <button type="button" class="kupu-pfui" id="kupu-pfui-button" title="Pfui" i18n:attributes="title;" onclick="drawertool.current_drawer.addPfuiEmoticon()">&#160;</button>
        <button type="button" class="kupu-wave" id="kupu-wave-button" title="Wave" i18n:attributes="title;" onclick="drawertool.current_drawer.addWaveEmoticon()">&#160;</button>
        <button type="button" class="kupu-weep" id="kupu-weep-button" title="Weep" i18n:attributes="title;" onclick="drawertool.current_drawer.addWeepEmoticon()">&#160;</button>
        <button type="button" class="kupu-wink" id="kupu-wink-button" title="Wink" i18n:attributes="title;" onclick="drawertool.current_drawer.addWinkEmoticon()">&#160;</button>
      </div>

      <div class="kupu-dialogbuttons">
        <button class="kupu-dialog-button" type="button" onclick="drawertool.closeDrawer()" i18n:translate="button_close">Close</button>
      </div>
    </div>
  </metal:block>

</body>
</html>

<!-- vim: set ft=html ts=2 sw=2 expandtab : -->
