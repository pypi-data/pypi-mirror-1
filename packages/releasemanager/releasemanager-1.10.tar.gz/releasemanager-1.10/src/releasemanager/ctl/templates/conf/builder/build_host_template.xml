<build_host
  name="template"
  klass="BuildHost"
  package="releasemanager.builder.buildhost"
  >
  <projects>
    <project name="template" />
  </projects>
  <work_dir>work</work_dir>
  <build_file>resource.zip</build_file>
  <logging_path>log</logging_path>
  <logging_level>DEBUG</logging_level>
  <listen_port>9999</listen_port>
  <release_managers>
    <host name="localhost" port="10000" />
  </release_managers>
  <!-- 
  Uncomment these lines to enable simpleauth
  <auth_plugin>simpleauth</auth_plugin>
  <svc_role>relman_svc</svc_role>
  -->
</build_host>