<release_installer
  name="template"
  klass="ReleaseInstaller"
  package="releasemanager.installer.releasedaemon"
  >
  <logging_path>log</logging_path>
  <logging_level>DEBUG</logging_level>
  <named_targets>
    <target name="named_target_template" environment="test" />
  </named_targets>
  <listen_port>9997</listen_port>
  <release_managers>
    <host name="localhost" port="10000" />
  </release_managers>
  <!-- 
  Uncomment these lines to enable simpleauth
  <auth_plugin>simpleauth</auth_plugin>
  <svc_role>relman_svc</svc_role>
  -->
</release_installer>