<build_behavior 
    name="template"
    klass="TemplateBuildBehavior"
    package="builder.template"
    >
  <svn_user></svn_user>
  <svn_pass></svn_pass>
  <svn_url>http://releasemanager.googlecode.com/svn/</svn_url>
  <svn_target>releasemanager</svn_target>
  <svn_work_dir>svn_checkout</svn_work_dir>
  <ignore_clean_error>True</ignore_clean_error>
  <archive_output>archive</archive_output>
  <output_targets>
    <filename target="template">releasemanager.zip</filename>
  </output_targets>
  <logging_path>log</logging_path>
  <logging_level>DEBUG</logging_level>
  <auth_role>relman_template_build</auth_role>
</build_behavior>
