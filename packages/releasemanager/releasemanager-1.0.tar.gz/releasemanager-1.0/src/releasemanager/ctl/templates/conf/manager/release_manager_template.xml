<release_manager
  name="template"
  klass="ReleaseManager"
  package="releasemanager.manager"
  >
  <registration name="release_installer" action="install">
    <param name="host" required="true" />
    <param name="port" required="true" />
  </registration>
  <registration name="build_host" action="build">
    <param name="host" required="true" />
    <param name="port" required="true" />
  </registration>
  <logging_path>log</logging_path>
  <logging_level>DEBUG</logging_level>
  <listen_port>10000</listen_port>
  <projects all="false" dir="">
    <project name="template" />
    <project name="oasis_webapp" />
    <project name="oasis_apache" />
    <project name="oasis_dropper" />
  </projects>
  <work_dir>work</work_dir>
  <db_type>mysql</db_type>
  <db_uri>mysql://user:password@localhost:3306/releasemanager</db_uri>
  <!-- 
  Uncomment these lines to enable simpleauth
  <auth_plugin>simpleauth</auth_plugin>
  <svc_role>relman_svc</svc_role>
  -->
</release_manager>
