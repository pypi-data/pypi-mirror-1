<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Portal 2.0">

 <object name="plonetheme_portaltwodotoh_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.portaltwodotoh:skins/plonetheme_portaltwodotoh_custom_images"/>
 <object name="plonetheme_portaltwodotoh_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.portaltwodotoh:skins/plonetheme_portaltwodotoh_custom_templates"/>
 <object name="plonetheme_portaltwodotoh_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.portaltwodotoh:skins/plonetheme_portaltwodotoh_styles"/>

 <skin-path name="Portal 2.0" based-on="Plone Default">
  <layer name="plonetheme_portaltwodotoh_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_portaltwodotoh_custom_templates"
     insert-after="plonetheme_portaltwodotoh_custom_images"/>
  <layer name="plonetheme_portaltwodotoh_styles"
     insert-after="plonetheme_portaltwodotoh_custom_templates"/>
 </skin-path>

</object>
