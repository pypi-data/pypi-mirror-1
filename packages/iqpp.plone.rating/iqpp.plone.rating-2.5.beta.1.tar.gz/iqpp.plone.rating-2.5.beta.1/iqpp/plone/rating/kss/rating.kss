#rate:click {    
    evt-click-preventdefault: True;
    action-server: kss-rate;
    kss-rate-portlethash: kssAttr(portlethash, True);
    kss-rate-form: currentForm();
}


.rate:click {
    evt-click-preventdefault: True;
    action-server: kss-starrate;
    kss-starrate-portlethash: kssAttr(portlethash, True);
    kss-starrate-rating_value: nodeContent();
}
