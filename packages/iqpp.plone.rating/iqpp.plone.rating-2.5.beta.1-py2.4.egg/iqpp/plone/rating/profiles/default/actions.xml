<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object" meta_type="CMF Action Category">
  <property name="title"></property>
  
  <object name="ratings" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Ratings</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/ratings</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:object.restrictedTraverse('@@ratingView').isRateable()</property>
   <property name="permissions">
    <element value="iqpp.plone.rating: manage"/>
   </property>
   <property name="visible">True</property>
  </object>

  <object name="rating-options" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Rating Options</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/rating-options</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:object.restrictedTraverse('@@ratingView').isRateable()</property>
   <property name="permissions">
    <element value="iqpp.plone.rating: manage"/>
   </property>
   <property name="visible">True</property>
  </object>
  
 </object>
</object>
