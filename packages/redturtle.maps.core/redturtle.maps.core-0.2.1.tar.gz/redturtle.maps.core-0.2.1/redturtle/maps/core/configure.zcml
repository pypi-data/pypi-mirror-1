    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="redturtle.maps.core">

  <five:registerPackage package="."/>

  <adapter factory=".adapter.CoreRichMarker" provides="Products.Maps.interfaces.IRichMarker"/>
