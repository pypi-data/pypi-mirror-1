<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<metal:head fill-slot="top_slot" tal:define="dummy python:request.set('disable_border',1)" />

  <metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main" define="view context/@@maps_googlemaps_view">

      <div class="googleMapView">
          <metal:marksers use-macro="here/maps_map/macros/markers" />
      </div>

    </tal:main-macro>
  </metal:main>

</html>