<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    i18n_domain="collective.marshall.workflow">

    <five:registerPackage package="." initialize=".initialize" />
    
    <!-- Simple Event Handler hooked on an ReviewState deserialization -->
    <subscriber
		for="*
    		 collective.marshall.workflow.interfaces.IObjectReviewStateDeserializedEvent"
		handler=".handlers.setWorkflowState"
    	/> 

</configure>
