<metal:block define-macro="page">
  <metal:block define-slot="doctype">
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
      "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
  </metal:block>
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title metal:define-slot="title" tal:content="context/++nsinterface++ng.adapter.title.interfaces.ITitle/title"> News Book </title>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <link
       href="style.css" rel="stylesheet" type="text/css"
 	   tal:attributes="href context/++resource++style.css" />   

    <metal:block define-slot="headers" />
    <metal:block define-slot="style_slot" />
    <metal:block define-slot="ecmascript_slot" /> 
    <link rel="icon" type="image/icon"
       tal:attributes="href context/++resource++images/favicon.ico" />	    
  </head>
  <body marginheight="0" marginwidth="0" topmargin="0" leftmargin="0" bgcolor="#FFFFFF">
  <table cellspacing="0" cellpadding="0" border="0" width="100%">
    <tr><td colspan=2 bgcolor="#ccddff">
      <metal:slot define-slot="breadcrumbs">
        <div id="breadcrumbs" metal:define-macro="breadcrumbs">
          <tal:block
             repeat="breadcrumb context/@@absolute_url/breadcrumbs"
            ><a href=""
                tal:condition="repeat/breadcrumb/start"
                tal:attributes="
                href string:${breadcrumb/url}/"
                i18n:translate="">Main</a
            ><a href=""
               tal:condition="not:repeat/breadcrumb/start"
               tal:content="string:${breadcrumb/name}"
               tal:attributes="
               href string:${breadcrumb/url}/">
              breadcrumb item</a
            ><tal:block tal:condition="not:repeat/breadcrumb/end">&nbsp;::&nbsp;</tal:block></tal:block>
        </div>
      </metal:slot> 
    </td></tr>
    <tr valign="top">
      <td bgcolor="#EEEEEE" width="218" valign="top">
        <a href="" tal:attributes="href context/@@root/root/@@absolute_url">
          <img src="" tal:attributes="src python:path('context/@@root/root/@@absolute_url') + '/++attribute++logo'" alt="" title="" height="197" width="201" border="0" />
        </a>          
        <metal:block tal:content="structure provider:column">
          <table cellspacing="2" cellpadding="0" border="0" width="100%">
             <tr><td bgcolor="#ccddff">
             <p class="wm" style="text-align:center;font-weight:bold;"
             ><a href="http://www.neural.ru/">Главная</a></p></td></tr>
           </table>
           <p class="wm" style="font-size:11px;font-weight:bold;"
           >&nbsp;&nbsp;&nbsp;<a class="drk" href="http://www.neural.ru/News" style=""
           >Новости</a></p>
        </metal:block> 
      </td><td rowspan="2" valign="top">
        <table cellspacing="0" cellpadding="0" width="100%"><tr><td align="right" bgcolor="#EEEEEE">
            <img src="" tal:attributes="src context/++resource++images/img_header.gif" alt="" title="" width="500" border="0" align="left" 
            /><metal:block tal:content="structure provider:header">
                ViewLets
              </metal:block 
        ></td></tr></table> 
        <table cellspacing="0" cellpadding="0" border="0" width="100%" height="100%">
          <tr><td height="100%">
            <table cellspacing="0" cellpadding="3" bgcolor="#CCDDFF" width="100%" border="1" bordercolor="black" bordercolordark="#CCDDFF">
              <tr><td><tal:block on-error="python:''" define="np context/@@nextprev">
                <p class="docttl" style="text-align:right;"
                  ><tal:block tal:define="p np/prev" tal:condition="python:p is not None"
                  ><< <a href="" 
                      tal:attributes="href p/@@absolute_url"
                      tal:content="p/++nsinterface++ng.adapter.title.interfaces.ITitle/title"
                      >Python Policy</a
                  >&nbsp;|&nbsp;</tal:block
                  >^<a href="" tal:attributes="href context/@@nextprev/up/@@absolute_url" tal:content="context/@@nextprev/up/++nsinterface++ng.adapter.title.interfaces.ITitle/title"
                  >Продукты</a
                  >^<tal:block tal:define="n np/next" tal:condition="python: n is not None"
                  >&nbsp;|&nbsp;<a href="" 
                      tal:attributes="href n/@@absolute_url"
                      tal:content="n/++nsinterface++ng.adapter.title.interfaces.ITitle/title"
                      >Python Policy</a
                  > >> </tal:block>
              </tal:block></td></tr>
            </table>     
            
            <metal:block define-slot="content">

            <table cellspacing="0" cellpadding="3" bgcolor="#CCDDFF" height="100%" width="100%" border="1" 
              bordercolor="black" bordercolordark="#CCDDFF">
              <tr><td height="100%">
                 <metal:block define-slot="body">
                 </metal:block>               
              </td></tr>
            </table>
                                              
            </metal:block>
                                              
          </td></tr>
        </table>
    
      </td>
    </tr>
    
    <tr><td width="218" align="center" valign="bottom" bgcolor="#eeeeee">
      <metal:block tal:content="structure provider:admin"/>
    </td></tr>
    
    <tr><td colspan="2" background="" tal:attributes="background context/++resource++images/img_lbluedot.gif"><img src="" tal:attributes="src context/++resource++images/img_transp.gif" alt="" title="" height="20" width="1" border="0"></td></tr>

  </table>    

  <!-- -->
    
    <br/>
    <div align="center"  tal:content="structure context/@@smartbannerview"/>
    
    
  </body>
</html>
</metal:block>
