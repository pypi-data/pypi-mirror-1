<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="enable_expires" meta_type="CMF Action" i18n:domain="redomino.autodelete">
   <property name="title" i18n:translate="">Enable autodelete</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@enable_expires</property>
   <property name="icon_expr"></property>
   <property name="available_expr">object/@@enable_expires_cond</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="disable_expires" meta_type="CMF Action" i18n:domain="redomino.autodelete">
   <property name="title" i18n:translate="">Disable autodelete</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@disable_expires</property>
   <property name="icon_expr"></property>
   <property name="available_expr">object/@@disable_expires_cond</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="object" meta_type="CMF Action Category">
  <object name="expires" meta_type="CMF Action" i18n:domain="redomino.autodelete">
   <property name="title" i18n:translate="">Autodelete settings</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object_url}/@@expires_form</property>
   <property name="icon_expr"></property>
   <property name="available_expr">object/@@disable_expires_cond</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>

</object>
