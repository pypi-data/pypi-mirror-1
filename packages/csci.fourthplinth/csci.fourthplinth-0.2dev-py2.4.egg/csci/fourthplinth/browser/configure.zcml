<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="csci.fourthplinth">

  <include package="plone.app.contentmenu" />

  <!-- -*- extra stuff goes here -*- -->

  <browser:page
      for="*"
      name="udschedule_view"
      class=".udscheduleview.UDscheduleView"
      template="udscheduleview.pt"
      allowed_interface=".udscheduleview.IUDscheduleView"
      permission="zope.Public"
      />


  <browser:page
      for="*"
      name="fpschedule_view"
      class=".fpscheduleview.FPscheduleView"
      template="fpscheduleview.pt"
      allowed_interface=".fpscheduleview.IFPscheduleView"
      permission="zope.Public"
      />


</configure>
