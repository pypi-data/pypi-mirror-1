    <addform
        label="Add Dictionary Item"
        name="AddDictionaryItem.html"
        schema="ng.skin.base.page.wiki.interfaces.IDictionaryItem"
        content_factory="ng.skin.base.page.wiki.dictionaryitem.DictionaryItem"
        class="..add.nexturl.NextUrl"
	template="dictionaryitem.pt"
        permission="zope.ManageContent"
        set_before_add="title"
        layer="...interfaces.BaseSkin"
        />

    <zope:adapter
        provides=".interfaces.IDictionaryItem"
        for = "ng.content.annotation.dictannotation.interfaces.IDictAnnotationAble"
        factory = ".dictionaryitemadapter.DictionaryItemAdapter"
        />

    <page
        name="undefined.html"
        permission="zope.View"
        template="undefined.pt"
        class=".undefined.UnDefined"
        for="*"
        layer="...interfaces.BaseSkin"
        />

</configure>
