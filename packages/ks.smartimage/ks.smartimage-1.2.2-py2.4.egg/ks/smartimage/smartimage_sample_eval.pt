<html>
	<body>
		<ul>
				<li tal:repeat="item context/sample/@@smartimagecontainer">
					<tal:block tal:define="img python:path('context/sample/@@smartimagecontainer')[item]">
						<p tal:content="img/width"/>
						<p tal:content="img/height"/>
						<p tal:content="img/title"/>
						<p tal:content="img/contentType"/>
						<p tal:content="img/uniqid"/>
						<p tal:content="img/@@shorttag"/>
						<p tal:content="structure img/@@shorttag"/> 
						<p tal:content="img/@@tag"/>
						<p tal:content="structure img/@@tag"/> 
					</tal:block>
				</li>
		</ul>
	</body>
</html>
