<configure
    xmlns="http://namespaces.zope.org/zope"
    i18n_domain="WatermarkImage">

  <permission
    id="watermarkimage.ViewOriginal"
    title="WatermarkImage: View original"
    />

  <subscriber
    for=".interfaces.IWatermarkImage
         zope.lifecycleevent.interfaces.IObjectModifiedEvent"
    handler=".eventhandlers.handleImageModifiedEvent"
    />

  <include package=".browser"/>

</configure>

