<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

  <browser:page
      for="..interfaces.IWatermarkImage"
      name="thumbnail.jpg"
      class=".image.ThumbnailView"
      permission="zope2.View"
      />

  <browser:page
      for="..interfaces.IWatermarkImage"
      name="original"
      class=".image.OriginalView"
      permission="watermarkimage.ViewOriginal"
      />

</configure>

