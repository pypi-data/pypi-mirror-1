# Month view: format for the "April 2005" label in large letters at the top 
# example: to put the year first (2005 April), use: "${year} ${monthname}"
msgid "calheader_fmt_date_month"
msgstr "${monthname} ${year}"

# Day view: format for the "Monday April 23, 2005" label at the top 
# example: for "23. April 2005" use: "${date}.${monthname} ${year}"
msgid "calheader_fmt_date_day"
msgstr "${date}. ${monthname} ${year} (${dayname})"

# Week view: format for the "April 23 2005 - April 30 2005" label at the top 
# example: "23.April 2005 - 30.April 2005" use: "${date1}.${mon1} ${year1} - ${date2}.${mon2} ${year2}"
msgid "calheader_fmt_date_week"
msgstr "${date1}. ${mon1} ${year1} - ${date2}. ${mon2} ${year2}"

