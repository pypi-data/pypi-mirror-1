<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="andreas02">

 <object name="plonetheme_andreas02_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.andreas02:skins/plonetheme_andreas02_custom_images"/>
 <object name="plonetheme_andreas02_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.andreas02:skins/plonetheme_andreas02_custom_templates"/>
 <object name="plonetheme_andreas02_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.andreas02:skins/plonetheme_andreas02_styles"/>

 <skin-path name="andreas02" based-on="Plone Default">
  <layer name="plonetheme_andreas02_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_andreas02_custom_templates"
     insert-after="plonetheme_andreas02_custom_images"/>
  <layer name="plonetheme_andreas02_styles"
     insert-after="plonetheme_andreas02_custom_templates"/>
 </skin-path>

</object>
