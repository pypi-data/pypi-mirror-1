#!/bin/sh
# $URL: svn+ssh://svn.mems-exchange.org/repos/trunk/dulcinea/bin/codecheck $
# $Id: codecheck 23144 2003-11-11 21:31:05Z dbinger $
# Parse files in this directory and below and report unknown names.
find . \( -name .svn  -o -name build -o -name dist \) -prune -o \( -name "*.py" -o -name "*.ptl" \) -exec unknown.py \{\} \;

