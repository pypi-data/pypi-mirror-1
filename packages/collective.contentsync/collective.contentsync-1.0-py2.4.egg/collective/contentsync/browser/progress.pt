<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
      lang="en-US"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="upfrontcontacts">

<body>

<div metal:fill-slot="main">

  <h1 i18n:translate="performing_synchronization">Performing Synchronization</h1>

  <div id="collective-contentsync-label" i18n:translate="in-progress-dot-dot">In progress...</div>
  <div id="collective-contentsync-bar" 
       style="float: left; width: 1px; height: 17px; background-color: red;"></div>
  <div id="collective-contentsync-text"></div>
  <div style="clear: both;"></div>
  <div id="collective-contentsync-log"></div>
  <div id="collective-contentsync-complete" 
       style="display: none;"
       i18n:translate="done">Done</div>

  <iframe tal:attributes="src string:${portal_url}/++resource++collective.contentsync.progress-iframe"
          style="width: 100%; height: 100px; border:0;"></iframe>

</div>
</body>
</html>
