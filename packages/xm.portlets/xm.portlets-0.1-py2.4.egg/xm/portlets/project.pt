<dl class="portlet portletProjectPortlet"
    i18n:domain="xm.portlets">
    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <span i18n:translate="project_links">Project links</span>
        <span class="portletTopRight"></span>
    </dt>

    <dd tal:condition="view/links"
        tal:repeat="link view/links"
        tal:attributes="class link/class">
        <a tal:attributes="href link/url"
           tal:content="link/title">Title</a>
    </dd>
    <dd class="portletFooter">
        <span class="portletBotomLeft"></span>
        <span class="portletBottomRight"></span>
    </dd>
</dl>
