##
## 'BGP FSM State Change', 'BGB Backward Transition' on same 'neighbor_ip' 
##
fsm_close
    use close($e1, $e2)
    when
        seq_object_classes_vars($e1, $e2, ('BGP FSM State Change','BGP Backward Transition'), ('neighbor_ip',))
