<html metal:use-macro="views/standard_macros/view">
<body metal:fill-slot="body">
<h1>Cache Statistics</H1>
<dl>
<dt>Size:</dt><dd tal:content="context/__len__">0</dd>
<tal:block tal:condition="options/count | nothing"><dt>Regenerated:</dt><dd tal:content="options/count | nothing">0</dd></tal:block>
<tal:block tal:condition="options/count | nothing"><dt>Elapsed:</dt><dd tal:content="options/elapsed | nothing">0</dd></tal:block>
</dl>

<form action="" tal:attributes="action string: ${context/@@absolute_url}/">
  <input type="submit" name="regenerate_all:method" value="Regenerate All"/>
  <input type="submit" name="clean:method" value="Clean"/>
</form>

</body>
</html>