<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Nautica05 Theme">

 <object name="plonetheme_nautica05_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.nautica05:skins/plonetheme_nautica05_custom_images"/>
 <object name="plonetheme_nautica05_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.nautica05:skins/plonetheme_nautica05_custom_templates"/>
 <object name="plonetheme_nautica05_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.nautica05:skins/plonetheme_nautica05_styles"/>

 <skin-path name="Nautica05 Theme" based-on="Plone Default">
  <layer name="plonetheme_nautica05_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_nautica05_custom_templates"
     insert-after="plonetheme_nautica05_custom_images"/>
  <layer name="plonetheme_nautica05_styles"
     insert-after="plonetheme_nautica05_custom_templates"/>
 </skin-path>

</object>
