<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="csci.postfeeds">
<body>
    <div metal:fill-slot="main">
        <tal:main-macro metal:define-macro="main"
                        tal:define="feedstat view/lookfornew;   
                                    p_title  here/title_or_id;
                                    p_desc   here/description;
                                    ">

            <h1 tal:content="p_title"> </h1>
            <p><b tal:content="p_desc" /> </p>
            <p>Feed Status: </p>
            <span tal:repeat="rss feedstat">
                <p>
                    <span tal:content="structure rss"></span>
                </p>
            </span>
            
        </tal:main-macro>
    </div>
</body>
</html>
