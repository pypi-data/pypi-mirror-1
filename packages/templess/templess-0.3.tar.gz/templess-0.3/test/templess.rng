<?xml version="1.0" ?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         xmlns:t="http://johnnydebris.net/xmlns/templess">

  <include href="xhtml.rng" />

  <define name="t.attrib">
    <optional>
      <attribute name="t:content" />
    </optional>
    <optional>
      <attribute name="t:replace" />
    </optional>
    <optional>
      <attribute name="t:attr" />
    </optional>
    <optional>
      <attribute name="t:cond" />
    </optional>
    <optional>
      <attribute name="t:not" />
    </optional>
  </define>

  <define name="Common.attrib" combine="interleave">
    <ref name="t.attrib" />
  </define>

</grammar>
