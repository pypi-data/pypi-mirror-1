#!/usr/bin/env paster

[filter-app:main]
use = egg:Paste#evalerror
next = main_app

[app:main_app]
use = egg:${project}
debug = true
# -*- extra config: -*-

[app:test]
use = main_app
# -*- extra automated test config: -*-

[server:main]
use = egg:Paste#http
port = 8080
# Change to 0.0.0.0 to make public:
host = 127.0.0.1

[exe]
command = serve
reload = true
verbose = true
