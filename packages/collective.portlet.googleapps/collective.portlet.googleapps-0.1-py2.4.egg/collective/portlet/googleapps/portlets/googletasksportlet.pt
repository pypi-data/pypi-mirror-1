<dl class="portlet portletGoogleAppsPortlet portletGoogleTasksPortlet"
    i18n:domain="collective.portlet.googleapps.portlets">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <a class="link-plain" target="_blank" tal:attributes="href view/google_url;">
            <span tal:content="view/data/header" />
        </a>
        <span class="portletTopRight"></span>
    </dt>

    <dd class="portletItem odd">
      <iframe frameborder="0" scrolling="auto" class="google_tasks_portlet_iframe"
              marginwidth="0" marginheight="0"
	      tal:attributes="src view/google_tasks_iframe_url; 
			      height view/data/portlet_height;
			      width view/data/portlet_width;">
      
      </iframe>
    </dd>

</dl>
