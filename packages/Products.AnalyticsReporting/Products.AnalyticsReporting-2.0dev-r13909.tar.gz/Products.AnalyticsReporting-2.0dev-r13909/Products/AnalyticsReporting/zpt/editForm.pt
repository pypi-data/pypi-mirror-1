<h1 tal:replace="structure here/manage_page_header|nothing">Header</h1>
<h2 tal:define="manage_tabs_message request/manage_tabs_message | nothing"
  tal:replace="structure here/manage_tabs">
  Tabs
</h2>
   
<br/>
<form name="form" action="" method="POST">

<b>Title:</b><br/>
<input type="text" name="title" tal:attributes="value context/REQUEST/title | context/title"><br/><br/>

<b>Google Analytics login (email address):</b><br/>
<input type="text" name="login" tal:attributes="value context/REQUEST/login | context/title"><br/><br/>

<b>Google Analytics password:</b><br/>
<input type="text" name="password" tal:attributes="value context/REQUEST/password | context/title"><br/><br/>

<b>Google Analytics site ID:</b><br/>
<input type="text" name="site_id" tal:attributes="value context/REQUEST/site_id | context/title"><br/><br/>

<input type="submit" value=" Add ">
</form>
    
<tal:footer tal:replace="structure here/manage_page_footer|nothing">footer</tal:footer>
