<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="wwp.translate">

  <include package="plone.app.contentmenu" />

  <!-- -*- extra stuff goes here -*- -->

  <browser:page
      for="*"
      name="translatefolder_view"
      class=".translatefolderview.translatefolderView"
      template="translatefolderview.pt"
      allowed_interface=".translatefolderview.ItranslatefolderView"
      permission="zope.Public"
      />


  <browser:page
      for="*"
      name="wwp_translate_view"
      class=".wwp_translateview.wwp_translateView"
      template="wwp_translateview.pt"
      allowed_interface=".wwp_translateview.Iwwp_translateView"
      permission="zope.Public"
      />


</configure>
