<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="wwp.translate">
<body>
     <div metal:fill-slot="main" >
     <div class="contentHeader">
         <h1 tal:content="here/title_or_id">Title or id</h1>
     </div>
	 
     <div class="description"
          tal:content="structure here/Description"> description 
     </div>
     <br>


<div tal:define="subitems python:here.objectValues();">
     
	 <table border="0" width="100%" cellpadding="2" cellspacing="2">
	 <span tal:repeat="item subitems">
		  <tr tal:condition="exists:item/is_published">
		  <span tal:condition="item/is_published">
			   <td width=25%></td><td>
						 Translate from:    &nbsp;&nbsp;&nbsp; <a tal:attributes="href item/absolute_url"><span tal:content="item/getFrom_lang_long" /> into <span tal:content="item/getTo_lang_long" /> 
					</a>		
			   </td>
		  </span>
		  </tr>
	 </span>
	 </table>

 </div>

 </div>
</body>
</html>
