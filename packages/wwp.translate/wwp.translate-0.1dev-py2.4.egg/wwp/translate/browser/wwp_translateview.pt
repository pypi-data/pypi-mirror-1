<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="wwp.translate">
<body>
    <div metal:fill-slot="main">
        <tal:main-macro metal:define-macro="main"
                        tal:define="title here/Title;
                                    desc  here/description;
                                    from  here/getFrom_lang_long;
                                    to    here/getTo_lang_long;
                                    goog_out  view/google_form">
            <h1 tal:content="title">Translator</h1>
            <p tal:content="desc"></p>

            <span tal:content="structure goog_out">to</span>
            
            
              <!-- end matter for template -->
        <div metal:use-macro="here/document_actions/macros/document_actions">
              Document actions (print, sendto etc)
        </div>    
  
        <tal:has_document_byline tal:condition="exists:here/document_byline">
              <div metal:use-macro="here/document_byline/macros/byline">
                Get the byline - contains details about author and modification date.
              </div>
        </tal:has_document_byline>  
   
  
        </tal:main-macro>
    </div>
</body>
</html>
