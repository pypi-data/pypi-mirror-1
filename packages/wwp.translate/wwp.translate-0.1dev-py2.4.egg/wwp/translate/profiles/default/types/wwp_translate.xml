<?xml version="1.0"?>
<object name="wwp_translate"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="wwp.translate" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">wwp_translate</property>
  <property name="description"
    i18n:translate="">Translation of text to and from different languages</property>
  <property name="content_meta_type">wwp_translate</property>
  <property name="content_icon">document_icon.gif</property>
  <property name="product">wwp.translate</property>
  <property name="factory">addwwp_translate</property>
  <property name="immediate_view">atct_edit</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">False</property>
  <property name="allow_discussion">False</property>
  <property name="default_view">@@wwp_translate_view</property>
  <property name="view_methods">
    <element value="@@wwp_translate_view" />
  </property>
  <alias from="(Default)" to="@@wwp_translate_view" />
  <alias from="edit" to="atct_edit" />
  <alias from="sharing" to="@@sharing" />
  <alias from="view" to="@@wwp_translate_view" />
  <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/" visible="True">
    <permission value="View" />
  </action>
  <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
    <permission value="Modify portal content" />
  </action>
</object>
