# Default log rotation settings
rotate 4
weekly
create
compress

${output_dir}/var/log/zeo.log {
    postrotate
        ${output_dir}/bin/zeo logreopen
    endscript
}

${output_dir}/var/log/instance1.log {
    postrotate
        /bin/kill -USR2 \$(cat ${output_dir}/var/instance1.pid)
    endscript
}
