<?xml version="1.0"?>

<!DOCTYPE ruleset SYSTEM "../dtd.d/daxfifw_0.5.dtd">

<!-- Block IGMP packets. -->

<append>
  <rule
	direction="in"
	source-ip="0.0.0.0"
	destination-ip="%LOCAL_IP">
    <protocol protocol="igmp" />
    <drop />
    <limit rate="1/s" burst="10" />
    <log />
  </rule>
</append>

