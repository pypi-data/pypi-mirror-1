<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
        default_skin="Web Couturier City Theme" request_varname="plone_skin">

 <object name="webcouturier_city_custom_images"
    meta_type="Filesystem Directory View"
    directory="webcouturier.city.theme:skins/webcouturier_city_custom_images"/>
 <object name="webcouturier_city_styles"
    meta_type="Filesystem Directory View"
    directory="webcouturier.city.theme:skins/webcouturier_city_styles"/>

 <skin-path name="Web Couturier City Theme" based-on="Plone Default">
  <layer name="webcouturier_city_styles"
     insert-after="custom"/>
  <layer name="webcouturier_city_custom_images"
     insert-after="webcouturier_city_styles"/>
 </skin-path>
 
</object>