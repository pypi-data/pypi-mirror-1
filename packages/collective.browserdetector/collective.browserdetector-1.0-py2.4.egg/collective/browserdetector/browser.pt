<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="here/main_template/macros/master"
    i18n:domain="collective.browserdetector">

    <body>
        <metal:main fill-slot="main">
            <tal:main-macro metal:define-macro="main">
            
                <div tal:replace="structure provider:plone.abovecontenttitle" />

                <h1 class="documentFirstHeading" i18n:translate="heading_browser_detection_results">
                    Browser Detection Results
                </h1>

                <div tal:replace="structure provider:plone.belowcontenttitle" />

                <p class="documentDescription" i18n:translate="help_browser_detection_page">
                    This is a browser detection results page.
                </p>

                <div tal:replace="structure provider:plone.abovecontentbody" />
                
                <dl class="portalMessage warning">
                    <dt i18n:translate="label_protocol">Protocol</dt>
                    <dd tal:content="view/protocol">value</dd>
                </dl>
                
                <dl class="portalMessage warning">
                    <dt i18n:translate="label_platform">Platform</dt>
                    <dd tal:content="view/platform">value</dd>
                </dl>
                
                <dl class="portalMessage warning">
                    <dt i18n:translate="label_browser">Browser</dt>
                    <dd>
                        <tal:block content="view/browser">value</tal:block>
                        <tal:block condition="view/isWebKit"> (<tal:block i18n:translate="label_webkit">WebKit</tal:block>)</tal:block>
                    </dd>
                </dl>
                
                <dl id="browser-detection-details" class="collapsible inline collapsedOnLoad">
                    <dt class="collapsibleHeader" i18n:translate="label_detection_details">Detection details</dt>
                    <dd class="collapsibleContent">
                        <table class="vertical listing" width="100%">
                            <tr>
                                <th colspan="2" i18n:translate="label_protocol">Protocol</th>
                                <th colspan="6" i18n:translate="label_platforms">Platforms</th>
                            </tr>
                            
                            <tr>
                                <th i18n:translate="label_secure">Secure</th>
                                <td tal:content="structure python:view.isSecure and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_mac">Mac</th>
                                <td tal:content="structure python:view.isMac and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_linux">Linux</th>
                                <td tal:content="structure python:view.isLinux and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_windows">Windows</th>
                                <td tal:content="structure python:view.isWindows and view.true_icon or view.false_icon">icon</td>
                            </tr>
                            
                            <tr>
                                <td colspan="8">&nbsp;</td>
                            </tr>
                            
                            <tr>
                                <th colspan="8" i18n:translate="label_browsers">Browsers</th>
                            </tr>
                            
                            <tr>
                                <th colspan="2" i18n:translate="label_safari_browsers">Safari browsers</th>
                                <th colspan="2" i18n:translate="label_mozilla_firefox_browsers">Mozilla/Firefox browsers</th>
                                <th colspan="2" i18n:translate="label_internet_explorer_browsers">Internet Explorer browsers</th>
                                <th colspan="2" i18n:translate="label_other_browsers">Other browsers</th>
                            </tr>
                            
                            <tr>
                                <th i18n:translate="label_safari">Safari</th>
                                <td tal:content="structure python:view.isSafari and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_gecko">Gecko</th>
                                <td tal:content="structure python:view.isGecko and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_ie">IE</th>
                                <td tal:content="structure python:view.isIE and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_chrome">Chrome</th>
                                <td tal:content="structure python:view.isChrome and view.true_icon or view.false_icon">icon</td>
                                
                            </tr>
                            
                            <tr>
                                <th i18n:translate="label_safari3">Safari 3</th>
                                <td tal:content="structure python:view.isSafari3 and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_gecko2">Gecko 2</th>
                                <td tal:content="structure python:view.isGecko2 and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_ie6">IE 6</th>
                                <td tal:content="structure python:view.isIE6 and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_opera">Opera</th>
                                <td tal:content="structure python:view.isOpera and view.true_icon or view.false_icon">icon</td>
                            </tr>
                            
                            <tr>
                                <th i18n:translate="label_safari4">Safari 4</th>
                                <td tal:content="structure python:view.isSafari4 and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_gecko3">Gecko 3</th>
                                <td tal:content="structure python:view.isGecko3 and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_ie7">IE 7</th>
                                <td tal:content="structure python:view.isIE7 and view.true_icon or view.false_icon">icon</td>
                                
                                <th i18n:translate="label_webkit">WebKit</th>
                                <td tal:content="structure python:view.isWebKit and view.true_icon or view.false_icon">icon</td>
                                
                            </tr>
                            
                            <tr>
                                <th>&nbsp;</th>
                                <td>&nbsp;</td>
                                
                                <th>&nbsp;</th>
                                <td>&nbsp;</td>
                                
                                <th i18n:translate="label_ie8">IE 8</th>
                                <td tal:content="structure python:view.isIE8 and view.true_icon or view.false_icon">icon</td>
                                
                                <th>&nbsp;</th>
                                <td>&nbsp;</td>
                            </tr>
                            
                            <tr>
                                <td colspan="8">&nbsp;</td>
                            </tr>
                            
                            <tr>
                                <th colspan="8" i18n:translate="label_user_agent">User Agent</th>
                            </tr>
                            
                            <tr>
                                <th i18n:translate="label_user_agent">User Agent</th>
                                <td colspan="7" tal:content="view/agent">ua</td>
                            </tr>

                        </table>
                    </dd>
                </dl>
                
                <!--
                <div tal:replace="structure provider:plone.belowcontentbody" />
                -->
            </tal:main-macro>
        </metal:main>
    </body>
</html>

