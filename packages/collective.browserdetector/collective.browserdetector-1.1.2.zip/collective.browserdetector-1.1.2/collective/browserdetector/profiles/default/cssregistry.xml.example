<?xml version="1.0"?>

<!-- This is just en example of stylesheet registration which you can
    use in your policy package profile to register only those stylesheets
    which you need to activate.
    
    See documentation for available detection conditions.
-->

<!-- CSS Hacks registration activated only for Internet Explorer 6.x -->
<object name="portal_css">
    <stylesheet title=""
        id="IE6.css" media="screen" rel="stylesheet" rendering="import"
        cacheable="True" compression="safe" cookable="True" enabled="1"
        expression="python:portal.restrictedTraverse('@@browser-detector').isIE6"/>
</object>
