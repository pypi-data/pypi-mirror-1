<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="digestocontenttypes_images"
         meta_type="Filesystem Directory View"
         directory="DigestoContentTypes/skins/digestocontenttypes_images"/>
 <object name="digestocontenttypes_styles"
         meta_type="Filesystem Directory View"
         directory="DigestoContentTypes/skins/digestocontenttypes_styles"/>
 <object name="digestocontenttypes_templates"
         meta_type="Filesystem Directory View"
         directory="DigestoContentTypes/skins/digestocontenttypes_templates"/>
 
 <skin-path name="*">
  <layer insert-after="custom" 
         name="digestocontenttypes_images"/>
  <layer insert-after="custom" 
         name="digestocontenttypes_styles"/>
  <layer insert-after="custom" 
         name="digestocontenttypes_templates"/>
 </skin-path>

</object>