<html tal:define="files view/get_files">
    <body>
        <tal:repeat tal:repeat="d files">
            <a tal:attributes="href string:${context/@@absolute_url}/download?package=${d/package}&filename=${d/name}"
               tal:content="d/name"
            />
            <br/>
        </tal:repeat>
    </body>
</html>
