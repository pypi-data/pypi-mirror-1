<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Web Couturier Hosting Theme">

 <object name="webcouturier_hosting_custom_images"
    meta_type="Filesystem Directory View"
    directory="webcouturier.hosting.theme:skins/webcouturier_hosting_custom_images"/>
 <object name="webcouturier_hosting_styles"
    meta_type="Filesystem Directory View"
    directory="webcouturier.hosting.theme:skins/webcouturier_hosting_styles"/>

 <skin-path name="Web Couturier Hosting Theme" based-on="Plone Default">
  <layer name="webcouturier_hosting_custom_images"
     insert-after="custom"/>
  <layer name="webcouturier_hosting_styles"
     insert-after="webcouturier_hosting_custom_images"/>
 </skin-path>

</object>
