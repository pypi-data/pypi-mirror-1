#portal-breadcrumbs {
	float: left; /* openplans add float style */
	background-color: &dtml-breadcrumb_personalbarBGcolor;;
	border: 0;
	white-space: nowrap;
	}

#portal-personaltools {
    /* Bar with personalized menu (user preferences, favorites etc) */
    background-color: &dtml-breadcrumb_personalbarBGcolor;;
    /* openplans omit  
	border-top-color: &dtml-globalBorderColor;;
    border-top-style: &dtml-borderStyle;;
    border-top-width: &dtml-borderWidth;;
    border-bottom-color: &dtml-globalBorderColor;;
    border-bottom-style: &dtml-borderStyle;;
    border-bottom-width: &dtml-borderWidth;;
	*/
    line-height: 1.6em;
    color: &dtml-fontColor;;
    padding: 0em 3em 0em 3em;
    margin: 0;
    text-align: right;
    text-transform: &dtml-textTransform;;
    list-style: none;
}


#portal-column-one {
    vertical-align: top;
    width: 175px;
	/*background-color: &dtml-columnOneBGColor;;*/
}


body {
    font: &dtml-fontBaseSize; <dtml-var fontFamily>;
    background: url(&dtml-portal_url;/bg.gif) repeat;
    background-color: &dtml-backgroundColor;;
    color: &dtml-fontColor;;
    margin-left: 0;
    margin-right: 0;
    /*top-margin: 0;
    left-margin: 0;*/
    margin: 0;
    padding: 0;
}


/*changed border bottom
    border-bottom: 1px dashed #999999; */
