<tal:block repeat="viewlet options/viewlets">
<div class="naviBoxContainer" id="the_viewlet_id"
       tal:attributes="id viewlet/__name__">
  <div class="naviBox">
    <div class="naviBoxHeader">
      <tal:block content="viewlet/title">Title</tal:block>
    </div>
    <div class="naviBoxBody">
      <tal:block replace="structure viewlet/render" />
    </div>
  </div>
  <div class="boxSpacer">
    &nbsp;
  </div>
</div>
</tal:block>
