<?xml version="1.0"?>
<!DOCTYPE catalog PUBLIC "-//OASIS//DTD Entity Resolution XML Catalog V1.0//EN" "http://www.oasis-open.org/committees/entity/release/1.0/catalog.dtd">
<!--
  This is the xml catalog for xooof entities.
  It maps public identifiers to local resources, using
  relative uris (relative to this catalog file).
  -->
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog" prefer="public">
  <!--extended struct with additional attribute for zope schema
  see "-//xooof.org//XML Specifications//struct
  -->
  <public publicId="-//xooof.org//XML Specifications//zope//struct" uri="struct.dtd"/>
  <!-- rewrite canonical uri's for xooof ressources -->
  <rewriteSystem systemIdStartString="http://zope.xooof.org/" rewritePrefix="./"/>
</catalog>

