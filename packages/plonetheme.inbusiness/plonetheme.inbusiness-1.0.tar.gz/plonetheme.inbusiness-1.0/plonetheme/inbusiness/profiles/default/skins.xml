<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="In Business Theme">

 <object name="plonetheme_inbusiness_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.inbusiness:skins/plonetheme_inbusiness_custom_images"/>
 <object name="plonetheme_inbusiness_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.inbusiness:skins/plonetheme_inbusiness_custom_templates"/>
 <object name="plonetheme_inbusiness_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.inbusiness:skins/plonetheme_inbusiness_styles"/>

 <skin-path name="In Business Theme" based-on="Plone Default">
  <layer name="plonetheme_inbusiness_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_inbusiness_custom_templates"
     insert-after="plonetheme_inbusiness_custom_images"/>
  <layer name="plonetheme_inbusiness_styles"
     insert-after="plonetheme_inbusiness_custom_templates"/>
 </skin-path>

</object>
