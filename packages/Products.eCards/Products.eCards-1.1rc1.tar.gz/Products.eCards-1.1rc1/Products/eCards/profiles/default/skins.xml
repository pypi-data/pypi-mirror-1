<?xml version="1.0"?>
<object name="portal_skins">
 <object name="ecards_images" meta_type="Filesystem Directory View"
    directory="eCards/skins/ecards_images"/>
 <object name="ecards_templates" meta_type="Filesystem Directory View"
    directory="eCards/skins/ecards_templates"/>
 <object name="ecards_styles" meta_type="Filesystem Directory View"
    directory="eCards/skins/ecards_styles"/>
 <skin-path name="*">
  <layer name="ecards_images" insert-after="custom" />
 </skin-path>
 <skin-path name="*">
  <layer name="ecards_templates" insert-after="custom" />
 </skin-path>
 <skin-path name="*">
  <layer name="ecards_styles" insert-after="custom" />
 </skin-path>
</object>
