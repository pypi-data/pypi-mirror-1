<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">
    
    <!-- Register images -->
    
    <browser:resource
        name="plonetabs.gif"
        image="./images/plonetabs.gif"
        />
    
    <browser:resource
        name="busy_icon.gif"
        image="./images/busy_icon.gif"
        />
    
    <browser:resource
        name="trash.gif"
        image="./images/trash.gif"
        />
    
    <browser:resource
        name="treeExpanded.gif"
        image="./images/treeExpanded.gif"
        />
    
    <browser:resource
        name="treeCollapsed.gif"
        image="./images/treeCollapsed.gif"
        />
    
    <browser:resource
        name="drag.gif"
        image="./images/drag.gif"
        />
    
    <!-- Register stylesheets -->
    
    <browser:resource
        name="plonetabs.css"
        file="./resources/plonetabs.css"
        />
    
    <!-- Register kukit stylesheets -->
    
    <browser:resource
        name="plonetabs.kss"
        file="./resources/plonetabs.kss"
        />
    
    <!-- Register scriptaculous dragdrop js library -->
    
    <browser:resource
        name="sa_dragdrop.js"
        file="./resources/dragdrop.js"
        />

    <browser:resource
        name="pt_effects.js"
        file="./resources/effects.js"
        />
    
</configure>
