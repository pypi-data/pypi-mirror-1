<?xml version="1.0"?>
<object>
    <hidden manager="plone.htmlhead.links" skinname="Bangalore Theme">
        <viewlet name="plone.links.favicon" />
    </hidden>
  <hidden manager="plone.portalfooter" skinname="Bangalore Theme">
    <viewlet name="plone.colophon" />
  </hidden>
  <hidden manager="plone.portalheader" skinname="Bangalore Theme" purge="True">
    <viewlet name="plone.site_actions" />
    <viewlet name="plone.searchbox" />
  </hidden>
  <hidden manager="plone.portaltop" skinname="Bangalore Theme" purge="True">
    <viewlet name="plone.app.i18n.locales.languageselector" />
    <viewlet name="plone.path_bar" />
    <viewlet name="plonetheme.Bangalore.bgl_path_bar"/>
  </hidden>

  <order manager="plone.portaltop" skinname="Bangalore Theme">
    <viewlet name="plone.personal_bar" insert-before="*" />
    <viewlet name="plone.header" insert-after="plone.personal_bar" />
  </order>
  <order manager="plone.portalheader" skinname="Bangalore Theme" >
    <viewlet name="plone.logo" insert-before="*" />
    <viewlet name="plone.global_sections" insert-after="plone.logo" />
    <viewlet name="plonetheme.Bangalore.bgl_path_bar" insert-after="plone.global_sections" />
  </order>
  <order manager="plone.htmlhead.links" skinname="Bangalore Theme" based-on="Plone Default">
        <viewlet name="plonetheme.Bangalore.favicon" insert-before="*" />
    </order>



</object>
