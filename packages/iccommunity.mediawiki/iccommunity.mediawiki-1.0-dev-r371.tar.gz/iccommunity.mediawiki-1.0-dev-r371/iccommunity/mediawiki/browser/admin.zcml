<configure xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n_domain='icCommunity'>

    <include package="iccommunity.core" />

    <!-- Mediawiki roles mapper -->
    <browser:page
        for="iccommunity.core.interfaces.IicCommunitySite"
        name="manage-mediawiki-roles-map"
        class=".admin.MediawikiRolesMapper"
        permission="cmf.ManagePortal"
        menu="icCommunity_settings"
        title="Mediawiki Roles Mapper"
        />

    <!-- Mediawiki SQL Server -->
    <browser:page
        for="iccommunity.core.interfaces.IicCommunitySite"
        name="manage-mediawiki-sql-server"
        class=".admin.MediawikiSQLServer"
        permission="cmf.ManagePortal"
        menu="icCommunity_settings"
        title="Mediawiki SQL Server"
        />

</configure>
