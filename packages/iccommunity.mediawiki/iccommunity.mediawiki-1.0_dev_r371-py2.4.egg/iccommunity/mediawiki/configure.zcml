<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:gs="http://namespaces.zope.org/genericsetup"
    i18n_domain="iccommunity.mediawiki">

    <five:registerPackage package="." initialize=".initialize" />

    <gs:registerProfile
        name="default"
        title="iccommunity.mediawiki"
        directory="profiles/default"
        description="Extension profile for iccommunity.mediawiki"
        provides="Products.GenericSetup.interfaces.EXTENSION"
        for="Products.CMFPlone.interfaces.IPloneSiteRoot"
        />

    <adapter
        for="iccommunity.core.interfaces.IicCommunitySite"
        provides=".interfaces.IicCommunityManagementMediawikiRolesMapper"
        factory=".preferences.mediawiki_roles_form_adapter"
        />

    <adapter
        for="iccommunity.core.interfaces.IicCommunitySite"
        provides=".interfaces.IicCommunityManagementMediawikiSQLServer"
        factory=".preferences.mediawiki_sql_server_adapter"
        />

    <include package=".browser" />

</configure>
