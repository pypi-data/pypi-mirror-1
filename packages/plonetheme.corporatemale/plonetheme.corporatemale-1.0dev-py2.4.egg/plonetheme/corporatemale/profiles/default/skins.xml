<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Corporate Male">

 <object name="plonetheme_corporatemale_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.corporatemale:skins/plonetheme_corporatemale_custom_images"/>
 <object name="plonetheme_corporatemale_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.corporatemale:skins/plonetheme_corporatemale_custom_templates"/>
 <object name="plonetheme_corporatemale_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.corporatemale:skins/plonetheme_corporatemale_styles"/>

 <skin-path name="Corporate Male" based-on="Plone Default">
  <layer name="plonetheme_corporatemale_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_corporatemale_custom_templates"
     insert-after="plonetheme_corporatemale_custom_images"/>
  <layer name="plonetheme_corporatemale_styles"
     insert-after="plonetheme_corporatemale_custom_templates"/>
 </skin-path>

</object>
