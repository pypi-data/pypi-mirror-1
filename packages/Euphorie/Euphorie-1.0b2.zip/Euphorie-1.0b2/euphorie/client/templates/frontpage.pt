   <script type="text/javascript">
// <![CDATA[
    function beforeTitleSwap(title, index) {
        var total = $("#home > a").size();
        var newindex = (index+1)%total;
        var newtitle = $("#home > a:eq("+newindex+")");
        index = index%total;

        document.title=newtitle.text();

        $("#home > a:eq("+index+")").fadeOut(200, function() {
            newtitle.fadeIn(200);
        });
    }

    $(document).ready(function() {
      document.title=$("#home > a:first").text();
      $("#home > a:not(:first)").hide();
      $("h1")
          .wrapInner("<div/>")
          .jCarouselLite({auto: 1000,
                          speed: 800,
                          circular: true,
                          visible: 1,
                          listSelector: "div",
                          itemSelector: "span",
                          easing: "easeOutElastic",
                          beforeStart: beforeTitleSwap
                          });
    });
// ]]>
