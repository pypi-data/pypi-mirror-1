[debinstall]
group=devel
origin=Logilab S.A.

[create]
destination=/var/www/debian
configurations=/var/lib/debinstall
default_distribution=unstable

[upload]
check_signature=yes
checkers=lintian

[publish]
sign_repo=no
keyid=FFFFFF
check_signature=yes
checkers=lintian

[archive]
archivedir=/var/www/debian/archive
