<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
      lang="en-US"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="Poi">

  <head>
  </head>
  
  
  <body>
    <div metal:fill-slot="main">
      <div metal:define-macro="body">
        <h2 i18n:translate="poi_heading_migration">Migration</h2>
        
        <p i18n:translate="poi_help_migration">
          A list of all migrations available for Poi.
        </p>

        <p i18n:translate="poi_help_migration_warning">
          This may change your data.  Make sure you have a backup for
          safety.
        </p>

        <form
            method="post"
            action="">
          <input
              tal:repeat="id view/available_migrations"
              class="standalone"
              type="submit"
              value="migration_id"
              name="migration_id"
              tal:attributes="value id"
              />
        </form>
      </div>
    </div>
  </body>
</html>
