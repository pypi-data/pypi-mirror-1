#!/usr/bin/env python2.5
# -*- python -*-

import sys

def usage(prog, modname=None):
    print >>sys.stderr, "usage: %s modulename [arguments]" % (prog, )
    if modname:
        print >>sys.stderr, "%s is not a know module." % modname
    raise SystemExit(1)

if __name__ == '__main__':
    import sys
    if len(sys.argv) == 1:
        usage(sys.argv[0])
    modname = sys.argv[1]
    try:
        res = __import__("pulp.%s" % modname, {}, {}, ["main", ])
        main = getattr(res, "main")
    except (ImportError, AttributeError):
        usage(sys.argv[0], modname)
    del sys.argv[1]
    main()
    
