<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="enable_workgroup_mode" meta_type="CMF Action" i18n:domain="redomino.workgroup">
   <property name="title" i18n:translate="">Enable workgroup mode</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@enable-workgroup-mode</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:portal.portal_workgrouptool.enable_wg_condition(object)</property>
   <property name="permissions">
    <element value="redomino.workgroup: manage Workgroups"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="disable_workgroup_mode" meta_type="CMF Action" i18n:domain="redomino.workgroup">
   <property name="title" i18n:translate="">Disable workgroup mode</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@disable-workgroup-mode</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:portal.portal_workgrouptool.disable_wg_condition(object)</property>
   <property name="permissions">
    <element value="redomino.workgroup: manage Workgroups"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="object" meta_type="CMF Action Category">
  <object name="workgroup" meta_type="CMF Action" i18n:domain="redomino.workgroup">
   <property name="title" i18n:translate="">Workgroup</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object_url}/prefs_wgusers_overview</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:object.portal_workgrouptool.providesIWorkgroup(object)</property>
   <property name="permissions">
    <element value="redomino.workgroup: manage Workgroups"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
