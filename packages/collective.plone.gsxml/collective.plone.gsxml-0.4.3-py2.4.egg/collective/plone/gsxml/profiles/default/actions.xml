<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <object name="object_buttons" meta_type="CMF Action Category">
  <property name="title"></property>

  <!--Export-->
  <object name="xmlexport" meta_type="CMF Action" i18n:domain="collective.plone.gsxml">
   <property name="title" i18n:translate="">Export</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@export</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>

  <!--Import-->
  <object name="xmlimport" meta_type="CMF Action" i18n:domain="collective.plone.gsxml">
   <property name="title" i18n:translate="">Import</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@import</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>

</object>

