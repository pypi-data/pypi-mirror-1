<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="subscribe" meta_type="CMF Action" i18n:domain="iw.subscriber">
   <property name="title" i18n:translate="">Subscribe</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@subscribe.html</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:object.restrictedTraverse('show_subscribed_action')()</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="unsubscribe" meta_type="CMF Action" i18n:domain="iw.subscriber">
   <property name="title" i18n:translate="">Unsubscribe</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/@@unsubscribe.html</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
