<div id="portal-siteactions-top"></div>


    <span id="portal-mini-logo" style="width:52px;height:52px;">
    <img src="++resource++plonetheme.pyar.images/solpiente.png" width="52" height="52" border="0" alt="" />
    </span>


    <ul id="portal-siteactions"
    tal:define="accesskeys python: {'sitemap' : '3', 'accessibility' : '0', 'contact' : '9'};"
    tal:condition="view/site_actions"
    i18n:domain="plone">

    <li tal:repeat="saction view/site_actions"
        tal:attributes="id string:siteaction-${saction/id}"><a
           href=""
           tal:define="title saction/title;
                       id saction/id;
                       accesskey python: accesskeys.get(id, '');"
           i18n:attributes="title"
           i18n:translate=""
           tal:content="title"
           tal:attributes="href saction/url;
                           title title;
                           accesskey accesskey;"
           >Site action</a>
    </li>
</ul>
