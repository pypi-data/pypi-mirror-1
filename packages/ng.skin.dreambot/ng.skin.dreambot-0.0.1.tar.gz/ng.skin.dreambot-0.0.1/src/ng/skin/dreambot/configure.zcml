<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">
  <!--
    Author: Andrey Orlov
    $Id: configure.zcml 49886 2008-02-02 15:09:05Z cray $ 
  -->

	<interface
    interface=".interfaces.DreamBotSkin"
    type="zope.publisher.interfaces.browser.IBrowserSkinType"
    name="dreambot"
    />

  <browser:resourceDirectory
    directory="images"
    name="images"
    layer=".interfaces.DreamBotSkin"
  />

  <browser:resource
    name="style.css"
    file="style.css"
    layer=".interfaces.DreamBotSkin"
    />

</configure>
