          <object class="GtkComboBox" id="combobox-section">
            <property name="width_request">150</property>
            <property name="height_request">27</property>
            <property name="visible">True</property>
            <property name="tooltip_text" translatable="yes">The section we're managing</property>
            <property name="model">liststore-section</property>
            <property name="active">0</property>
            <signal name="changed" handler="section_changed"/>
            <child>
              <object class="GtkCellRendererText" id="cellrenderertext-sections"/>
              <attributes>
                <attribute name="text">0</attribute>
              </attributes>
            </child>
          </object>
          <packing>
            <property name="x">150</property>
            <property name="y">62</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="delete-button">
            <property name="label" translatable="yes">delete</property>
            <property name="width_request">50</property>
            <property name="height_request">25</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="tooltip_text" translatable="yes">Delete this section</property>
