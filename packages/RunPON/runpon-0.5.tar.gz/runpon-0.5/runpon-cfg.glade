<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkAdjustment" id="adjustment-check_interval">
    <property name="value">20</property>
    <property name="upper">86400</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
    <property name="page_size">10</property>
  </object>
  <object class="GtkAdjustment" id="adjustment-check_grace_time">
    <property name="value">10</property>
    <property name="upper">86400</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
    <property name="page_size">10</property>
  </object>
  <object class="GtkListStore" id="liststore-section">
    <columns>
      <!-- column-name session -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">DEFAULT</col>
      </row>
    </data>
  </object>
  <object class="GtkListStore" id="liststore-active-section">
    <columns>
      <!-- column-name gchararray-active-section -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkAdjustment" id="adjustment-cumulative_time_slot">
    <property name="value">1</property>
    <property name="lower">1</property>
    <property name="upper">86400</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
    <property name="page_size">10</property>
  </object>
  <object class="GtkWindow" id="window-runpon-cfg">
    <property name="title" translatable="yes">RunPON Configuration</property>
    <property name="default_width">440</property>
    <property name="default_height">550</property>
    <signal name="destroy" handler="save_close"/>
