<?xml version="1.0" encoding="iso-8859-1"?>
<D:multistatus xmlns:D="DAV:" xmlns:template="http://www.boddie.org.uk/ns/xmltools/template"
  template:element="root">
  <D:response>
    <D:href template:value="$path"></D:href>
    <D:propstat>
      <D:prop>
        <D:creationdate>20050101</D:creationdate>
        <D:displayname>/</D:displayname>
        <D:resourcetype>
          <D:collection/>
        </D:resourcetype>
      </D:prop>
      <D:status>HTTP/1.1 200 OK</D:status>
    </D:propstat>
  </D:response>
  <D:response template:element="directory">
    <D:href template:value="concat($path, '/', @name)"></D:href>
    <D:propstat>
      <D:prop>
        <D:creationdate>20050101</D:creationdate>
        <D:displayname template:value="@name"></D:displayname>
        <D:resourcetype>
          <D:collection/>
        </D:resourcetype>
      </D:prop>
      <D:status>HTTP/1.1 200 OK</D:status>
    </D:propstat>
  </D:response>
  <D:response template:element="file">
    <D:href template:value="concat($path, '/', @name)"></D:href>
    <D:propstat>
      <D:prop>
        <D:creationdate>20050101</D:creationdate>
        <D:displayname template:value="@name"></D:displayname>
        <D:resourcetype>
        </D:resourcetype>
      </D:prop>
      <D:getcontenttype>text/calendar</D:getcontenttype>
      <D:getcontentlength>0</D:getcontentlength>
      <D:status>HTTP/1.1 200 OK</D:status>
    </D:propstat>
  </D:response>
</D:multistatus>
