    <property name="border_width">5</property>
    <property name="icon">presenter.png</property>
    <property name="type_hint">normal</property>
    <property name="has_separator">False</property>
    <property name="program_name">Presenter</property>
    <property name="version">0.1</property>
    <property name="copyright" translatable="yes">Copyright &#xA9; 2009 Peter Ward</property>
    <property name="comments" translatable="yes">The presentation tool for control freaks.</property>
    <property name="website">http://flowblok.selfip.net:8001/trac/presenter</property>
    <property name="license" translatable="yes">This program is free software: you can redistribute it and/or modify
