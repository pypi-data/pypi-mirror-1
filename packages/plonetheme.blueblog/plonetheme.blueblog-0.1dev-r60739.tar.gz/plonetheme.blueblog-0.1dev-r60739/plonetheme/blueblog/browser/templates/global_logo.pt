<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone">

<body>

	<!-- THE PORTAL LOG -->
<!--<a metal:define-macro="portal_logo"
   id="portal-logo"
   accesskey="1"
   tal:attributes="href here/navigation_root_url">-->
   <div id="portal-logo-wrapper">
   
   <a tal:attributes="href context/@@plone_portal_state/navigation_root_url">
   <span id="portal-logo" accesskey="1"
      tal:define="portal_title here/portal_properties/title;"
      tal:content="string:$portal_title"
      >
   </span> 
   </a>
   
   <div id="subtitle" tal:define="portal_des here/portal_properties/description;"
      tal:content="string:$portal_des"
>
        
   </div>
</div>



</body>
</html>
