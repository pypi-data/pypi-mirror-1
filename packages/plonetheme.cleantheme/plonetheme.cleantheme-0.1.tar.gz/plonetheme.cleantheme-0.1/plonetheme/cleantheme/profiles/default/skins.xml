<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Clean theme 4 Plone">

 <object name="plonetheme_cleantheme_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.cleantheme:skins/plonetheme_cleantheme_custom_images"/>
 <object name="plonetheme_cleantheme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.cleantheme:skins/plonetheme_cleantheme_custom_templates"/>
 <object name="plonetheme_cleantheme_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.cleantheme:skins/plonetheme_cleantheme_styles"/>

 <skin-path name="Clean theme 4 Plone" based-on="Plone Default">
  <layer name="plonetheme_cleantheme_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_cleantheme_custom_templates"
     insert-after="plonetheme_cleantheme_custom_images"/>
  <layer name="plonetheme_cleantheme_styles"
     insert-after="plonetheme_cleantheme_custom_templates"/>
 </skin-path>

</object>
