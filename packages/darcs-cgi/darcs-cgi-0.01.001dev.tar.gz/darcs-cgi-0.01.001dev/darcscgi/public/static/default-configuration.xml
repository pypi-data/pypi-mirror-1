<settings xmlns="darcs-server-config">
    <global>
        <darcs>darcs</darcs>
        <darcs-options></darcs-options>
    </global>
    <defaults>
        <allow-read>yes</allow-read>
        <allow-write>no</allow-write>
        <verify-read>no</verify-read>
        <verify-write>global.keyring</verify-write>
        <quarantine-max-patches>15</quarantine-max-patches>
        <quarantine-max-size>25</quarantine-max-size>
        <patch-max-size>5</patch-max-size>
    </defaults>
</settings>
