<?xml version="1.0"?>
<object name="portal_analytics" meta_type="Google Analytics Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="site-visits-month-line" meta_type="Google Analytics Report"
    i18n:domain="analytics">
  <property name="title"
     i18n:translate="">Site Visits This Month: Line Chart</property>
  <property name="description"
     i18n:translate="">Displays the number of site visits for the last 30 days as a line graph.</property>
  <property name="metrics">
   <element value="ga:visits"/>
  </property>
  <property name="dimensions">
   <element value="ga:day"/>
   <element value="ga:month"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="ga:month"/>
  </property>
  <property name="start_date">python:today - timedelta(days=30)</property>
  <property name="end_date">today</property>
  <property name="max_results">1000</property>
  <property name="is_page_specific">False</property>
  <property name="column_labels">
   <element value="string:Day"/>
   <element value="string:Visits"/>
  </property>
  <property name="column_exps">
   <element value="python:str(ga_day)"/>
   <element value="python:int(ga_visits)"/>
  </property>
  <property name="viz_type">LineChart</property>
  <property name="viz_options">
   <element value="title string:Site Visits this Month"/>
   <element value="height python:250"/>
   <element value="legend string:none"/>
   <element value="titleX string:Day"/>
   <element value="titleY string:Visits"/>
   <element value="axisFontSize python:10"/>
  </property>
  <property name="introduction"></property>
  <property
     name="conclusion">&lt;p&gt;&lt;strong tal:content="python:sum(data_columns[1])"&gt;1000&lt;/strong&gt; visits this month&lt;/p&gt;
   &lt;p&gt;&lt;strong tal:content="python:int(sum(data_columns[1])/len(data_columns[1]))"&gt;1000&lt;/strong&gt; 
   average visits per day&lt;/p&gt;</property>
 </object>
 <object name="site-visits-month-sparkline"
    meta_type="Google Analytics Report" i18n:domain="analytics">
