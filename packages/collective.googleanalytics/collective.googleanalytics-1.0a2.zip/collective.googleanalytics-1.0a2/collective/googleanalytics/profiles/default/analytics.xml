<?xml version="1.0"?>
<object name="portal_analytics" meta_type="Google Analytics Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="site-visits-line" meta_type="Google Analytics Report"
    i18n:domain="analytics">
  <property name="title" i18n:translate="">Site Visits: Line Chart</property>
  <property name="description"
     i18n:translate="">Displays the number of site visits as a line graph.</property>
  <property name="is_page_specific">False</property>
  <property name="categories">
   <element value="Site Wide"/>
  </property>
  <property name="metrics">
   <element value="ga:visits"/>
  </property>
  <property name="dimensions">
   <element value="date_range_dimension"/>
   <element value="date_range_sort_dimension"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="date_range_sort_dimension"/>
   <element value="date_range_dimension"/>
  </property>
  <property name="max_results">1000</property>
  <property name="column_labels">
   <element value="python:date_range_unit"/>
   <element value="string:Visits"/>
  </property>
  <property name="column_exps">
   <element value="python:str(date_range_dimension)"/>
   <element value="python:int(ga_visits)"/>
  </property>
  <property name="introduction"></property>
  <property
     name="conclusion">&lt;p tal:condition="data_length"&gt;&lt;strong tal:content="python:sum(data_columns[1])"&gt;1000&lt;/strong&gt; visits in the last &lt;span tal:replace="string:${data_length} ${date_range_unit_plural/lower}"&gt;&lt;/span&gt;&lt;/p&gt;
   &lt;p tal:condition="data_length"&gt;&lt;strong tal:content="python:int(sum(data_columns[1])/len(data_columns[1]))"&gt;1000&lt;/strong&gt; 
   average visits per &lt;span tal:replace="string:${date_range_unit/lower}"&gt;&lt;/span&gt;&lt;/p&gt;</property>
  <property name="viz_type">LineChart</property>
  <property name="viz_options">
   <element value="title string:Site Visits"/>
   <element value="height python:250"/>
   <element value="legend string:none"/>
   <element value="titleX python:date_range_unit"/>
   <element value="titleY string:Visits"/>
   <element value="axisFontSize python:10"/>
  </property>
 </object>
 <object name="site-visits-sparkline" meta_type="Google Analytics Report"
    i18n:domain="analytics">
  <property name="title" i18n:translate="">Site Visits: Sparkline</property>
  <property name="description"
     i18n:translate="">Displays the number of site visits as a sparkline.</property>
  <property name="is_page_specific">False</property>
  <property name="categories">
   <element value="Portlet"/>
  </property>
  <property name="metrics">
   <element value="ga:visits"/>
  </property>
  <property name="dimensions">
   <element value="date_range_dimension"/>
   <element value="date_range_sort_dimension"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="date_range_sort_dimension"/>
   <element value="date_range_dimension"/>
  </property>
  <property name="max_results">1000</property>
  <property name="column_labels">
   <element value="string:Visits"/>
  </property>
  <property name="column_exps">
   <element value="python:int(ga_visits)"/>
  </property>
  <property name="introduction">&lt;h3&gt;Site Visits&lt;/h3&gt;</property>
  <property
     name="conclusion">&lt;p tal:condition="data_length"&gt;&lt;strong tal:content="python:sum(data_columns[0])"&gt;1000&lt;/strong&gt; total visits during the last &lt;span tal:content="python:str(len(data_rows)) + ' ' + date_range_unit_plural.lower()"&gt;&lt;/span&gt; &lt;/p&gt;</property>
  <property name="viz_type">ImageSparkLine</property>
  <property name="viz_options">
   <element value="height python:40"/>
   <element value="showAxisLines python:False"/>
   <element value="showValueLabels python:False"/>
  </property>
 </object>
 <object name="top-5-pageviews-table" meta_type="Google Analytics Report"
    i18n:domain="analytics">
  <property name="title" i18n:translate="">Top 5 Pages: Table</property>
  <property name="description"
     i18n:translate="">Displays a table of the top pages by number of page views for this month.</property>
  <property name="is_page_specific">False</property>
  <property name="categories">
   <element value="Site Wide"/>
  </property>
  <property name="metrics">
   <element value="ga:pageviews"/>
  </property>
  <property name="dimensions">
   <element value="ga:pagePath"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="-ga:pageviews"/>
  </property>
  <property name="max_results">5</property>
  <property name="column_labels">
   <element value="string:URL"/>
   <element value="string:Views"/>
  </property>
  <property name="column_exps">
   <element value="ga_pagePath"/>
   <element value="ga_pageviews"/>
  </property>
  <property name="introduction">&lt;h3&gt;Top 5 Pages&lt;/h3&gt;</property>
  <property name="conclusion"></property>
  <property name="viz_type">Table</property>
  <property name="viz_options"/>
 </object>
 <object name="top-5-sources-table" meta_type="Google Analytics Report"
    i18n:domain="analytics">
  <property name="title" i18n:translate="">Top 5 Sources: Table</property>
  <property name="description"
     i18n:translate="">Displays a table of the top source domains by number of visits.</property>
  <property name="is_page_specific">False</property>
  <property name="categories">
   <element value="Site Wide"/>
  </property>
  <property name="metrics">
   <element value="ga:visits"/>
  </property>
  <property name="dimensions">
   <element value="ga:source"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="-ga:visits"/>
  </property>
  <property name="max_results">5</property>
  <property name="column_labels">
   <element value="string:Domain"/>
   <element value="string:Visits"/>
  </property>
  <property name="column_exps">
   <element value="ga_source"/>
   <element value="ga_visits"/>
  </property>
  <property name="introduction">&lt;h3&gt;Top 5 Sources&lt;/h3&gt;</property>
  <property name="conclusion"></property>
  <property name="viz_type">Table</property>
  <property name="viz_options"/>
 </object>
</object>
