<?xml version="1.0"?>
<object name="portal_skins">

 <object name="kupumashups"
    meta_type="Filesystem Directory View"
    directory="kupu.mashups:skins/kupumashups"/>

 <object name="kupumashups_images"
    meta_type="Filesystem Directory View"
    directory="kupu.mashups:skins/kupumashups_images"/>

 <object name="kupumashups_styles"
    meta_type="Filesystem Directory View"
    directory="kupu.mashups:skins/kupumashups_styles"/>

 <skin-path name="*" based-on="Plone Default">
  <layer name="kupumashups_images"
     insert-after="custom"/>
  <layer name="kupumashups_styles"
     insert-after="custom"/>
  <layer name="kupumashups"
     insert-after="custom"/>
 </skin-path>

</object>
