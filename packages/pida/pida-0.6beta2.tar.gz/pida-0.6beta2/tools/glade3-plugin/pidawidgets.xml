<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="pidawidgets" library="pidawidgets" domain="glade-3" depends="gtk+" language="python">
  <glade-widget-classes>
    <glade-widget-class title="PIDA View Widget"
                        name="PidaViewWidget"
                        generic-name="pidaviewwidget"
                        adaptor="PidaViewWidgetAdaptor">
      <properties>
        <property id="size" query="False" default="0" visible="False" />
        <property id="spacing" visible="False" />
        <property id="homogeneous" visible="False" />
      </properties>
    </glade-widget-class>
    <glade-widget-class title="VTE Terminal Widget"
                        name="VteTerminal"
                        generic-name="vteterminal">
    </glade-widget-class>
                        
  </glade-widget-classes>

  <glade-widget-group name="PidaWidgets" title="Pida Widgets">
    <glade-widget-class-ref name="PidaViewWidget"/>
    <glade-widget-class-ref name="VteTerminal"/>
  </glade-widget-group>
</glade-catalog>

