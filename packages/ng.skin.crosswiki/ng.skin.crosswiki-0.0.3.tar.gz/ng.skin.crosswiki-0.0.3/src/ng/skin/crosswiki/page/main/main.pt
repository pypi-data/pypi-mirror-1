<html metal:use-macro="views/standard_macros/page">
  <metal:block fill-slot="other">
    <link
      href="main.css" rel="stylesheet" type="text/css"
        tal:attributes="href context/++resource++main.css" />
  </metal:block>
  
  <body metal:fill-slot="mainbody" border="2" tal:omit-tag="">
    <div id="main">
      <h1 class="abstract" tal:content="structure context/++nsinterface++ng.app.converter.object2psadapter.interfaces.IPropertySheet/title"/>
      <div class="abstract" tal:content="structure context/++nsinterface++ng.app.converter.object2psadapter.interfaces.IPropertySheet/abstract"/>
      <form action="@@searchpage.html">
        <input type="text" name="query" value="" tal:attributes="value request/query|nothing"/>
        <select name="predicate"> 
          <option>keyword</option>
          <option>name</option>
        </select>
        <input type="submit" value="Перейти"/>
      </form>
    </div>
  </body>
</html>
