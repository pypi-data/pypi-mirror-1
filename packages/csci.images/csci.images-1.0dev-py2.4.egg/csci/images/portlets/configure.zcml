<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="csci.images">

  <!-- Ensure Plone's portlets ZCML has already been processed;
       without this, we may get a permission lookup error -->
  <include package="plone.app.portlets" />

	<!-- -*- extra stuff goes here -*- -->

  <plone:portlet
      name="csci.images.portlets.csciimages"
      interface=".csciimages.Icsciimages"
      assignment=".csciimages.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".csciimages.Renderer"
      addview=".csciimages.AddForm"
      editview=".csciimages.EditForm"
      />


</configure>
