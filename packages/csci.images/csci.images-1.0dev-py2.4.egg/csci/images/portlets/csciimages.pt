<dl class="portlet portletcsciimages"
    i18n:domain="csci.images.portlets">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <span>
           <span tal:content="view/title">
        </span>
        <span class="portletTopRight"></span>
    </dt>

    <dd class="portletItem odd">
        
    
        <p align=center>
        <span tal:repeat="image view/getimages">
        
            <a tal:attributes="href python:image.absolute_url()+'/view'" tal:content="structure image/image_thumb" />
        
        </span>
        </p>
        
    </dd>

    <dd class="portletFooter">
        <span class="portletBottomLeft"></span>
        <span>
        </span>
        <span class="portletBottomRight"></span>
    </dd>

</dl>
