<?xml version="1.0"?>
<object name="portal_actions"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object">
    <object name="sitestat" meta_type="CMF Action" i18n:domain="iw.sitestat">
      <property name="title" i18n:translate="">Sitestat</property>
      <property name="description"
		i18n:translate="">Sitestat local settings</property>
      <property
	 name="url_expr">string:${plone_context_state/object_url}/@@sitestat-content-options</property>
      <property name="icon_expr"></property>
      <property
	 name="available_expr">python:object.restrictedTraverse('@@plone_interface_info').provides('Products.CMFCore.interfaces.IContentish')</property>
      <property name="permissions">
	<element value="Modify portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
