<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    i18n_domain="wwp.shopfronts">

  <!-- configure your content components here -->
  
  <!-- -*- extra stuff goes here -*- -->
  <class class=".street_view.street_view">
    <require
        permission="zope2.View"
        interface="..interfaces.Istreet_view"
        />
    <require
        permission="cmf.ModifyPortalContent"
        set_schema="..interfaces.Istreet_view"
        />
  </class>
  <class class=".shop_front.shop_front">
    <require
        permission="zope2.View"
        interface="..interfaces.Ishop_front"
        />
    <require
        permission="cmf.ModifyPortalContent"
        set_schema="..interfaces.Ishop_front"
        />
  </class>

</configure>
