<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="wwp.shopfronts">

  <include package="plone.app.contentmenu" />

  <!-- -*- extra stuff goes here -*- -->
  <!-- Include the sub-packages that use their own configure.zcml files. -->
    <browser:resourceDirectory
        name="wwp.shopfronts.images"
        directory="images"
        />
  
  <browser:page
      for="*"
      name="street_view_view"
      class=".street_viewview.street_viewView"
      template="street_viewview.pt"
      allowed_interface=".street_viewview.Istreet_viewView"
      permission="zope.Public"
      />


  <browser:page
      for="*"
      name="shop_front_view"
      class=".shop_frontview.shop_frontView"
      template="shop_frontview.pt"
      allowed_interface=".shop_frontview.Ishop_frontView"
      permission="zope.Public"
      />


</configure>
