<?xml version="1.0"?>
<object name="PersonalData" meta_type="Factory-based Type Information"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n"
   i18n:domain="plone">
 <property name="title"
           i18n:translate="personaldata-contenttype-name">Personal data</property>
 <property name="description">
      </property>
 <property name="content_icon">document_icon.gif</property>
 <property name="content_meta_type">PersonalData</property>
 <property name="product">plonehrm.personaldata</property>
 <property name="factory">addPersonalData</property>
 <property name="immediate_view">../</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="base_view"/>
 <alias from="edit" to="base_edit"/>
 <action title="view" action_id="view" category="object"
    condition_expr="python:1" url_expr="string:${object_url}/../"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/base_edit" visible="True">
  <permission value="plonehrm: Modify personaldata"/>
 </action>
</object>
