<!-- 
     Set a qtree's security mode on a NetApp
-->

<changetemplate name="netapp_set_qtree_security" module='modipy.netapp' type="ZAPIChange">

  <preimpl>
    <zapicommand>
      <qtree-list>
	<volume>%(volname)s</volume>
      </qtree-list>
    </zapicommand>
  </preimpl>

  <impl>
    <zapicommand>
      <qtree-set-security>
	<volume>%(volname)s</volume>
	<qtree>%(qtree_name)s</qtree>
	<security-mode>%(qtree_security)s</security-mode>
      </qtree-set-security>
    </zapicommand>
  </impl>

</changetemplate>

