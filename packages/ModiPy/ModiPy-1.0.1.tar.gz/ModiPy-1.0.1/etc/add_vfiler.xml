<change name='add_vfiler' type='CommandChange'>

  <!-- This is a change specific namespace. -->
  <namespace/>

  <useprov>netapp_provisioner</useprov>

  <target>%(netapp_target)s</target>

  <preimpl>
    <command>
      <send>vfiler status %(vfiler_name)s</send>
    </command>

    <condition>results.cmdoutput.find('Vfiler not found:') >= 0</condition>

  </preimpl>

