<!-- 
     Create a qtree on a NetApp
-->

<changetemplate name="create_netapp_qtree" module='modipy.netapp' type="ZAPIChange">

  <preimpl>
    <zapicommand>
      <qtree-list>
	<volume>%(volname)s</volume>
      </qtree-list>
    </zapicommand>
  </preimpl>

  <impl>
    <zapicommand>
      <qtree-create>
	<volume>%(volname)s</volume>
	<qtree>%(qtree_name)s</qtree>
      </qtree-create>
    </zapicommand>

  </impl>

</changetemplate>

