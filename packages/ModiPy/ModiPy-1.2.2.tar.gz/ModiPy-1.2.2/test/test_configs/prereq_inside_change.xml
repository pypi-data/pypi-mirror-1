<!-- 
     A test config that sets change dependencies
     within a change.
-->
<config>
  <provisioner name="prov1"
               module="modipy.provisioner" type="Provisioner"/>

  <change name="change1"
	  module="modipy.change" type="Change">
  </change>

  <change name="change2"
	  module="modipy.change" type="Change">
    <prereq>change1</prereq>
  </change>
</config>
