<!-- 
     Put a file onto a filer
-->

<changetemplate name="netapp_set_volume_option" module='netapp' type="ZAPIChange">

  <preimpl>
    <zapicommand>
      <volume-list-info>
	<volume>%(volname)s</volume>
      </volume-list-info>
    </zapicommand>
  </preimpl>

  <impl>
    <zapicommand>
      <volume-set-option>
	<volume>%(volname)s</volume>
	<option-name>%(option_name)s</option-name>
	<option-value>%(option_value)s</option-value>
      </volume-set-option>
    </zapicommand>

  </impl>

</changetemplate>

