<!-- 
     Get a file from a filer
-->

<changetemplate name="netapp_get_file" module='modipy.netapp' type="ZAPIChange">

  <impl>
    <getfile>
      <filer_filepath root='/vol/vol0'>/etc/exports</filer_filepath>
      <localpath absolute='false'>%(device.name)s-etc-messages</localpath>
    </getfile>
  </impl>

</changetemplate>

