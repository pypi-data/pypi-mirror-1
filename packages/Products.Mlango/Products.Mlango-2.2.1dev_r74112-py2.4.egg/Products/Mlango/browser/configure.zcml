<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           xmlns:five="http://namespaces.zope.org/five"
           xmlns:kss="http://namespaces.zope.org/kss">
           
    <browser:page
           for="Products.Mlango.content.interfaces.IMlangoRSSViewlet"
           name="rssfeed"
           class=".feedparserview.Feedparser"
           template="rssfeed_viewlet.pt"
           permission="zope2.View"
           />      
    <browser:pages
           for="*"
           class=".viewletfeeder.ViewletFeeder"
           permission="zope2.View"
           >
        <browser:page        
            name="renderviewlet"
            attribute="feeder" 
            />  
        <browser:page
            name="rendercontent"
            attribute="contentfeeder"
            />      
 
    </browser:pages>
    <kss:action
           name="menuSlideLeft"
           command_factory="selector"
           params_mandatory="speed node"
           jsfile="./mlango/effects.js"
           />

    <kss:action
           name="menuSlideRight"
           command_factory="selector"
           params_mandatory="speed node"
           jsfile="./mlango/effects.js"
           />
           
    <kss:action
           name="menuSlideToggle"
           command_factory="selector"
           params_mandatory="speed node"
           jsfile="./mlango/effects.js"
           />
           
    <kss:action
           name="menuToggleContent"
           command_factory="selector"
           params_mandatory="node content_class"
           jsfile="./mlango/effects.js"
           />
           
    <browser:resource
           file="./mlango/mlango.kss"
           name="mlango.kss"
           />
</configure>
