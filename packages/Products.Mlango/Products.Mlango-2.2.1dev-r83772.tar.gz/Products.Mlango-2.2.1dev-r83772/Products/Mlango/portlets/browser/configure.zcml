<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           xmlns:five="http://namespaces.zope.org/five"
           xmlns:kss="http://namespaces.zope.org/kss">

    <browser:page
        for="*"
        name="mlangoDeletePortlet"
        class=".kss.PortletManagerKSS"
        attribute="delete_portlet"
        permission="plone.app.portlets.ManageOwnPortlets"
        />

    <browser:page
        for="*"
        name="mlangoMovePortlet"
        class=".kss.PortletManagerKSS"
        attribute="move_portlet"
        permission="plone.app.portlets.ManageOwnPortlets"
        />

    <browser:page
        for="*"
        name="mlangoTogglePortlet"
        class=".kss.PortletManagerKSS"
        attribute="toggle_portlet"
        permission="plone.app.portlets.ManageOwnPortlets"
        />

    <kss:action
        name="menuSlideLeft"
        command_factory="selector"
        params_mandatory="speed node"
        jsfile="./templates/effects.js"
        />

    <kss:action
        name="menuSlideRight"
        command_factory="selector"
        params_mandatory="speed node"
        jsfile="./templates/effects.js"
        />
           
    <kss:action
        name="menuSlideToggle"
        command_factory="selector"
        params_mandatory="speed node"
        jsfile="./templates/effects.js"
        />
           
    <kss:action
        name="menuToggleContent"
        command_factory="selector"
        params_mandatory="node content_class"
        jsfile="./templates/effects.js"
        />

    <browser:resource
        file="./templates/mlango.kss"
        name="mlango.kss"
        />

    <browser:resource
        file="./templates/mlango.js"
        name="mlango.js"
        />

    <browser:resource
        file="./templates/jquery-ui-personalized-1.6rc2.min.js"
        name="mlangodd.js"
        />

</configure>
