<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en" domain="mlango">   
        <div tal:define="
        url python: context.REQUEST.get('url');        
        data python:view.parse(url)">
        <h2 tal:content="python: data['feed']['title']"/>
          <table>
            <tr tal:repeat="m python: data['entries']">
              <td>
                <a tal:attributes="target string:_new; href python: m['link']" tal:content="python: m['title']"/>
              </td>
            </tr>
          </table>
        </div>
</html>
