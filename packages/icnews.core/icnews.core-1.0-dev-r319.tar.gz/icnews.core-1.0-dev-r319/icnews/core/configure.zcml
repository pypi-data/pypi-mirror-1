    xmlns:five="http://namespaces.zope.org/five"
    xmlns:gs="http://namespaces.zope.org/genericsetup"
    i18n_domain='icNews'>

    <five:registerPackage package="." initialize=".initialize" />

    <gs:registerProfile
            name="default"
            title="icnews.core"
            directory="profiles/default"
            description="icnews.core"
