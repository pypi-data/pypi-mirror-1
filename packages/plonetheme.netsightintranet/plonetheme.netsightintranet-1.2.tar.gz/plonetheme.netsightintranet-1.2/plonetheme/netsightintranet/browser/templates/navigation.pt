<div id="navigationTree"
     tal:define="itemdict view/getSubNavigation"
     tal:attributes="class python: 
		     'portlet ' + (itemdict and 'normal' or 'empty')">

  <div id="title">
    <h3 tal:define="section view/getSubSection">
      <a tal:attributes="href section/absolute_url"
	 tal:content="section/Title" />
    </h3>
  </div>

  <ul tal:condition="itemdict">
    <li tal:replace="structure view/createNavTree">
      SUBTREE
    </li>
  </ul>
  
</div>
