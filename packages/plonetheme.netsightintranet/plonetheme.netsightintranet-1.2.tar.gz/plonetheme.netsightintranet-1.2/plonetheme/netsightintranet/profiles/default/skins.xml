<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Intranet Theme">

 <object name="netsightintranet_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.netsightintranet:skins/netsightintranet_custom_images"/>
 <object name="netsightintranet_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.netsightintranet:skins/netsightintranet_custom_templates"/>
 <object name="netsightintranet_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.netsightintranet:skins/netsightintranet_styles"/>

 <skin-path name="Intranet Theme" based-on="Plone Default">
  <layer name="netsightintranet_custom_images"
     insert-after="custom"/>
  <layer name="netsightintranet_custom_templates"
     insert-after="netsightintranet_custom_images"/>
  <layer name="netsightintranet_styles"
     insert-after="netsightintranet_custom_templates"/>
 </skin-path>

</object>
