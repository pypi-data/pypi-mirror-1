      for="..interfaces.ILinkBackReference"
      label="Edit"
      name="edit.html"
      permission="zope.ManageContent"
      menu="zmi_views" title="Edit" 
      />
      
  <containerViews
        for="..interfaces.ILinkBackReference"
        index="zope.View"
        contents="zope.View"
        add="zope.ManageContent"
        />

</configure>
