<?xml version="1.0"?>
<object name="plonehrm" meta_type="WorkflowPolicy">
 <property name="title">Plone HRM</property>
 <bindings>
  <default>
   <bound-workflow workflow_id="worklocation_workflow"/>
  </default>
  <type type_id="Employee">
    <bound-workflow workflow_id="employee_workflow"/>
  </type>
  <type type_id="Absence">
    <bound-workflow workflow_id="absence_workflow"/>
  </type>
  <type type_id="File">
    <bound-workflow workflow_id="(Default)"/>
  </type>
  <type type_id="Image">
    <bound-workflow workflow_id="(Default)"/>
  </type>
  <type type_id="ChecklistTool">
   <bound-workflow workflow_id="plonehrm_tool_workflow"/>
  </type>
  <type type_id="ContractTool">
   <bound-workflow workflow_id="plonehrm_tool_workflow"/>
  </type>
  <type type_id="JobPerformanceTool">
   <bound-workflow workflow_id="plonehrm_tool_workflow"/>
  </type>
 </bindings>
</object>
