<html metal:use-macro="context/@@standard_macros/page">
<body>

<metal:slot metal:fill-slot="body">
<metal:macro metal:define-macro="heading">
<h1 i18n:translate="heading_edit_marker">Object Information</h1>
</metal:macro>
<metal:macro metal:define-macro="main">

  <tal:info repeat="info view/getInfos">
    <div tal:define="info_name python:info[0];
                      info_obj python:info[1];">
      <h1 tal:content="info_name"/>
      <div tal:content="structure info_obj/@@index.html | string:Sorry, No view for this information type defined." />
    </div>
  </tal:info>

</metal:macro>
</metal:slot>

</body>
</html>
