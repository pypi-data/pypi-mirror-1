<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Plone Default">

 <object name="collective_workflowed_custom_images"
    meta_type="Filesystem Directory View"
    directory="collective.workflowed:skins/collective_workflowed_custom_images"/>
 <object name="collective_workflowed_custom_templates"
    meta_type="Filesystem Directory View"
    directory="collective.workflowed:skins/collective_workflowed_custom_templates"/>
 <object name="collective_workflowed_custom_javascripts"
    meta_type="Filesystem Directory View"
    directory="collective.workflowed:skins/collective_workflowed_custom_javascripts"/>
 <object name="collective_workflowed_styles"
    meta_type="Filesystem Directory View"
    directory="collective.workflowed:skins/collective_workflowed_styles"/>
 <object name="openjacob"
    meta_type="Filesystem Directory View"
    directory="collective.workflowed:skins/openjacob"/>

 <skin-path name="Plone Default">
  <layer name="collective_workflowed_custom_images"
     insert-after="custom"/>
  <layer name="collective_workflowed_custom_templates"
     insert-after="collective_workflowed_custom_images"/>
  <layer name="collective_workflowed_custom_javascripts"
     insert-after="collective_workflowed_custom_templates"/>
  <layer name="collective_workflowed_styles"
     insert-after="collective_workflowed_custom_javascripts"/>
  <layer name="openjacob"
     insert-after="collective_workflowed_styles"/>
 </skin-path>

</object>
