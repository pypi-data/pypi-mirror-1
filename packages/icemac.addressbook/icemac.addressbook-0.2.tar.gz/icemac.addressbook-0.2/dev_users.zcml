<configure xmlns="http://namespaces.zope.org/zope">
  <principal
     id="icemac.addressbook.global.Administrator"
     title="global administrator"
     login="admin"
     password_manager="Plain Text"
     password="admin-ia" />
  <grant
     role="icemac.addressbook.Administrator"
     principal="icemac.addressbook.global.Administrator" />
  
  <principal 
     id="icemac.addressbook.global.Editor"
     title="global editor"
     login="editor"
     password_manager="Plain Text"
     password="editor-ia" />
  <grant
     role="icemac.addressbook.global.Editor"
     principal="icemac.addressbook.global.Editor" />

  <principal
     id="icemac.addressbook.global.Visitor"
     title="global vistor"
     login="visitor"
     password_manager="Plain Text"
     password="visitor-ia" />
  <grant 
     role="icemac.addressbook.global.Visitor" 
     principal="icemac.addressbook.global.Visitor" />

</configure>
