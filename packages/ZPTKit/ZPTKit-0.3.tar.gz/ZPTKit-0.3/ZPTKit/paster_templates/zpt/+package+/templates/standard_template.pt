<metal:tpl define-macro="page">
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html lang="en-US">
<head>
 <title tal:content="options/title | servlet/title" />
 <meta http-equiv="content-language" content="en-us" />
 <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
 <meta name="author"
  content="Imaginary Landscape LLC, http://www.imagescape.com/" />
 <meta name="Rating" content="General" />
 <meta name="Distribution" content="Global" />
 <meta name="Resource-type" content="Document" />
 <meta name="Copyright" tal:attributes="content python: '''%s''' % servlet.copyright" />
 <link rel="stylesheet" type="text/css" tal:attributes="href python: '%s/stylesheet.css' % options['static_url'];" />
 <metal:slot define-slot="extra_head"></metal:slot>
</head>
<body>

<!-- BEGIN CONTENT -->

<span tal:replace="structure servlet/messageText">
This is where the notification messages go.
</span>

<span tal:replace="options/error_message | nothing">
This is where the options/error_message will go.
</span>

<div id="content">
<metal:tpl define-slot="body">
[This page has not customized its "body" slot]
</metal:tpl>
</div>

<!-- END CONTENT -->

</body>
</html>
</metal:tpl>
