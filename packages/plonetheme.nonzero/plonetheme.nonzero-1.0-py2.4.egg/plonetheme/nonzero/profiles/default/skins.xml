<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Nonzero Theme">

 <object name="plonetheme_nonzero_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.nonzero:skins/plonetheme_nonzero_custom_images"/>
 <object name="plonetheme_nonzero_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.nonzero:skins/plonetheme_nonzero_custom_templates"/>
 <object name="plonetheme_nonzero_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.nonzero:skins/plonetheme_nonzero_styles"/>

 <skin-path name="Nonzero Theme" based-on="Plone Default">
  <layer name="plonetheme_nonzero_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_nonzero_custom_templates"
     insert-after="plonetheme_nonzero_custom_images"/>
  <layer name="plonetheme_nonzero_styles"
     insert-after="plonetheme_nonzero_custom_templates"/>
 </skin-path>

</object>
