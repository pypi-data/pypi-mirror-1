<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="peerstheme">

 <object name="plonetheme_peerstheme_custom_images"
    meta_type="Filesystem Directory View"
    directory="plonetheme.peerstheme:skins/plonetheme_peerstheme_custom_images"/>
 <object name="plonetheme_peerstheme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="plonetheme.peerstheme:skins/plonetheme_peerstheme_custom_templates"/>
 <object name="plonetheme_peerstheme_styles"
    meta_type="Filesystem Directory View"
    directory="plonetheme.peerstheme:skins/plonetheme_peerstheme_styles"/>

 <skin-path name="peerstheme" based-on="Plone Default">
  <layer name="plonetheme_peerstheme_custom_images"
     insert-after="custom"/>
  <layer name="plonetheme_peerstheme_custom_templates"
     insert-after="plonetheme_peerstheme_custom_images"/>
  <layer name="plonetheme_peerstheme_styles"
     insert-after="plonetheme_peerstheme_custom_templates"/>
 </skin-path>

</object>
