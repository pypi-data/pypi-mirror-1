<?xml version="1.0"?>

<object name="portal_skins" allow_any="False" cookie_persistence="False"
        default_skin="Web Couturier iCompany Theme" request_varname="plone_skin">

 <object name="webcouturier_icompany_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="webcouturier.icompany.theme:skins/webcouturier_icompany_theme_custom_images"/>
 <object name="webcouturier_icompany_theme_styles"
    meta_type="Filesystem Directory View"
    directory="webcouturier.icompany.theme:skins/webcouturier_icompany_theme_styles"/>

 <skin-path name="Web Couturier iCompany Theme" based-on="Plone Default">
  <layer name="webcouturier_icompany_theme_styles"
     insert-after="custom"/>
  <layer name="webcouturier_icompany_theme_custom_images"
     insert-after="webcouturier_icompany_theme_styles"/>
 </skin-path>
 
</object>