<tal:tabs tal:condition="view/portal_tabs"
          i18n:domain="plone">
    <h5 class="hiddenStructure" i18n:translate="heading_sections">Sections</h5>

<div id="globalnav-wrapper"
     tal:define="selected python:view.selected_portal_tab;
                 home_selected python:view.selected_portal_tab != 'index_html';
                 sublevels python:home_selected and view.getSubmenu(selected_id = selected);">
    <ul id="portal-globalnav">
        <tal:tabs tal:repeat="tab view/portal_tabs"><li tal:attributes="id string:portaltab-${tab/id};
                              class python:view.selected_portal_tab==tab['id'] and 'selected' or 'plain'"><a 
               href="" 
               tal:content="tab/name"
               tal:attributes="href tab/url;
                               title tab/description|nothing">
            Tab Name
            </a></li></tal:tabs>
    </ul>
    <tal:block condition="sublevels">
        <ul id="portal-globalnav-sublevels">
            <tal:subitems tal:replace="structure sublevels">
                Submenu
            </tal:subitems>
        </ul>
    </tal:block>
    <div class="visualClear" id="globalnav-bottom">
        <span><!-- --></span>
    </div>
</div>    
    
</tal:tabs>