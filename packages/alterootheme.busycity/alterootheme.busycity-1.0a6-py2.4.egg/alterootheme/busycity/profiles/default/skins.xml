<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Busy City Free Theme">
 <object name="alterootheme_busycity_custom_images"
    meta_type="Filesystem Directory View"
    directory="alterootheme.busycity:skins/alterootheme_busycity_custom_images"/>
<!--
 <object name="alterootheme_busycity_custom_templates"
    meta_type="Filesystem Directory View"
    directory="alterootheme.busycity:skins/alterootheme_busycity_custom_templates"/>
-->
 <object name="alterootheme_busycity_styles"
    meta_type="Filesystem Directory View"
    directory="alterootheme.busycity:skins/alterootheme_busycity_styles"/>

 <skin-path name="Busy City Free Theme" based-on="Plone Default">
  <layer name="alterootheme_busycity_custom_images"
     insert-after="custom"/>
  <layer name="alterootheme_busycity_custom_templates"
     insert-after="alterootheme_busycity_custom_images"/>
  <layer name="alterootheme_busycity_styles"
     insert-after="alterootheme_busycity_custom_templates"/>
 </skin-path>

</object>
