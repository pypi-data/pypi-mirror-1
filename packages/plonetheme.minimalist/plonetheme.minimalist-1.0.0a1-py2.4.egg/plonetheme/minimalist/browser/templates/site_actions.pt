<div id="develop-by"  i18n:domain="plone">

<p>

    <span i18n:translate="developed_by">
       Desarrollado por
   </span>
    Menttes - 
    <span i18n:translate="developed_by" tal:omit-tag="">
         Construido sobre
    </span>
    <a href="http://plone.org" i18n:translate="label_plone_cms">Plone </a>

</p>

</div>

<ul id="portal-siteactions"
    tal:define="accesskeys python: {'sitemap' : '3', 'accessibility' : '0', 'contact' : '9'};"
    tal:condition="view/site_actions"
    i18n:domain="plone">

    <li tal:repeat="saction view/site_actions"
        tal:attributes="id string:siteaction-${saction/id}"><a
           href=""
           tal:define="title saction/title;
                       id saction/id;
                       accesskey python: accesskeys.get(id, '');"
           i18n:attributes="title"
           i18n:translate=""
           tal:content="title"
           tal:attributes="href saction/url;
                           title title;
                           accesskey accesskey;"
           >Site action</a><span class="footer_separator" tal:condition="python:not(repeat['saction'].end)"> - </span></li>

</ul>
