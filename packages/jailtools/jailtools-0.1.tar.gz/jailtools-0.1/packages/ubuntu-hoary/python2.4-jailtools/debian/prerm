#! /bin/sh 
# prerm script for jailtools

set -e

PACKAGE=python2.4-jailtools
VERSION=2.4
LIB="/usr/lib/python$VERSION"
DIR="$LIB/site-packages"

case "$1" in
    remove|upgrade|failed-upgrade)
        rm ${DIR}/jailtools.py[co]
        rm ${DIR}/jailtalk.py[co]
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac



exit 0
