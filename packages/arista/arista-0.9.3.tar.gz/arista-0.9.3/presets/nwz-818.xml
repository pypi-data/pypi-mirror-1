<?xml version="1.0"?>

<device>
    <make>Sony</make>
    <model>NWZ-818</model>
    <description>H.264/AAC for Sony NWZ-818</description>
    <author>
        <name>Patrick Ulbrich</name>
        <email>zulu99@gmx.net</email>
    </author>
    <version>1.0</version>
    <icon>file://nwz-818.svg</icon>
    <default>Normal</default>
    
    <preset>
        <name>Low</name>
        <container>ffmux_mp4</container>
        <extension>mp4</extension>
        <audio>
            <name>faac</name>
            <passes>
                <pass>bitrate=96000 profile=LC</pass>
            </passes>
            <channels>1, 2</channels>
        </audio>
        <video>
            <name>x264enc</name>
            <passes>
                <pass>pass=qual quantizer=27 subme=4 cabac=0 threads=0</pass>
            </passes> 
            <width>320</width>
            <height>1, 240</height>
            <rate>1, 25</rate>
        </video>
    </preset>
    <preset>
        <name>Normal</name>
        <container>ffmux_mp4</container>
        <extension>mp4</extension>
        <audio>
            <name>faac</name>
            <passes>
                <pass>bitrate=128000 profile=LC</pass>
            </passes>
            <channels>1, 2</channels>
        </audio>
        <video>
            <name>x264enc</name>
            <passes>
                <pass>pass=qual quantizer=21 me=umh subme=6 ref=3 cabac=0 threads=0</pass>
            </passes>
            <width>320</width>
            <height>1, 240</height>
            <rate>1, 30</rate>
        </video>
    </preset>
</device>

