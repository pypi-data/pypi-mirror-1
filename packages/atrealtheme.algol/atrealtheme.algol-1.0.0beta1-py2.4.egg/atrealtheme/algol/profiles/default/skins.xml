<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Algol ThAlgol Theme">

 <object name="atrealtheme_algol_custom_images"
    meta_type="Filesystem Directory View"
    directory="atrealtheme.algol:skins/atrealtheme_algol_custom_images"/>
 <object name="atrealtheme_algol_custom_templates"
    meta_type="Filesystem Directory View"
    directory="atrealtheme.algol:skins/atrealtheme_algol_custom_templates"/>
 <object name="atrealtheme_algol_styles"
    meta_type="Filesystem Directory View"
    directory="atrealtheme.algol:skins/atrealtheme_algol_styles"/>

 <skin-path name="Algol ThAlgol Theme" based-on="Plone Default">
  <layer name="atrealtheme_algol_custom_images"
     insert-after="custom"/>
  <layer name="atrealtheme_algol_custom_templates"
     insert-after="atrealtheme_algol_custom_images"/>
  <layer name="atrealtheme_algol_styles"
     insert-after="atrealtheme_algol_custom_templates"/>
 </skin-path>

</object>
