<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n">

  <object name="object_buttons" meta_type="CMF Action Category">
    <object name="deactivate_audio" meta_type="CMF Action" i18n:domain="p4a.ploneaudio">
      <property name="title" i18n:translate="">Deactivate Audio</property>
      <property name="description" i18n:translate=""></property>
      <property
	 name="url_expr">string:${object_url}/media-config.html</property>
      <property name="icon_expr"></property>
      <property
	 name="available_expr">object/@@p4aaudiosupport/can_deactivate_audio</property>
      <property name="permissions">
	<element value="Modify portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
    <object name="activate_audio" meta_type="CMF Action" i18n:domain="p4a.ploneaudio">
      <property name="title" i18n:translate="">Activate Audio</property>
      <property name="description" i18n:translate=""></property>
      <property
	 name="url_expr">string:${object_url}/media-config.html</property>
      <property name="icon_expr"></property>
      <property
	 name="available_expr">object/@@p4aaudiosupport/can_activate_audio</property>
      <property name="permissions">
	<element value="Modify portal content"/>
      </property>
      <property name="visible">True</property>
    </object>

  </object>


</object>
