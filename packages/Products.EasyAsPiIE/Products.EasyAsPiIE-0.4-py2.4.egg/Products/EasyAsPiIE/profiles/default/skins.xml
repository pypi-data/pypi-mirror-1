<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False">

 <object name="Products_EasyAsPiIE_custom_images"
    meta_type="Filesystem Directory View"
    directory="Products.EasyAsPiIE:skins/Products_EasyAsPiIE_custom_images"/>
 <object name="Products_EasyAsPiIE_custom_templates"
    meta_type="Filesystem Directory View"
    directory="Products.EasyAsPiIE:skins/Products_EasyAsPiIE_custom_templates"/>
 <object name="Products_EasyAsPiIE_styles"
    meta_type="Filesystem Directory View"
    directory="Products.EasyAsPiIE:skins/Products_EasyAsPiIE_styles"/>

 <skin-path name="Plone Default" based-on="Plone Default">
  <layer name="Products_EasyAsPiIE_custom_images"
     insert-after="custom"/>
  <layer name="Products_EasyAsPiIE_custom_templates"
     insert-after="Products_EasyAsPiIE_custom_images"/>
  <layer name="Products_EasyAsPiIE_styles"
     insert-after="Products_EasyAsPiIE_custom_templates"/>
 </skin-path>

</object>
