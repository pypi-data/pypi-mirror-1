<configure
	xmlns="http://namespaces.zope.org/zope"
	xmlns:five="http://namespaces.zope.org/five"
	i18n_domain="qi.Goban">

	<class class=".gogame.GoGame">
		<require
			permission="zope2.View"
			interface="..interfaces.IGoGame"
			/>
		<require
			permission="cmf.ModifyPortalContent"
			set_schema="..interfaces.IGoGame"
			/>
	</class>

	<subscriber
		for="..interfaces.IGoGame 
			zope.app.container.interfaces.IObjectModifiedEvent" 
		handler=".gogame.GameModified" 
		/> 

</configure>
