<?xml version="1.0"?>
<object name="Go Game" meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="qi.Goban" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Go Game</property>
 <property name="description" i18n:translate="">A Go game record.</property>
 <property name="content_icon">++resource++qi.Goban.images/goGame.ico</property>
 <property name="content_meta_type">Go Game</property>
 <property name="product">qi.Goban</property>
 <property name="factory">addGoGame</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">True</property>
 <property name="immediate_view">atct_edit</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view"/>
 </property>

 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="atct_edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="addVariation" to="@@addVariation"/>
 <alias from="view" to="(selected layout)"/>
 
 
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr="not:object/@@plone_lock_info/is_locked_for_current_user|python:True"
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>

<action title="History" action_id="history" category="object"
    condition_expr="" url_expr="string:${object_url}/atct_history" visible="False">
  <permission value="ATContentTypes: View history"/>
 </action>

 <action title="Add sgf variation" action_id="addVariation" category="object" condition_expr=""
    url_expr="string:${object_url}/addVariation" visible="True">
  <permission value="qi.Goban: Add Variation"/>
 </action>
</object>
