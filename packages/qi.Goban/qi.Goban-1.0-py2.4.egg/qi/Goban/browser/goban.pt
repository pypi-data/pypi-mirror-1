<div id="gobanContainer">
  <div id="commTxt" class="userComm"></div>
  <div id="goban" tal:define="bSize view/boardSize ;">
    <div class="gobanRow coordRow">
      <div class="gobanCoord"
      tal:repeat="num python: range(97,105)+range(106,97+bSize+1)"
      tal:content="python: chr(num)"></div>
    </div>
    <div class="gobanRow">
      <div class="gobanCoord" tal:content="bSize"></div>
      <div class="stoneContainer">
        <img src="++resource++qi.Goban.images/tl.gif" id="0-0" />
      </div>
      <tal:items repeat="col python: range(1,bSize-1)">
        <div class="stoneContainer">
          <img tal:attributes="id python:'0-'+str(col)"
          src="++resource++qi.Goban.images/tp.gif" />
        </div>
      </tal:items>
      <div class="stoneContainer">
        <img tal:attributes="id python:'0-'+str(bSize-1)"
        src="++resource++qi.Goban.images/tr.gif" />
      </div>
      <div class="gobanCoord" tal:content="bSize"></div>
	  <div class="clearRight"/>
    </div>
	<tal:items repeat="row python: range(1,bSize-1)">
    <div class="gobanRow">
      <div class="gobanCoord" tal:content="python: bSize-row">
      </div>
      <div class="stoneContainer">
        <img tal:attributes="id python:str(row)+'-0'"
        src="++resource++qi.Goban.images/lp.gif" />
      </div>
      <tal:items repeat="col python: range(1,bSize-1)">
        <div class="stoneContainer">
          <img tal:attributes="id python:str(row)+'-'+str(col)"
          src="++resource++qi.Goban.images/ep.gif" />
        </div>
      </tal:items>
      <div class="stoneContainer">
        <img tal:attributes="id python:str(row)+'-'+str(bSize-1)"
        src="++resource++qi.Goban.images/rp.gif" />
      </div>
      <div class="gobanCoord" tal:content="python: bSize-row">
      </div>
    </div>
</tal:items>
    <div class="gobanRow">
      <div class="gobanCoord">1</div>
      <div class="stoneContainer">
        <img tal:attributes="id python:str(bSize-1)+'-0'"
        src="++resource++qi.Goban.images/bl.gif" />
      </div>
      <tal:items repeat="col python: range(1,bSize-1)">
        <div class="stoneContainer">
          <img tal:attributes="id python:str(bSize-1)+'-'+str(col)"
          src="++resource++qi.Goban.images/bp.gif" />
        </div>
      </tal:items>
      <div class="stoneContainer">
        <img tal:attributes="id python:str(bSize-1)+'-'+str(bSize-1)"
        src="++resource++qi.Goban.images/br.gif" />
      </div>
      <div class="gobanCoord">1</div>
    </div>
    <div class="gobanRow coordRow">
      <div></div>
      <div class="gobanCoord"
      tal:repeat="num python: range(97,105)+range(106,97+bSize+1)"
      tal:content="python: chr(num)"></div>
      <div></div>
    </div>
  </div>
</div>
