  <adapter
      factory=".orderedcontainer2pagersourceadapter.OrderedContainer2PagerSourceAdapter"
      permission="zope.View"
      trusted="True"
      />

  <adapter
      factory=".container2pagersourceadapter.Container2PagerSourceAdapter"
