<div>
  <b tal:content="context/title"></b>
  <a tal:attributes="href view/googleMapsLink|nothing">
    <div tal:content="context/address1" />
    <div tal:content="context/address2" />
    <div>
      <span tal:content="context/city" />,
      <span tal:content="context/state" />
      <span tal:content="context/postalCode" />
    </div>
    <div tal:content="context/country" />
    <div tal:condition="context/alternate">
      <i tal:content="context/alternate" />
    </div>
  </a>
</div>