<html>
  <head>
    <title tal:content="context/title"></title>
    <link type="text/css" rel="stylesheet" href="@@/print.css" media="all" />
  </head>
  <body>
    <h1 tal:content="context/title" id="contact-list-title">Application Title</h1>
    
    <div tal:repeat="contact python:sorted(view.selectedContacts, lambda a,b: cmp(a.title, b.title))"
	 class="contact">
      <h3 tal:content="contact/title" class="contact-title">Title</h3>
      <tal:block tal:repeat="fieldContainer contact/values">
	<div
	    class="contact-fields"
	    tal:condition="fieldContainer"
	    tal:content="structure fieldContainer/@@print.html">
	</div>
      </tal:block>
    </div>
  </body>
</html>