<html metal:use-macro="views/standard_macros/view"
      i18n:domain="ks.installtool">
    <body metal:fill-slot="body">
        <h3 i18n:translate="">Error running script!</h3>
        <div class="row" tal:condition="context/res">
            <h4 i18n:translate="">Completion statuses on scripts before:</h4>
            <tal:block tal:repeat="item context/res">
                <tal:block i18n:translate="">Script</tal:block> <strong tal:content="python:item[0]">ScriptName</strong> - <strong tal:content="python:item[1]">ScriptName</strong><br/>
            </tal:block>
        </div>
        <h4 i18n:translate="">Script, caused error:</h4>
        <div class="row">
            <tal:block i18n:translate="">Script</tal:block> <strong tal:content="context/script">ScriptName</strong> - <tal:block i18n:translate="">Error!</tal:block><br/>
            <strong i18n:translate="">Error description:</strong>
            <span tal:replace="python:str(context.msg).decode('utf-8', 'replace')">Error message</span>
        </div>
    </body>
</html>
