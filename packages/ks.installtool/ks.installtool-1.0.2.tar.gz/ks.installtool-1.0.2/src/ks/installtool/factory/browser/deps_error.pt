<html metal:use-macro="views/standard_macros/view"
      i18n:domain="ks.installtool">
    <body metal:fill-slot="body">
        <h3 i18n:translate="">Error</h3>
        <div class="row" i18n:translate="">Can't resolve dependenсies in one of scripts:</div>
        <div class="row">
            <tal:block tal:repeat="item view/getScripts" i18n:translate="">
                <tal:block content="python:item[0]" i18n:name="Script" /> depends on
                <tal:block content="python:item[1]" i18n:name="Depends" />
            </tal:block>
            <br/>
        </div>
    </body>
</html>
