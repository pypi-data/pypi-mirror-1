<VirtualHost %(binding)s>
    ServerName %(visible_hostname)s

    <Proxy http://%(squid_localisation)s:%(squid_port)s>
            Allow from all
    </Proxy>


    RewriteEngine On
    RewriteLog %(squid_log_dir)s/rewrite_%(visible_hostname)s.log
    RewriteLogLevel %(debug_apache_rewrite_rules)s

    CustomLog %(squid_log_dir)s/access_%(visible_hostname)s.log common
    ErrorLog %(squid_log_dir)s/error_%(visible_hostname)s.log

%(vhost_apache_zopeconf)s
</VirtualHost>
