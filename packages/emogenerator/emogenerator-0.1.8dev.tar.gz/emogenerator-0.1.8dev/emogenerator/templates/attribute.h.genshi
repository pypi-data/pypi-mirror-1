{% if property.CType %}\
@property (readwrite, assign) ${property.CType} ${property.name};
{% if options['suppressAccessorDeclarations'] == False %}\
- (${property.CType})${property.name};
- (void)set${C(property.name)}:(${property.CType})in${C(property.name)};
{% end %}{# endif options['suppressAccessorDeclarations'] == False #}\
@property (readwrite, retain) ${property.CocoaType}${property.name}Value;
