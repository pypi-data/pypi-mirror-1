<?xml version="1.0"?>
<object
    name="Questionnaire"
    meta_type="Factory-based Type Information with dynamic views"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title">Questionnaire</property>
  <property name="description">A questionnaire.</property>
  <property name="content_icon">questionnaire.gif</property>
  <property name="content_meta_type">Questionnaire</property>
  <property name="product">CMFQuestionnaire</property>
  <property name="factory">addQuestionnaire</property>
  <property name="immediate_view">edit</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">True</property>
  <property name="allow_discussion">False</property>

  <action
      title="View"
      action_id="view"
      category="object"
      condition_expr=""
      url_expr="string:$object_url"
      visible="True">
    <permission value="View"/>
  </action>
  <action
      title="Edit"
      action_id="edit"
      category="object"
      condition_expr=""
      url_expr="string:$object_url/edit"
      visible="True">
    <permission value="Modify portal content"/>
  </action>
  <action
      title="Design"
      action_id="design"
      category="object"
      condition_expr=""
      url_expr="string:$object_url/design"
      visible="True">
    <permission value="Modify portal content"/>
  </action>
  <action
      title="Submissions"
      action_id="submissions"
      category="object"
      condition_expr=""
      url_expr="string:$object_url/submissions"
      visible="True">
    <permission value="Modify portal content"/>
  </action>
  <action
      title="Results"
      action_id="results"
      category="object"
      condition_expr=""
      url_expr="string:$object_url/results"
      visible="True">
    <permission value="CMFQuestionnaire: View Results"/>
  </action>

  <alias from="(Default)" to="questionnaire_view_form"/>
  <alias from="view" to="questionnaire_view_form"/>
  <alias from="edit" to="base_edit"/>
  <alias from="design" to="questionnaire_design_form"/>
  <alias from="submissions" to="questionnaire_submissions_form"/>
  <alias from="results" to="questionnaire_results_form"/>
  <alias from="properties" to="base_metadata"/>
</object>
