<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="csci.shortlinks">

  <include package="plone.app.contentmenu" />

  <!-- -*- extra stuff goes here -*- -->

  <browser:page
      for="*"
      name="newlink_view"
      class=".newlinkview.newlinkView"
      template="newlinkview.pt"
      allowed_interface=".newlinkview.InewlinkView"
      permission="zope.Public"
      />

    
    <browser:page
        for="*"
        name="newlink"
        class=".newlink.newlink"
        permission="zope.Public" />


</configure>
