<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <!-- include type ObjectType -->
    <xsd:include schemaLocation="../../schema/object.xsd" />

    <!-- root element -->
    <xsd:element name="PAGEOBJECTS" type="PageobjectsType" />

    <!-- arbitrary number of PAGEOBJECT nodes -->
    <xsd:complexType name="PageobjectsType">
        <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="PAGEOBJECT" type="ObjectType" />
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>