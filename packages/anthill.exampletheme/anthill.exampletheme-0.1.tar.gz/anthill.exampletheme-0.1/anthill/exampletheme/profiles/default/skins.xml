<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Plone Default">

 <object name="anthill_exampletheme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="anthill.exampletheme:skins/anthill_exampletheme_custom_templates"/>
 <object name="anthill_exampletheme_custom_images"
    meta_type="Filesystem Directory View"
    directory="anthill.exampletheme:skins/anthill_exampletheme_custom_images"/>
 <object name="anthill_exampletheme_styles"
    meta_type="Filesystem Directory View"
    directory="anthill.exampletheme:skins/anthill_exampletheme_styles"/>

 <skin-path name="publicview" based-on="publicview">
  <layer name="anthill_exampletheme_custom_templates"
     insert-before="anthill_skinner_templates"/>
  <layer name="anthill_exampletheme_custom_images"
     insert-after="anthill_exampletheme_custom_templates"/>
  <layer name="anthill_exampletheme_styles"
     insert-after="anthill_exampletheme_custom_templates"/>
 </skin-path>

</object>
