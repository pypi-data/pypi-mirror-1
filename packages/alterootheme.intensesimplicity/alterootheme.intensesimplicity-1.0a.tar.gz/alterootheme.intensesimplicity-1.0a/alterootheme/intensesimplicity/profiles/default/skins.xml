<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Intense Simplicity">

 <object name="alterootheme_intensesimplicity_custom_images"
    meta_type="Filesystem Directory View"
    directory="alterootheme.intensesimplicity:skins/alterootheme_intensesimplicity_custom_images"/>
 <object name="alterootheme_intensesimplicity_custom_templates"
    meta_type="Filesystem Directory View"
    directory="alterootheme.intensesimplicity:skins/alterootheme_intensesimplicity_custom_templates"/>
 <object name="alterootheme_intensesimplicity_styles"
    meta_type="Filesystem Directory View"
    directory="alterootheme.intensesimplicity:skins/alterootheme_intensesimplicity_styles"/>

 <skin-path name="Intense Simplicity" based-on="Plone Default">
  <layer name="alterootheme_intensesimplicity_custom_images"
     insert-after="custom"/>
  <layer name="alterootheme_intensesimplicity_custom_templates"
     insert-after="alterootheme_intensesimplicity_custom_images"/>
  <layer name="alterootheme_intensesimplicity_styles"
     insert-after="alterootheme_intensesimplicity_custom_templates"/>
 </skin-path>

</object>
