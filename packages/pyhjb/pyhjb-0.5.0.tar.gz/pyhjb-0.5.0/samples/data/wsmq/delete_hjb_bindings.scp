# ----------------------------------------
# JMS Admin script that automates 
# removal of the JMS bindings used
# in testing HJB with WebSphere MQ
#
# Successfully ran on 2006/05/07 with 
#      WebSphere MQ 6.0
#   on Windows XP SP (2) 
# ----------------------------------------

# ----------------------------------------
# Navigate to context the HJB objects are
# stored
# ----------------------------------------
CHANGE CTX(HJB)
CHANGE CTX(SAMPLE)

# ----------------------------------------
# Delete the Queue Connection Factories
# ----------------------------------------
DELETE CF(ConnectionFactory)
DELETE QCF(QueueConnectionFactory)
DELETE TCF(TopicConnectionFactory)

# ----------------------------------------
# Delete the Topic bindings
# ----------------------------------------
DELETE T(QOTD)
DELETE T(LOGMESSAGE)
DELETE T(HEARTBEAT)

# ----------------------------------------
# Delete the Queue bindings
# ----------------------------------------
DELETE Q(BYTES)
DELETE Q(MAP)
DELETE Q(OBJECT)
DELETE Q(TEXT)
DELETE Q(STREAM)

# ----------------------------------------
# Delete the contexts
# ----------------------------------------
CHANGE CTX(=UP)
DELETE CTX(SAMPLE)
CHANGE CTX(=UP)
DELETE CTX(HJB)

# ----------------------------------------
# JMSAdmin script must end with 'END'
# ----------------------------------------
END
